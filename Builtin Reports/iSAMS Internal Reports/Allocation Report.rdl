<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>4262a49b-48f0-4907-a6aa-4155cb038fdf</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Reports"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intReportCycle"><Value>=Parameters!intReportCycle.Value</Value></QueryParameter></QueryParameters><CommandText>WITH SubjectAllocations
     AS (
     SELECT DISTINCT
            CAST(TblTeachingManagerSetsID AS VARCHAR(MAX)) AS txtGroupID,
            txtSetCode AS txtGroupName,
            COALESCE(TblStaff.Title+' '+TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Title+' '+TblStaff.Surname) AS txtTeacher,
            intSubject,
            txtSubjectCode,
            txtSubjectName,
            txtSubjectReportName,
            tblPupilManagementPupils.intNCYear,
            tblPupilManagementPupils.txtForm,
            intTemplate,
            DENSE_RANK() OVER(PARTITION BY TblTeachingManagerSets.txtSetCode,
                                           tblPupilManagementPupils.intNCYear ORDER BY TblReportsManagementCycleAllocations.intOrder) AS intAllocationRank,
            'Set' AS txtGroupSubType,
            CASE
                WHEN TblReportsManagementCycleAllocations.intAllType = 0
                THEN 'All Pupils'
                WHEN TblReportsManagementCycleAllocations.intAllType = 1
                THEN 'Year Allocation'
                WHEN TblReportsManagementCycleAllocations.intAllType = 2
                THEN 'Subject Allocation'
                WHEN TblReportsManagementCycleAllocations.intAllType = 3
                THEN 'Form Allocation'
            END AS txtAllocationType,
            TblReportsManagementCycleAllocations.txtAllValue,
            TblReportsManagementCycleAllocations.intAllValue
     FROM TblTeachingManagerSets
          LEFT OUTER JOIN tblTeachingManagerSubjects ON TblTeachingManagerSets.intSubject = tblTeachingManagerSubjectsID
          LEFT OUTER JOIN TblStaff ON TblTeachingManagerSets.txtTeacher = TblStaff.User_Code
          LEFT OUTER JOIN tblTeachingManagerSetLists ON tblTeachingManagerSetLists.intSetID = TblTeachingManagerSetsID
          LEFT OUTER JOIN tblPupilManagementPupils ON tblTeachingManagerSetLists.txtSchoolID = tblPupilManagementPupils.txtSchoolID
          INNER JOIN TblReportsManagementCycleAllocations ON --All pupils allocated display all sets.
          (intAllType = 0
           OR --Filter to sets that have year allocated.
           (intAllType = 1
            AND intAllValue = tblPupilManagementPupils.intNCYear
            AND blnExclusion = 0
            AND tblPupilManagementPupils.intSystemStatus = 1)
           --Filter to sets that have subject allocated.
           OR (intAllType = 2
               AND intAllValue = TblTeachingManagerSets.intSubject
               AND blnExclusion = 0)
           --Filter to sets that have forms allocated.
           OR (tblPupilManagementPupils.txtForm = txtAllValue
               AND intReportType = 1
               AND blnExclusion = 0
               AND tblPupilManagementPupils.intSystemStatus = 1))
          --Filter to sets that have pupils in years included.
          --    OR (TblReportsManagementCycleAllocations.intAllType = 1 AND TblTeachingManagerSetsID IN
          --(
          --    SELECT DISTINCT
          --           intSetID
          --    FROM tblTeachingManagerSetLists
          --         INNER JOIN tblPupilManagementPupils ON tblteachingmanagersetlists.txtschoolid = tblpupilmanagementpupils.txtschoolid
          --         INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.intAllValue = tblPupilManagementPupils.intNCYear
          --                                                            AND TblReportsManagementCycleAllocations.intAllType = 1
          --    WHERE intReportCycle = @intReportCycle
          --          AND intReportType = 1
          --          AND blnExclusion = 0
          --))
		
          AND NOT( --Filter out sets that have year excluded.
          TblTeachingManagerSetsID IN
                 (
                     SELECT DISTINCT
                            TblTeachingManagerSetsID
                     FROM tblteachingmanagersets
                          INNER JOIN TblReportsManagementCycleAllocations ON intAllValue = TblTeachingManagerSets.intYear
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 1
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 1
                           AND blnExclusion = 1
                 )
                     --Filter out sets that have subject excluded.
                     OR TblTeachingManagerSetsID IN
                 (
                     SELECT DISTINCT
                            TblTeachingManagerSetsID
                     FROM tblteachingmanagersets
                          INNER JOIN TblReportsManagementCycleAllocations ON intAllValue = TblTeachingManagerSets.intSubject
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 2
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 1
                           AND blnExclusion = 1
                 )
                     --Filter out sets that have forms excluded.
                     OR tblTeachingManagerSetLists.txtSchoolID IN
                 (
                     SELECT DISTINCT
                            tblPupilManagementPupils.txtSchoolID
                     FROM tblteachingmanagersetlists
                          INNER JOIN tblPupilManagementPupils ON tblteachingmanagersetlists.txtschoolid = tblpupilmanagementpupils.txtschoolid
                          INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.txtAllValue = tblPupilManagementPupils.txtForm
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 3
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 1
                           AND blnExclusion = 1
                 )
                     --Filter out sets that have pupils excluded.
                     OR tblTeachingManagerSetLists.txtSchoolID IN
                 (
                     SELECT DISTINCT
                            tblPupilManagementPupils.txtSchoolID
                     FROM tblteachingmanagersetlists
                          INNER JOIN tblPupilManagementPupils ON tblteachingmanagersetlists.txtSchoolID = tblpupilmanagementpupils.txtSchoolID
                          INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.txtAllValue = tblpupilmanagementpupils.txtSchoolID
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 8
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 1
                           AND blnExclusion = 1
                 ))
     WHERE intReportCycle = @intReportCycle
           AND tblTeachingManagerSetLists.txtSchoolID &lt;&gt; ''
           AND intReportType = 1
     UNION ALL
     SELECT DISTINCT
            CAST(TblTeachingManagerSubjectFormsID AS VARCHAR(MAX)) AS txtGroupID,
            txtTimetableCode AS txtGroupName,
            COALESCE(TblStaff.Title+' '+TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Title+' '+TblStaff.Surname) AS txtTeacher,
            intSubject,
            txtSubjectCode,
            txtSubjectName,
            txtSubjectReportName,
            tblPupilManagementPupils.intNCYear,
            tblPupilManagementPupils.txtForm,
            intTemplate,
            DENSE_RANK() OVER(PARTITION BY TblTeachingManagerSubjectForms.txtTimetableCode,
                                           tblPupilManagementPupils.intNCYear ORDER BY TblReportsManagementCycleAllocations.intOrder) AS intAllocationRank,
            'Teaching Form' AS txtGroupSubType,
            CASE
                WHEN TblReportsManagementCycleAllocations.intAllType = 0
                THEN 'All Pupils'
                WHEN TblReportsManagementCycleAllocations.intAllType = 1
                THEN 'Year Allocation'
                WHEN TblReportsManagementCycleAllocations.intAllType = 2
                THEN 'Subject Allocation'
                WHEN TblReportsManagementCycleAllocations.intAllType = 3
                THEN 'Form Allocation'
            END AS txtAllocationType,
            TblReportsManagementCycleAllocations.txtAllValue,
            TblReportsManagementCycleAllocations.intAllValue
     FROM TblTeachingManagerSubjectForms
          LEFT OUTER JOIN tblTeachingManagerSubjects ON TblTeachingManagerSubjectForms.intSubject = tblTeachingManagerSubjectsID
          LEFT OUTER JOIN tblSchoolManagementForms ON TblTeachingManagerSubjectForms.txtForm = tblSchoolManagementForms.txtForm
          LEFT OUTER JOIN TblStaff ON TblTeachingManagerSubjectForms.txtTeacher = TblStaff.User_Code
          LEFT OUTER JOIN TblPupilManagementPupils ON tblSchoolManagementForms.txtForm = TblPupilManagementPupils.txtForm
          INNER JOIN TblReportsManagementCycleAllocations ON --All pupils allocated display all teaching forms.
          (intAllType = 0
           --Filter to teaching forms that have year allocated.
           OR (intAllType = 1
               AND intAllValue = TblPupilManagementPupils.intNCYear
               AND blnExclusion = 0
               AND tblPupilManagementPupils.intSystemStatus = 1)
           --Filter to teaching forms that have subject allocated.
           OR (intAllType = 2
               AND intAllValue = TblTeachingManagerSubjectForms.intSubject
               AND blnExclusion = 0)
           --Filter to teaching forms that have form allocated.
           OR (intAllType = 3
               AND txtAllValue = TblTeachingManagerSubjectForms.txtForm
               AND tblPupilManagementPupils.intSystemStatus = 1)
          --Filter to Teaching Forms that have pupils in years included
          -- OR (TblReportsManagementCycleAllocations.intAllType = 1 AND tblPupilManagementPupils.txtSchoolID IN
          --(
          --    SELECT DISTINCT
          --           tblPupilManagementPupils.txtSchoolID
          --    FROM tblPupilManagementPupils
          --         INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.intAllValue = tblPupilManagementPupils.intNCYear
          --                                                            AND TblReportsManagementCycleAllocations.intAllType = 1
          --    WHERE intReportCycle = @intReportCycle
          --          AND intReportType = 0
          --          AND blnExclusion = 1
          --))
          )
          AND NOT((intAllType = 1
                   AND intAllValue = tblSchoolManagementForms.intNCYear
                   AND blnExclusion = 1)
                  --Filter out teaching forms that have subject excluded.
                  OR TblTeachingManagerSubjectFormsID IN
                 (
                     SELECT DISTINCT
                            TblTeachingManagerSubjectFormsID
                     FROM TblTeachingManagerSubjectForms
                          INNER JOIN TblReportsManagementCycleAllocations ON intAllValue = TblTeachingManagerSubjectForms.intSubject
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 2
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 1
                           AND blnExclusion = 1
                 )
                     --Filter out Teaching Forms that have form excluded.
                     OR TblTeachingManagerSubjectFormsID IN
                 (
                     SELECT DISTINCT
                            TblTeachingManagerSubjectFormsID
                     FROM TblTeachingManagerSubjectForms
                          INNER JOIN TblReportsManagementCycleAllocations ON txtAllValue = TblTeachingManagerSubjectForms.txtForm
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 3
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 1
                           AND blnExclusion = 1
                 )
                     --Filter out Teaching Forms that have pupils in years excluded.
                     OR tblPupilManagementPupils.txtSchoolID IN
                 (
                     SELECT DISTINCT
                            txtSchoolID
                     FROM tblPupilManagementPupils
                          INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.intAllValue = tblPupilManagementPupils.intNCYear
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 1
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 1
                           AND blnExclusion = 1
                 )
                     --Filter out Teaching Forms that have pupils excluded.
                     OR txtSchoolID IN
                 (
                     SELECT DISTINCT
                            txtSchoolID
                     FROM tblPupilManagementPupils
                          INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.txtAllValue = tblPupilManagementPupils.txtSchoolID
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 8
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 1
                           AND blnExclusion = 1
                 ))
     WHERE intReportCycle = @intReportCycle
           AND TblPupilManagementPupils.txtSchoolID &lt;&gt; ''
           AND intReportType = 1
           AND blnExclusion = 0),
     FormAllocations
     AS (SELECT DISTINCT
                tblSchoolManagementForms.txtForm AS txtGroupID,
                tblSchoolManagementForms.txtForm AS txtGroupName,
                COALESCE(TblStaff.Title+' '+TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Title+' '+TblStaff.Surname) AS txtTeacher,
                tblPupilManagementPupils.intNCYear,
                tblPupilManagementPupils.txtForm,
                intTemplate,
                DENSE_RANK() OVER(PARTITION BY tblSchoolManagementForms.txtForm,
                                               tblPupilManagementPupils.txtForm,
                                               tblPupilManagementPupils.intNCYear ORDER BY TblReportsManagementCycleAllocations.intOrder) AS intAllocationRank,
                CASE
                    WHEN TblReportsManagementCycleAllocations.intAllType = 0
                    THEN 'All Pupils'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 1
                    THEN 'Year Allocation'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 3
                    THEN 'Form Allocation'
                END AS txtAllocationType,
                TblReportsManagementCycleAllocations.txtAllValue,
                TblReportsManagementCycleAllocations.intAllValue
         FROM tblSchoolManagementForms
              LEFT OUTER JOIN TblPupilManagementPupils ON tblSchoolManagementForms.txtForm = TblPupilManagementPupils.txtForm
              LEFT OUTER JOIN TblStaff ON tblSchoolManagementForms.txtFormTutor = TblStaff.User_Code
              INNER JOIN TblReportsManagementCycleAllocations ON --All pupils allocated display all forms. 
              (intAllType = 0
               AND tblPupilManagementPupils.intSystemStatus = 1
               --Filter to forms that have year allocated.
               OR (intAllType = 1
                   AND intAllValue = TblPupilManagementPupils.intNCYear
                   AND blnExclusion = 0
                   AND tblPupilManagementPupils.intSystemStatus = 1)
               --Filter to specific forms
               OR (intAllType = 3
                   AND txtAllValue = TblPupilManagementPupils.txtForm
                   AND blnExclusion = 0
                   AND tblPupilManagementPupils.intSystemStatus = 1))
              AND NOT(tblSchoolManagementForms.txtForm IN
                     (
                         SELECT DISTINCT
                                tblSchoolManagementForms.txtForm
                         FROM tblSchoolManagementForms
                              INNER JOIN TblReportsManagementCycleAllocations ON intAllValue = tblSchoolManagementForms.intNCYear
                                                                                 AND TblReportsManagementCycleAllocations.intAllType = 1
                         WHERE intReportCycle = @intReportCycle
                               AND intReportType = 6
                               AND blnExclusion = 1
                     )

                         --Filter out specific forms
                         OR tblSchoolManagementForms.txtForm IN
                     (
                         SELECT DISTINCT
                                tblSchoolManagementForms.txtForm
                         FROM tblSchoolManagementForms
                              INNER JOIN TblReportsManagementCycleAllocations ON txtAllValue = tblSchoolManagementForms.txtForm
                                                                                 AND TblReportsManagementCycleAllocations.intAllType = 3
                         WHERE intReportCycle = @intReportCycle
                               AND intReportType = 6
                               AND blnExclusion = 1
                     )
                         -- Filters out forms with pupils in years excluded
                         OR TblPupilManagementPupils.txtSchoolID IN
                     (
                         SELECT DISTINCT
                                TblPupilManagementPupils.txtSchoolID
                         FROM TblPupilManagementPupils
                              INNER JOIN TblReportsManagementCycleAllocations ON intAllValue = TblPupilManagementPupils.intNCYear
                                                                                 AND TblReportsManagementCycleAllocations.intAllType = 1
                         WHERE intReportCycle = @intReportCycle
                               AND intReportType = 6
                               AND blnExclusion = 1
                     )
                         --Filters out pupils that have been individually excluded
                         OR TblPupilManagementPupils.txtSchoolID IN
                     (
                         SELECT DISTINCT
                                txtSchoolID
                         FROM tblPupilManagementPupils
                              INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.txtAllValue = tblPupilManagementPupils.txtSchoolID
                                                                                 AND TblReportsManagementCycleAllocations.intAllType = 8
                         WHERE intReportCycle = @intReportCycle
                               AND intReportType = 1
                               AND blnExclusion = 1
                     ))
         WHERE intReportCycle = @intReportCycle
               AND intReportType = 6
               AND blnExclusion = 0),
     PastoralAllocations
     AS (SELECT DISTINCT
                TblPupilManagementPupils.txtTutor AS txtGroupID,
                COALESCE(TblStaff.Title+' '+TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Title+' '+TblStaff.Surname) AS txtGroupName,
                COALESCE(TblStaff.Title+' '+TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Title+' '+TblStaff.Surname) AS txtTeacher,
                tblPupilManagementPupils.intNCYear,
                tblPupilManagementPupils.txtForm,
                intTemplate,
                DENSE_RANK() OVER(PARTITION BY TblPupilManagementPupils.txtTutor,
                                               tblPupilManagementPupils.txtForm,
                                               tblPupilManagementPupils.intNCYear ORDER BY TblReportsManagementCycleAllocations.intOrder) AS intAllocationRank,
                CASE
                    WHEN TblReportsManagementCycleAllocations.intAllType = 0
                    THEN 'All Pupils'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 1
                    THEN 'Year Allocation'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 3
                    THEN 'Form Allocation'
                END AS txtAllocationType,
                TblReportsManagementCycleAllocations.txtAllValue,
                TblReportsManagementCycleAllocations.intAllValue
         FROM TblPupilManagementPupils
              INNER JOIN TblStaff ON txtTutor = User_Code
              INNER JOIN TblReportsManagementCycleAllocations ON --All pupils allocated display all forms. 
              (intAllType = 0
               AND tblPupilManagementPupils.intSystemStatus = 1
               --Filter to tutors that have pupils in year allocated.
               OR (intAllType = 1
                   AND intAllValue = TblPupilManagementPupils.intNCYear
                   AND blnExclusion = 0
                   AND tblPupilManagementPupils.intSystemStatus = 1)
               --Filter to tutors that have pupils in form allocated.
               OR (intAllType = 3
                   AND txtAllValue = TblPupilManagementPupils.txtForm
                   AND blnExclusion = 0
                   AND tblPupilManagementPupils.intSystemStatus = 1))
              --Filter out tutors that have pupils in year excluded.
              AND NOT(intNCYEAR IN
                     (
                         SELECT DISTINCT
                                intNCYEAR
                         FROM tblPupilManagementPupils
                              INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.intAllValue = tblPupilManagementPupils.intNCYear
                                                                                 AND TblReportsManagementCycleAllocations.intAllType = 1
                         WHERE intReportCycle = @intReportCycle
                               AND intReportType = 7
                               AND blnExclusion = 1
                     )
                         --Filter out tutors that have pupils in form Excluded.
                         OR tblPupilManagementPupils.txtschoolID IN
                     (
                         SELECT DISTINCT
                                tblPupilManagementPupils.txtschoolID
                         FROM tblPupilManagementPupils
                              INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.txtAllValue = tblPupilManagementPupils.txtForm
                                                                                 AND TblReportsManagementCycleAllocations.intAllType = 3
                         WHERE intReportCycle = @intReportCycle
                               AND intReportType = 7
                               AND blnExclusion = 1
                     ))
         WHERE intReportCycle = @intReportCycle
               AND intReportType = 7
               AND blnExclusion = 0),
     YearAllocation
     AS (SELECT DISTINCT
                CAST(tblSchoolManagementYears.intNCYear AS VARCHAR(MAX)) AS txtGroupID,
                txtYearName AS txtGroupName,
                COALESCE(TblStaff.Title+' '+TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Title+' '+TblStaff.Surname) AS txtTeacher,
                tblPupilManagementPupils.intNCYear,
                tblPupilManagementPupils.txtForm,
                intTemplate,
                DENSE_RANK() OVER(PARTITION BY tblSchoolManagementYears.txtYearName,
                                               tblPupilManagementPupils.txtForm,
                                               tblPupilManagementPupils.intNCYear ORDER BY TblReportsManagementCycleAllocations.intOrder) AS intAllocationRank,
                CASE
                    WHEN TblReportsManagementCycleAllocations.intAllType = 0
                    THEN 'All Pupils'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 1
                    THEN 'Year Allocation'
                END AS txtAllocationType,
                TblReportsManagementCycleAllocations.txtAllValue,
                TblReportsManagementCycleAllocations.intAllValue
         FROM tblSchoolManagementYears
              LEFT OUTER JOIN TblStaff ON tblSchoolManagementYears.txtYearTutor = TblStaff.User_Code
              LEFT OUTER JOIN TblPupilManagementPupils ON tblSchoolManagementYears.intNCYear = TblPupilManagementPupils.intNCYear
              INNER JOIN TblReportsManagementCycleAllocations ON --All pupils allocated display all years. 
              (intAllType = 0
               AND tblPupilManagementPupils.intSystemStatus = 1
               --Filter to specific years
               OR (intAllType = 1
                   AND intAllValue = tblSchoolManagementYears.intNCYear
                   AND blnExclusion = 0
                   AND tblPupilManagementPupils.intSystemStatus = 1))
              --Filter out specific years

              AND NOT tblSchoolManagementYears.intNCYEAR IN
         (
             SELECT DISTINCT
                    intNCYEAR
             FROM tblSchoolManagementYears
                  INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.intAllValue = tblSchoolManagementYears.intNCYear
                                                                     AND TblReportsManagementCycleAllocations.intAllType = 1
             WHERE intReportCycle = @intReportCycle
                   AND intReportType = 8
                   AND blnExclusion = 1
         )
         WHERE intReportCycle = @intReportCycle
               AND intReportType = 8
               AND blnExclusion = 0),
     BoardingAllocations
     AS (SELECT DISTINCT
                CAST(tblSchoolManagementHousesID AS VARCHAR(MAX)) AS txtGroupID,
                txtBoardingHouse AS txtGroupName,
                COALESCE(TblStaff.Title+' '+TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Title+' '+TblStaff.Surname) AS txtTeacher,
                tblPupilManagementPupils.intNCYear,
                tblPupilManagementPupils.txtForm,
                intTemplate,
                DENSE_RANK() OVER(PARTITION BY TblPupilManagementPupils.txtBoardingHouse,
                                               tblPupilManagementPupils.txtForm,
                                               tblPupilManagementPupils.intNCYear ORDER BY TblReportsManagementCycleAllocations.intOrder) AS intAllocationRank,
                CASE
                    WHEN TblReportsManagementCycleAllocations.intAllType = 0
                    THEN 'All Pupils'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 1
                    THEN 'Year Allocation'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 4
                    THEN 'House Allocation'
                END AS txtAllocationType,
                TblReportsManagementCycleAllocations.txtAllValue,
                TblReportsManagementCycleAllocations.intAllValue
         FROM TblPupilManagementPupils
              INNER JOIN tblSchoolManagementHouses ON tblSchoolManagementHouses.txtHouseName = txtBoardingHouse
              LEFT OUTER JOIN TblStaff ON tblSchoolManagementHouses.txtHousemaster = TblStaff.User_Code
              INNER JOIN TblReportsManagementCycleAllocations ON --All pupils allocated display all forms. 
              (intAllType = 0
               AND tblPupilManagementPupils.intSystemStatus = 1
               --Filter to forms that have year allocated.
               OR (intAllType = 1
                   AND intAllValue = TblPupilManagementPupils.intNCYear
                   AND blnExclusion = 0
                   AND tblPupilManagementPupils.intSystemStatus = 1)
               --Filter to specific Houses
               OR (intAllType = 4
                   AND txtAllValue = TblPupilManagementPupils.txtBoardingHouse
                   AND blnExclusion = 0
                   AND tblPupilManagementPupils.intSystemStatus = 1))

              --filters out houses that have pupils in year excluded
              AND NOT(txtSchoolID IN
                     (
                         SELECT DISTINCT
                                txtSchoolID
                         FROM tblPupilManagementPupils
                              INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.intAllValue = tblPupilManagementPupils.intNCYear
                                                                                 AND TblReportsManagementCycleAllocations.intAllType = 1
                         WHERE intReportCycle = @intReportCycle
                               AND intReportType = 9
                               AND blnExclusion = 1
                     )
                         --Filter out Houses that have pupils in Boardinghouses Excluded.
                         OR txtSchoolID IN
                     (
                         SELECT DISTINCT
                                txtSchoolID
                         FROM tblPupilManagementPupils
                              INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.txtAllValue = tblPupilManagementPupils.txtBoardingHouse
                                                                                 AND TblReportsManagementCycleAllocations.intAllType = 4
                         WHERE intReportCycle = @intReportCycle
                               AND intReportType = 9
                               AND blnExclusion = 1
                     ))
         WHERE intReportCycle = @intReportCycle
               AND intReportType = 9
               AND blnExclusion = 0
               AND txtBoardingHouse IS NOT NULL),
     AcademicAllocations
     AS (SELECT DISTINCT
                CAST(tblSchoolManagementHousesID AS VARCHAR(MAX)) AS txtGroupID,
                txtAcademicHouse AS txtGroupName,
                COALESCE(TblStaff.Title+' '+TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Title+' '+TblStaff.Surname) AS txtTeacher,
                tblPupilManagementPupils.intNCYear,
                tblPupilManagementPupils.txtForm,
                intTemplate,
                DENSE_RANK() OVER(PARTITION BY TblPupilManagementPupils.txtAcademicHouse,
                                               tblPupilManagementPupils.txtForm,
                                               tblPupilManagementPupils.intNCYear ORDER BY TblReportsManagementCycleAllocations.intOrder) AS intAllocationRank,
                CASE
                    WHEN TblReportsManagementCycleAllocations.intAllType = 0
                    THEN 'All Pupils'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 1
                    THEN 'Year Allocation'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 4
                    THEN 'House Allocation'
                END AS txtAllocationType,
                TblReportsManagementCycleAllocations.txtAllValue,
                TblReportsManagementCycleAllocations.intAllValue
         FROM TblPupilManagementPupils
              INNER JOIN tblSchoolManagementHouses ON tblSchoolManagementHouses.txtHouseName = txtAcademicHouse
              LEFT OUTER JOIN TblStaff ON tblSchoolManagementHouses.txtHousemaster = TblStaff.User_Code
              INNER JOIN TblReportsManagementCycleAllocations ON --All pupils allocated display all forms. 
              (intAllType = 0
               AND tblPupilManagementPupils.intSystemStatus = 1
               --Filter to house that have year allocated.
               OR (intAllType = 1
                   AND intAllValue = TblPupilManagementPupils.intNCYear
                   AND blnExclusion = 0
                   AND tblPupilManagementPupils.intSystemStatus = 1)
               --Filter to specific Houses
               OR (intAllType = 4
                   AND txtAllValue = TblPupilManagementPupils.txtAcademicHouse
                   AND blnExclusion = 0
                   AND tblPupilManagementPupils.intSystemStatus = 1))
              --Filter out house that have year excluded.
              AND NOT(intNCYEAR IN
                     (
                         SELECT DISTINCT
                                intNCYEAR
                         FROM tblPupilManagementPupils
                              INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.intAllValue = tblPupilManagementPupils.intNCYear
                                                                                 AND TblReportsManagementCycleAllocations.intAllType = 1
                         WHERE intReportCycle = @intReportCycle
                               AND intReportType = 9
                               AND blnExclusion = 1
                     )
                         --Filter out tutors that have pupils in academic houses Excluded.
                         OR txtAcademicHouse IN
                     (
                         SELECT DISTINCT
                                txtAcademicHouse
                         FROM tblPupilManagementPupils
                              INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.txtAllValue = tblPupilManagementPupils.txtAcademicHouse
                                                                                 AND TblReportsManagementCycleAllocations.intAllType = 4
                         WHERE intReportCycle = @intReportCycle
                               AND intReportType = 9
                               AND blnExclusion = 1
                     ))
         WHERE intReportCycle = @intReportCycle
               AND intReportType = 9
               AND blnExclusion = 0
               AND txtAcademicHouse IS NOT NULL),
     HeadTeacherAllocations
     AS (SELECT DISTINCT
                TblPupilManagementPupils.txtForm AS txtGroupID,
                TblPupilManagementPupils.txtForm AS txtGroupName,
                COALESCE(TblStaff.Title+' '+TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Title+' '+TblStaff.Surname) AS txtTeacher,
                tblPupilManagementPupils.intNCYear,
                tblPupilManagementPupils.txtForm,
                intTemplate,
                DENSE_RANK() OVER(PARTITION BY TblPupilManagementPupils.txtForm,
                                               tblPupilManagementPupils.txtForm,
                                               tblPupilManagementPupils.intNCYear ORDER BY TblReportsManagementCycleAllocations.intOrder) AS intAllocationRank,
                CASE
                    WHEN TblReportsManagementCycleAllocations.intAllType = 0
                    THEN 'All Pupils'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 1
                    THEN 'Year Allocation'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 3
                    THEN 'Form Allocation'
                END AS txtAllocationType,
                TblReportsManagementCycleAllocations.txtAllValue,
                TblReportsManagementCycleAllocations.intAllValue
         FROM TblPupilManagementPupils
              INNER JOIN tblSchoolManagementForms ON TblPupilManagementPupils.txtForm = tblSchoolManagementForms.txtForm
              LEFT OUTER JOIN TblStaff ON tblSchoolManagementForms.txtFormTutor = TblStaff.User_Code
              INNER JOIN TblReportsManagementCycleAllocations ON --All pupils allocated display all forms. 
              (intAllType = 0
               AND tblPupilManagementPupils.intSystemStatus = 1
               --Filter to forms that have year allocated.
               OR (intAllType = 1
                   AND intAllValue = TblPupilManagementPupils.intNCYear
                   AND blnExclusion = 0
                   AND tblPupilManagementPupils.intSystemStatus = 1)
               --Filter to specific forms
               OR (intAllType = 3
                   AND txtAllValue = TblPupilManagementPupils.txtForm
                   AND blnExclusion = 0
                   AND tblPupilManagementPupils.intSystemStatus = 1))
              --Filter out forms that have pupils in years excluded
              AND NOT(TblPupilManagementPupils.txtSchoolID IN
                     (
                         SELECT DISTINCT
                                TblPupilManagementPupils.txtSchoolID
                         FROM TblPupilManagementPupils
                              INNER JOIN TblReportsManagementCycleAllocations ON intAllValue = TblPupilManagementPupils.intNCYear
                                                                                 AND TblReportsManagementCycleAllocations.intAllType = 1
                         WHERE intReportCycle = @intReportCycle
                               AND intReportType = 10
                               AND blnExclusion = 1
                     )

                         --Filter out forms that have pupils in forms excluded
                         OR TblPupilManagementPupils.txtSchoolID IN
                     (
                         SELECT DISTINCT
                                TblPupilManagementPupils.txtSchoolID
                         FROM TblPupilManagementPupils
                              INNER JOIN TblReportsManagementCycleAllocations ON txtAllValue = TblPupilManagementPupils.txtForm
                                                                                 AND TblReportsManagementCycleAllocations.intAllType = 3
                         WHERE intReportCycle = @intReportCycle
                               AND intReportType = 10
                               AND blnExclusion = 1
                     ))
         WHERE intReportCycle = @intReportCycle
               AND intReportType = 10
               AND blnExclusion = 0),
     HOSAllocations
     AS (SELECT DISTINCT
            txtHOS AS txtGroupID,
            txtSetCode AS txtGroupName,
            COALESCE(TblStaff.Title+' '+TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Title+' '+TblStaff.Surname) AS txtTeacher,
            tblTeachingManagerSubjectsID,
            txtSubjectCode,
            txtSubjectName,
            txtSubjectReportName,
            tblPupilManagementPupils.intNCYear,
            tblPupilManagementPupils.txtForm,
            intTemplate,
            DENSE_RANK() OVER(PARTITION BY TblTeachingManagerSets.txtSetCode,
                                           tblPupilManagementPupils.intNCYear ORDER BY TblReportsManagementCycleAllocations.intOrder) AS intAllocationRank,
            'Set' AS txtGroupSubType,
            CASE
                WHEN TblReportsManagementCycleAllocations.intAllType = 0
                THEN 'All Pupils'
                WHEN TblReportsManagementCycleAllocations.intAllType = 1
                THEN 'Year Allocation'
                WHEN TblReportsManagementCycleAllocations.intAllType = 2
                THEN 'Subject Allocation'
                WHEN TblReportsManagementCycleAllocations.intAllType = 3
                THEN 'Form Allocation'
            END AS txtAllocationType,
            TblReportsManagementCycleAllocations.txtAllValue,
            TblReportsManagementCycleAllocations.intAllValue
     FROM TblTeachingManagerSets
          LEFT OUTER JOIN tblTeachingManagerSubjects ON TblTeachingManagerSets.intSubject = tblTeachingManagerSubjectsID
          LEFT OUTER JOIN tblTeachingManagerSetLists ON tblTeachingManagerSetLists.intSetID = TblTeachingManagerSetsID
          LEFT OUTER JOIN tblPupilManagementPupils ON tblTeachingManagerSetLists.txtSchoolID = tblPupilManagementPupils.txtSchoolID
		              LEFT OUTER JOIN TblTeachingManagerSubjectsHeads ON tblTeachingManagerSubjects.tblTeachingManagerSubjectsID = TblTeachingManagerSubjectsHeads.intSubject
              INNER JOIN  TBLStaff ON TblTeachingManagerSubjectsHeads.txtHOS = TBLStaff.User_Code
          INNER JOIN TblReportsManagementCycleAllocations ON --All pupils allocated display all sets.
          (intAllType = 0
           OR --Filter to sets that have year allocated.
           (intAllType = 1
            AND intAllValue = tblPupilManagementPupils.intNCYear
            AND blnExclusion = 0
            AND tblPupilManagementPupils.intSystemStatus = 1)
           --Filter to sets that have subject allocated.
           OR (intAllType = 2
               AND intAllValue = TblTeachingManagerSets.intSubject
               AND blnExclusion = 0)
           --Filter to sets that have forms allocated.
           OR (tblPupilManagementPupils.txtForm = txtAllValue
               AND intReportType = 13
               AND blnExclusion = 0
               AND tblPupilManagementPupils.intSystemStatus = 1))
          --Filter to sets that have pupils in years included.
          --    OR (TblReportsManagementCycleAllocations.intAllType = 1 AND TblTeachingManagerSetsID IN
          --(
          --    SELECT DISTINCT
          --           intSetID
          --    FROM tblTeachingManagerSetLists
          --         INNER JOIN tblPupilManagementPupils ON tblteachingmanagersetlists.txtschoolid = tblpupilmanagementpupils.txtschoolid
          --         INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.intAllValue = tblPupilManagementPupils.intNCYear
          --                                                            AND TblReportsManagementCycleAllocations.intAllType = 1
          --    WHERE intReportCycle = @intReportCycle
          --          AND intReportType = 13
          --          AND blnExclusion = 0
          --))
		
          AND NOT( --Filter out sets that have year excluded.
          tblPupilManagementPupils.txtSchoolID IN
                 (
                     SELECT DISTINCT
                            tblPupilManagementPupils.txtSchoolID
                     FROM tblteachingmanagersetlists
                          INNER JOIN tblPupilManagementPupils ON tblTeachingManagerSetLists.txtSchoolID = tblPupilManagementPupils.txtSchoolID
                          INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.intallvalue = tblPupilManagementPupils.intNCYear
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 1
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 13
                           AND blnExclusion = 1
                 )
                     --Filter out sets that have subject excluded.
                     OR TblTeachingManagerSetsID IN
                 (
                     SELECT DISTINCT
                            TblTeachingManagerSetsID
                     FROM tblteachingmanagersets
                          INNER JOIN TblReportsManagementCycleAllocations ON intAllValue = TblTeachingManagerSets.intSubject
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 2
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 13
                           AND blnExclusion = 1
                 )
                     --Filter out sets that have forms excluded.
                     OR tblTeachingManagerSetLists.txtSchoolID IN
                 (
                     SELECT DISTINCT
                            tblPupilManagementPupils.txtSchoolID
                     FROM tblteachingmanagersetlists
                          INNER JOIN tblPupilManagementPupils ON tblteachingmanagersetlists.txtschoolid = tblpupilmanagementpupils.txtschoolid
                          INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.txtAllValue = tblPupilManagementPupils.txtForm
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 3
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 13
                           AND blnExclusion = 1
                 )
                     --Filter out sets that have pupils excluded.
                     OR tblTeachingManagerSetLists.txtSchoolID IN
                 (
                     SELECT DISTINCT
                            tblPupilManagementPupils.txtSchoolID
                     FROM tblteachingmanagersetlists
                          INNER JOIN tblPupilManagementPupils ON tblteachingmanagersetlists.txtSchoolID = tblpupilmanagementpupils.txtSchoolID
                          INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.txtAllValue = tblpupilmanagementpupils.txtSchoolID
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 8
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 13
                           AND blnExclusion = 1
                 ))
     WHERE intReportCycle = @intReportCycle
           AND tblTeachingManagerSetLists.txtSchoolID &lt;&gt; ''
           AND intReportType = 13
     UNION ALL
     SELECT DISTINCT
            txtHOS AS txtGroupID,
            txtTimetableCode AS txtGroupName,
            COALESCE(TblStaff.Title+' '+TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Title+' '+TblStaff.Surname) AS txtTeacher,
            tblTeachingManagerSubjectsID,
            txtSubjectCode,
            txtSubjectName,
            txtSubjectReportName,
            tblPupilManagementPupils.intNCYear,
            tblPupilManagementPupils.txtForm,
            intTemplate,
            DENSE_RANK() OVER(PARTITION BY TblTeachingManagerSubjectForms.txtTimetableCode,
                                           tblPupilManagementPupils.intNCYear ORDER BY TblReportsManagementCycleAllocations.intOrder) AS intAllocationRank,
            'Teaching Form' AS txtGroupSubType,
            CASE
                WHEN TblReportsManagementCycleAllocations.intAllType = 0
                THEN 'All Pupils'
                WHEN TblReportsManagementCycleAllocations.intAllType = 1
                THEN 'Year Allocation'
                WHEN TblReportsManagementCycleAllocations.intAllType = 2
                THEN 'Subject Allocation'
                WHEN TblReportsManagementCycleAllocations.intAllType = 3
                THEN 'Form Allocation'
            END AS txtAllocationType,
            TblReportsManagementCycleAllocations.txtAllValue,
            TblReportsManagementCycleAllocations.intAllValue
     FROM TblTeachingManagerSubjectForms
          LEFT OUTER JOIN tblTeachingManagerSubjects ON TblTeachingManagerSubjectForms.intSubject = tblTeachingManagerSubjectsID
          LEFT OUTER JOIN tblSchoolManagementForms ON TblTeachingManagerSubjectForms.txtForm = tblSchoolManagementForms.txtForm
                    LEFT OUTER JOIN TblTeachingManagerSubjectsHeads ON tblTeachingManagerSubjects.tblTeachingManagerSubjectsID = TblTeachingManagerSubjectsHeads.intSubject
              INNER JOIN  TBLStaff ON TblTeachingManagerSubjectsHeads.txtHOS = TBLStaff.User_Code
          LEFT OUTER JOIN TblPupilManagementPupils ON tblSchoolManagementForms.txtForm = TblPupilManagementPupils.txtForm
          INNER JOIN TblReportsManagementCycleAllocations ON --All pupils allocated display all teaching forms.
          (intAllType = 0
           --Filter to teaching forms that have year allocated.
           OR (intAllType = 1
               AND intAllValue = TblPupilManagementPupils.intNCYear
               AND blnExclusion = 0
               AND tblPupilManagementPupils.intSystemStatus = 1)
           --Filter to teaching forms that have subject allocated.
           OR (intAllType = 2
               AND intAllValue = TblTeachingManagerSubjectForms.intSubject
               AND blnExclusion = 0)
           --Filter to teaching forms that have form allocated.
           OR (intAllType = 3
               AND txtAllValue = TblTeachingManagerSubjectForms.txtForm
               AND tblPupilManagementPupils.intSystemStatus = 1)
          --Filter to Teaching Forms that have pupils in years included
          -- OR (TblReportsManagementCycleAllocations.intAllType = 1 AND tblPupilManagementPupils.txtSchoolID IN
          --(
          --    SELECT DISTINCT
          --           tblPupilManagementPupils.txtSchoolID
          --    FROM tblPupilManagementPupils
          --         INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.intAllValue = tblPupilManagementPupils.intNCYear
          --                                                            AND TblReportsManagementCycleAllocations.intAllType = 1
          --    WHERE intReportCycle = @intReportCycle
          --          AND intReportType = 0
          --          AND blnExclusion = 1
          --))
          )
          AND NOT((intAllType = 1
                   AND intAllValue = tblSchoolManagementForms.intNCYear
                   AND blnExclusion = 1)
                  --Filter out teaching forms that have subject excluded.
                  OR TblTeachingManagerSubjectFormsID IN
                 (
                     SELECT DISTINCT
                            TblTeachingManagerSubjectFormsID
                     FROM TblTeachingManagerSubjectForms
                          INNER JOIN TblReportsManagementCycleAllocations ON intAllValue = TblTeachingManagerSubjectForms.intSubject
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 2
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 13
                           AND blnExclusion = 1
                 )
                     --Filter out Teaching Forms that have form excluded.
                     OR TblTeachingManagerSubjectFormsID IN
                 (
                     SELECT DISTINCT
                            TblTeachingManagerSubjectFormsID
                     FROM TblTeachingManagerSubjectForms
                          INNER JOIN TblReportsManagementCycleAllocations ON txtAllValue = TblTeachingManagerSubjectForms.txtForm
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 3
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 13
                           AND blnExclusion = 1
                 )
                     --Filter out Teaching Forms that have pupils in years excluded.
                     OR tblPupilManagementPupils.txtSchoolID IN
                 (
                     SELECT DISTINCT
                            txtSchoolID
                     FROM tblPupilManagementPupils
                          INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.intAllValue = tblPupilManagementPupils.intNCYear
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 1
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 13
                           AND blnExclusion = 1
                 )
                     --Filter out Teaching Forms that have pupils excluded.
                     OR txtSchoolID IN
                 (
                     SELECT DISTINCT
                            txtSchoolID
                     FROM tblPupilManagementPupils
                          INNER JOIN TblReportsManagementCycleAllocations ON TblReportsManagementCycleAllocations.txtAllValue = tblPupilManagementPupils.txtSchoolID
                                                                             AND TblReportsManagementCycleAllocations.intAllType = 8
                     WHERE intReportCycle = @intReportCycle
                           AND intReportType = 13
                           AND blnExclusion = 1
                 ))
			       WHERE intReportCycle = @intReportCycle
           AND TblPupilManagementPupils.txtSchoolID &lt;&gt; ''
           AND intReportType = 13
           AND blnExclusion = 0),
     ActivityGroupAllocation
     AS (SELECT DISTINCT
                CAST(TblActivityManagerGroupID AS VARCHAR(MAX)) AS txtGroupID,
                TblActivityManagerGroup.txtName AS txtGroupName,
                COALESCE(TblStaff.Title+' '+TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Firstname+' '+TblStaff.Surname, TblStaff.Title+' '+TblStaff.Surname) AS txtTeacher,
                tblPupilManagementPupils.intNCYear,
                tblPupilManagementPupils.txtForm,
                TblActivityManagerGroup.intActivity,
                TblActivityManagerActivity.txtCode AS txtSubjectCode,
                TblActivityManagerActivity.txtName AS txtSubjectName,
                intTemplate,
                DENSE_RANK() OVER(PARTITION BY TblActivityManagerGroup.TblActivityManagerGroupID,
                                               tblPupilManagementPupils.txtForm,
                                               tblPupilManagementPupils.intNCYear ORDER BY TblReportsManagementCycleAllocations.intOrder) AS intAllocationRank,
                DENSE_RANK() OVER(PARTITION BY TblActivityManagerGroupteacherlink.intGroup ORDER BY TblActivityManagerGroupteacherlink.txtUserCode) AS intTeacherRank,
                CASE
                    WHEN TblReportsManagementCycleAllocations.intAllType = 0
                    THEN 'All Pupils'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 1
                    THEN 'Year Allocation'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 9
                    THEN 'Activity Allocation'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 11
                    THEN 'Activity Group Allocation'
                    WHEN TblReportsManagementCycleAllocations.intAllType = 3
                    THEN 'Form Allocation'
                END AS txtAllocationType,
                TblReportsManagementCycleAllocations.txtAllValue,
                TblReportsManagementCycleAllocations.intAllValue
         FROM TblActivityManagerGroup
              INNER JOIN TblActivityManagerGroupPupilLink ON TblActivityManagerGroup.TblActivityManagerGroupId = TblActivityManagerGroupPupilLink.intGroup
              INNER JOIN TblPupilManagementPupils ON TblActivityManagerGroupPupilLink.txtSchoolID = TblPupilManagementPupils.txtSchoolID
              INNER JOIN TblActivityManagerActivity ON TblActivityManagerGroup.intActivity = TblActivityManagerActivityID
              LEFT OUTER JOIN TblActivityManagerGroupteacherlink ON TblActivityManagerGroup.TblActivityManagerGroupid = TblActivityManagerGroupteacherlink.intGroup
              LEFT OUTER JOIN TblStaff ON TblActivityManagerGroupteacherlink.txtUserCode = TblStaff.User_Code
              INNER JOIN TblReportsManagementCycleAllocations ON --All pupils allocated display all sets.
              (intAllType = 0
               AND tblPupilManagementPupils.intSystemStatus = 1
               ----Filter to sets that have year allocated.
               OR (intAllType = 1
                   AND intAllValue = TblPupilManagementPupils.intNCYear
                   AND blnExclusion = 0
                   AND tblPupilManagementPupils.intSystemStatus = 1)
               --Filter to groups that have activity allocated.
               OR (intAllType = 9
                   AND intAllValue = TblActivityManagerGroup.intActivity
                   AND blnExclusion = 0)
               --Filter to groups that have been allocated.
               OR (intAllType = 11
                   AND intAllValue = TblActivityManagerGroup.TblActivityManagerGroupID
                   AND blnExclusion = 0))
              AND NOT(TblPupilManagementPupils.txtSchoolID IN
                     (
                         SELECT DISTINCT
                                txtSchoolID
                         FROM TblPupilManagementPupils
                              INNER JOIN TblReportsManagementCycleAllocations ON intAllValue = TblPupilManagementPupils.intNCYear
                                                                                 AND TblReportsManagementCycleAllocations.intAllType = 1
                         WHERE intReportCycle = @intReportCycle
                               AND intReportType = 15
                               AND blnExclusion = 1
                     )

                         --Filters out  groups that have activity excluded.
                         OR (TblActivityManagerGroup.intActivity IN
                            (
                                SELECT DISTINCT
                                       TblActivityManagerGroup.intActivity
                                FROM TblActivityManagerGroup
                                     INNER JOIN TblReportsManagementCycleAllocations ON intAllValue = TblActivityManagerGroup.intActivity
                                                                                        AND TblReportsManagementCycleAllocations.intAllType = 9
                                WHERE intReportCycle = @intReportCycle
                                      AND intReportType = 15
                                      AND blnExclusion = 1
                            ))
                                --Filters out groups that have been excluded.
                                OR (TblActivityManagerGroup.TblActivityManagerGroupID IN
                                   (
                                       SELECT DISTINCT
                                              TblActivityManagerGroup.TblActivityManagerGroupID
                                       FROM TblActivityManagerGroup
                                            INNER JOIN TblReportsManagementCycleAllocations ON intAllValue = TblActivityManagerGroup.intActivity
                                                                                               AND TblReportsManagementCycleAllocations.intAllType = 11
                                       WHERE intReportCycle = @intReportCycle
                                             AND intReportType = 15
                                             AND blnExclusion = 1
                                   )))
         WHERE intReportCycle = @intReportCycle
               AND intReportType = 15
               AND blnExclusion = 0),
     Elements
     AS (
     SELECT TblReportsManagementTemplatesID,
            TblreportsManagementTemplates.txtTemplateName,
            TblreportsManagementTemplates.blnGeneralComment,
            txtGradingName AS txtElementName,
            STUFF(
                 (
                     SELECT ','+'Grade:'+txtGrade+' Transpose:'+CAST(intTransposeValue AS VARCHAR(MAX))
                     FROM TblReportsManagementTemplatesGradingValues
                     WHERE intTemplateGradingID = TblReportsManagementTemplatesGradingID
                     ORDER BY TblReportsManagementTemplatesGradingValuesID
                     FOR XML PATH('')
                 ), 1, 1, '') AS txtElementOptions,
            'Grade' AS txtElementType,
            DENSE_RANK() OVER(PARTITION BY TblReportsManagementTemplatesID ORDER BY TblReportsManagementTemplatesID,
                                                                                    TblReportsManagementTemplatesGradingID) AS intElementRank
     FROM TblReportsManagementTemplates
          LEFT OUTER JOIN TblReportsManagementTemplatesGrading ON TblReportsManagementTemplates.TblReportsManagementTemplatesID = TblReportsManagementTemplatesGrading.intTemplateID
     UNION ALL
     SELECT TblReportsManagementTemplatesID,
            TblreportsManagementTemplates.txtTemplateName,
            TblreportsManagementTemplates.blnGeneralComment,
            txtResultName AS txtElementName,
            NULL AS txtElementOptions,
            'Result' AS txtElementType,
            DENSE_RANK() OVER(PARTITION BY TblReportsManagementTemplatesID ORDER BY TblReportsManagementTemplatesID,
                                                                                    TblReportsManagementTemplatesResultsID) AS intElementRank
     FROM TblReportsManagementTemplates
          LEFT OUTER JOIN TblReportsManagementTemplatesResults ON TblReportsManagementTemplates.TblReportsManagementTemplatesID = TblReportsManagementTemplatesResults.intTemplateID
     UNION ALL
     SELECT TblReportsManagementTemplatesID,
            TblreportsManagementTemplates.txtTemplateName,
            TblreportsManagementTemplates.blnGeneralComment,
            txtCommentName AS txtElementName,
            NULL AS txtElementOptions,
            'Comment' AS txtElementType,
            DENSE_RANK() OVER(PARTITION BY TblReportsManagementTemplatesID ORDER BY TblReportsManagementTemplatesID,
                                                                                    TblReportsManagementTemplatesCommentsID) AS intElementRank
     FROM TblReportsManagementTemplates
          LEFT OUTER JOIN TblReportsManagementTemplatesComments ON TblReportsManagementTemplates.TblReportsManagementTemplatesID = TblReportsManagementTemplatesComments.intTemplateID
     UNION ALL
     SELECT TblReportsManagementTemplatesID,
            TblreportsManagementTemplates.txtTemplateName,
            TblreportsManagementTemplates.blnGeneralComment,
            txtPositionName AS txtElementName,
            NULL AS txtElementOptions,
            'Position' AS txtElementType,
            DENSE_RANK() OVER(PARTITION BY TblReportsManagementTemplatesID ORDER BY TblReportsManagementTemplatesID,
                                                                                    TblReportsManagementTemplatesPositionsID) AS intElementRank
     FROM TblReportsManagementTemplates
          LEFT OUTER JOIN TblReportsManagementTemplatesPositions ON TblReportsManagementTemplates.TblReportsManagementTemplatesID = TblReportsManagementTemplatesPositions.intTemplateID),
     Allocations
     AS (
     SELECT SubjectAllocations.txtGroupID AS txtGroupID,
            SubjectAllocations.txtGroupName AS txtGroupName,
            SubjectAllocations.txtTeacher AS txtTeacher,
            'Subject' AS txtGroupType,
            txtGroupSubType AS txtGroupSubType,
            1 AS txtGroupTypeNumber,
            SubjectAllocations.intSubject AS intSubject,
            SubjectAllocations.txtSubjectCode AS txtSubjectCode,
            SubjectAllocations.txtSubjectName AS txtSubjectName,
            SubjectAllocations.txtSubjectReportName AS txtReportSubjectName,
            SubjectAllocations.intNCYear AS intYear,
            SubjectAllocations.txtForm AS txtForm,
            intTemplate,
            txtAllocationType,
            intAllValue,
            txtAllValue
     FROM SubjectAllocations
     WHERE intAllocationRank = 1
     UNION ALL
     SELECT FormAllocations.txtGroupID AS txtGroupID,
            FormAllocations.txtGroupName AS txtGroupName,
            FormAllocations.txtTeacher AS txtTeacher,
            'Form' AS txtGroupType,
            'Form' AS txtGroupSubType,
            6 AS txtGroupTypeNumber,
            NULL AS intSubject,
            NULL AS txtSubjectCode,
            NULL AS txtSubjectName,
            NULL AS txtReportSubjectName,
            FormAllocations.intNCYear AS intYear,
            FormAllocations.txtForm AS txtForm,
            intTemplate,
            txtAllocationType,
            intAllValue,
            txtAllValue
     FROM FormAllocations
     WHERE intAllocationRank = 1
     UNION ALL
     SELECT PastoralAllocations.txtGroupID AS txtGroupID,
            PastoralAllocations.txtGroupName AS txtGroupName,
            PastoralAllocations.txtTeacher AS txtTeacher,
            'Pastoral' AS txtGroupType,
            'Pastoral' AS txtGroupSubType,
            7 AS txtGroupTypeNumber,
            NULL AS intSubject,
            NULL AS txtSubjectCode,
            NULL AS txtSubjectName,
            NULL AS txtReportSubjectName,
            PastoralAllocations.intNCYear AS intYear,
            PastoralAllocations.txtForm AS txtForm,
            intTemplate,
            txtAllocationType,
            intAllValue,
            txtAllValue
     FROM PastoralAllocations
     WHERE intAllocationRank = 1
     UNION ALL
     SELECT YearAllocation.txtGroupID AS txtGroupID,
            YearAllocation.txtGroupName AS txtGroupName,
            YearAllocation.txtTeacher AS txtTeacher,
            'Year' AS txtGroupType,
            'Year' AS txtGroupSubType,
            8 AS txtGroupTypeNumber,
