      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT  TblPupilManagementPupils.txtForename, TblPupilManagementPupils.txtSurname, TblPupilManagementPupils.txtSchoolID, TblPupilManagementPupils.txtAdmissionsStatus,
	   TblSchoolManagementYears.txtYearName, TblPupilManagementAddresses.txtLetterSalutation, TblPupilManagementAddressLink.intAddressID, TblFeeBillingAccountCode.txtAccountCode,
	   TblFeeBillingBillPayerAccountDetail.txtBankAccountName, TblFeeBillingBillPayerAccountDetail.txtBankSortCode, TblFeeBillingBillPayerAccountDetail.txtBankAccountNo, (CASE 
	   WHEN TblFeeBillingDirectDebitPaymentPlan.txtCode IS NULL THEN 'No Plan' ELSE TblFeeBillingDirectDebitPaymentPlan.txtCode END) AS 'txtCode',
	   TblFeeBillingDirectDebitPaymentPlan.txtName AS 'txtPaymentPlanName', TblFeeBillingDirectDebitPaymentPlanLinkType.txtName AS 'txtPaymentPlanLinkType',
	   TblFeeBillingDirectDebitPaymentPlanLink.TblFeeBillingDirectDebitPaymentPlanLinkID
FROM	   TblPupilManagementPupils
LEFT OUTER JOIN TblSchoolManagementYears ON TblSchoolManagementYears.intNCYear = TblPupilManagementPupils.intNCYear
INNER JOIN TblPupilManagementAddressLink ON TblPupilManagementAddressLink.txtSchoolID = TblPupilManagementPupils.txtSchoolID
INNER JOIN TblPupilManagementAddresses ON TblPupilManagementAddresses.TblPupilManagementAddressesID = TblPupilManagementAddressLink.intAddressID
	AND (TblPupilManagementAddresses.intBillingMailMerge = 1 OR TblPupilManagementAddresses.intMailMergeAll = 1)
LEFT OUTER JOIN TblFeeBillingBillPayerAccountDetail ON TblFeeBillingBillPayerAccountDetail.intAddressID = TblPupilManagementAddresses.TblPupilManagementAddressesID
LEFT OUTER JOIN TblFeeBillingAccountCode ON TblFeeBillingAccountCode.intAddressID = TblFeeBillingBillPayerAccountDetail.intAddressID
LEFT OUTER JOIN TblFeeBillingDirectDebitPaymentPlanLink ON TblFeeBillingDirectDebitPaymentPlanLink.intAccountDetailID = TblFeeBillingBillPayerAccountDetail.TblFeeBillingBillPayerAccountDetailID
LEFT OUTER JOIN TblFeeBillingDirectDebitPaymentPlan ON TblFeeBillingDirectDebitPaymentPlan.TblFeeBillingDirectDebitPaymentPlanID = TblFeeBillingDirectDebitPaymentPlanLink.intPlanID
LEFT OUTER JOIN TblFeeBillingDirectDebitPaymentPlanLinkType ON TblFeeBillingDirectDebitPaymentPlanLinkType.TblFeeBillingDirectDebitPaymentPlanLinkTypeID = TblFeeBillingDirectDebitPaymentPlan.intPlanType
WHERE  (TblFeeBillingDirectDebitPaymentPlanLink.bArchived = 0 OR TblFeeBillingDirectDebitPaymentPlanLink.bArchived IS NULL)
	   AND (TblFeeBillingBillPayerAccountDetail.bArchived = 0 OR TblFeeBillingBillPayerAccountDetail.bArchived IS NULL)
	   AND (TblFeeBillingDirectDebitPaymentPlan.bActive = 1 OR TblFeeBillingDirectDebitPaymentPlan.bActive IS NULL)
	   AND (TblPupilManagementPupils.txtSchoolID IN (SELECT ID FROM SplitCSV(@txtSchoolID)))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@txtSchoolID">
            <Value>=Parameters!txtSchoolID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
