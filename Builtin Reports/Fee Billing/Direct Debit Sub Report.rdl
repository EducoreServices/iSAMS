<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>251b798f-9090-4891-ba16-c92906c30aa8</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Invoice"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@TblFeeBillingInvoiceRunID"><Value>=Parameters!TblFeeBillingInvoiceRunID.Value</Value></QueryParameter><QueryParameter Name="@txtAccountCode"><Value>=Parameters!txtAccountCode.Value</Value></QueryParameter><QueryParameter Name="@intInvoiceNumber"><Value>=Parameters!intInvoiceNumber.Value</Value></QueryParameter></QueryParameters><CommandText>WITH MainDDTable AS
	(SELECT	DISTINCT
	txtPayeeType, intInvoiceID, dtInvoiceDateTime, intInvoiceNumber, txtPupilAccountCode, txtContactAccountCode, intInvoiceRunID
	 FROM	VwFeeBillingInvoicePrinting
	 WHERE	(VwFeeBillingInvoicePrinting.intInvoiceRunID = (@TblFeeBillingInvoiceRunID)) 
		AND (VwFeeBillingInvoicePrinting.txtContactAccountCode = (@txtAccountCode) OR @txtAccountCode IS NULL) 
		AND (VwFeeBillingInvoicePrinting.intInvoiceNumber = (@intInvoiceNumber) OR @intInvoiceNumber IS NULL) 
	
	)
	,DDFinalTable AS
	(SELECT	*
	 FROM	MainDDTable
	 Outer APPLY	(
	 SELECT	'Direct Debits' AS 'txtType', 
	 Schedule.TblFeeBillingDirectDebitScheduleID, 
	 Schedule.intPlanLinkID, 
	 Payments.TblFeeBillingDirectDebitScheduledPaymentId, 
	 Payments.dPaymentDate, 
	 Payments.mAmount, 
	 BankDetails.txtBankSortCode, 
	 BankDetails.txtBankAccountNo, 
	 BankDetails.txtBankAccountName, 
	 BankDetails.txtBankName, 
	 BankDetails.txtBankBranch, 
	 AccountCodes.txtAccountCode AS 'txtBillingAccountCode', 
	 TblPupilAccountCodeDetails.txtAccountCode                                              
					 FROM	TblFeeBillingDirectDebitSchedule AS Schedule
					 INNER JOIN TblFeeBillingDirectDebitScheduledPayment AS Payments ON Schedule.TblFeeBillingDirectDebitScheduleId = Payments.intScheduleId
					 INNER JOIN TblFeeBillingDirectDebitPaymentPlanLink AS Link ON Schedule.intPlanLinkID = Link.TblFeeBillingDirectDebitPaymentPlanLinkID
					 INNER JOIN TblFeeBillingBillPayerAccountDetail AS BankDetails ON Link.intAccountDetailID = BankDetails.TblFeeBillingBillPayerAccountDetailID
					 LEFT OUTER JOIN TblFeeBillingInvoiceRun AS InvoiceRun ON Schedule.intRunID = InvoiceRun.TblFeeBillingInvoiceRunId
					 INNER JOIN TblPupilManagementAddresses  ON TblPupilManagementAddresses.TblPupilManagementAddressesID = BankDetails.intAddressID
					 INNER JOIN TblFeeBillingAccountCode AS AccountCodes ON AccountCodes.intAddressID = BankDetails.intAddressID AND AccountCodes.intAccountType = 2
					 LEFT OUTER JOIN TblFeeBillingAccountCode AS TblPupilAccountCodeDetails ON Link.txtSchoolID = TblPupilAccountCodeDetails.txtSchoolID
					 LEFT OUTER JOIN TblPupilManagementPupils ON TblPupilManagementPupils.txtSchoolID = Link.txtSchoolID
					 LEFT OUTER JOIN TblFeeBillingDirectDebitPaymentPlan ON Link.intPlanID = TblFeeBillingDirectDebitPaymentPlan.TblFeeBillingDirectDebitPaymentPlanID
					 WHERE	MainDDTable.dtInvoiceDateTime &lt;= Payments.dPaymentDate

									     AND (MainDDTable.txtPupilAccountCode = TblPupilAccountCodeDetails.txtAccountCode
        OR TblPupilAccountCodeDetails.txtAccountCode IS NULL)    -- To cover DD for ALL PUPILS

							AND MainDDTable.txtContactAccountCode = AccountCodes.txtAccountCode
							AND Link.bArchived &lt;&gt; 1
							AND Schedule.intRunID IN (@TblFeeBillingInvoiceRunID)
							AND TblFeeBillingDirectDebitPaymentPlan.intTotalPayments &gt; 1
					 ) AS DDSchedule
	)



SELECT	
DDFinalTable.*

FROM	DDFinalTable
where txtType like 'Direct Debits'</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="txtType"><DataField>txtType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPayeeType"><DataField>txtPayeeType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intInvoiceRunID"><DataField>intInvoiceRunID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="TblFeeBillingDirectDebitScheduleID"><DataField>TblFeeBillingDirectDebitScheduleID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="intPlanLinkID"><DataField>intPlanLinkID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="intInvoiceID"><DataField>intInvoiceID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="dtInvoiceDateTime"><DataField>dtInvoiceDateTime</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="intInvoiceNumber"><DataField>intInvoiceNumber</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dPaymentDate"><DataField>dPaymentDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="mAmount"><DataField>mAmount</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="txtPupilAccountCode"><DataField>txtPupilAccountCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtContactAccountCode"><DataField>txtContactAccountCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtBankSortCode"><DataField>txtBankSortCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtBankAccountNo"><DataField>txtBankAccountNo</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtBankAccountName"><DataField>txtBankAccountName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtBankName"><DataField>txtBankName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtBankBranch"><DataField>txtBankBranch</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtBillingAccountCode"><DataField>txtBillingAccountCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAccountCode"><DataField>txtAccountCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TblFeeBillingDirectDebitScheduledPaymentId"><DataField>TblFeeBillingDirectDebitScheduledPaymentId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="InvoiceRun"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT        TblFeeBillingInvoiceRunId, txtName, dSubmitDateTime
