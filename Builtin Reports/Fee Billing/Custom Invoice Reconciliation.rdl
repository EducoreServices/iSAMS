    <DataSet Name="PreInvoice">
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@intCycleId">
            <Value>=Parameters!intCycleId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @MyTempTable TABLE
	(txtType NVARCHAR(20),
	 intID INTEGER,
	 txtAccountCode NVARCHAR(50),
	 txtSchoolID NVARCHAR(50),
	 txtSageCode NVARCHAR(50),
	 ChargeCode NVARCHAR(50),
	 txtDescription NVARCHAR(200),
	 mAmount DECIMAL(18,2),
	 percentageAmount DECIMAL(18,2),
	 txtChargeType NVARCHAR(50),
	 txtNominalAccount NVARCHAR(50),
	 bBilledInAdvance BIT  
	)

INSERT INTO @MyTempTable
EXEC	   usp_cust_GetTransactionsByCycle @intCycleId;

WITH Temp AS
	(SELECT	TblPupilManagementAddresses.TblPupilManagementAddressesID, Charges.*, TblPupilManagementPupils.txtForename, 
			TblPupilManagementPupils.txtSurname, TblFeeBillingCharge.txtDepartment, txtYearName, TblPupilManagementPupils.txtType 
			AS 'txtPupilType', TblFeeBillingCharge.txtExtraDescription, TblFeeBillingAccountCode.txtAccountCode AS 
			'txtBillingAccountCode', Splitbilling.intPercentage, Splitbilling.mFixedAmount
	 FROM	@MyTempTable AS Charges
	 INNER JOIN TblPupilManagementPupils ON Charges.txtSchoolID = TblPupilManagementPupils.txtSchoolID
	 LEFT OUTER JOIN TblFeeBillingCharge ON Charges.ChargeCode = TblFeeBillingCharge.ChargeCode
	 LEFT OUTER JOIN TblSchoolManagementYears ON TblPupilManagementPupils.intNCYear = TblSchoolManagementYears.intNCYear
	 INNER JOIN TblPupilManagementAddressLink ON TblPupilManagementAddressLink.txtSchoolID = TblPupilManagementPupils.txtSchoolID
	 INNER JOIN TblPupilManagementAddresses ON TblPupilManagementAddresses.TblPupilManagementAddressesID = TblPupilManagementAddressLink.intAddressID
			AND (TblPupilManagementAddresses.intBillingMailMerge = 1 OR TblPupilManagementAddresses.intMailMergeAll = 1)
	 LEFT OUTER JOIN TblFeeBillingAccountCode ON TblFeeBillingAccountCode.intAddressID = TblPupilManagementAddresses.TblPupilManagementAddressesID
	 OUTER APPLY (SELECT	TblFeeBillingSplitBillingRule.txtSchoolID, TblFeeBillingSplitBillingRule.intContactAddressID,
							TblFeeBillingSplitBillingRule.intChargeType, TblFeeBillingChargeType.txtName, 
							TblFeeBillingSplitBillingRule.intPercentage, TblFeeBillingSplitBillingRule.mFixedAmount
				  FROM		TblFeeBillingSplitBillingRule
				  INNER JOIN TblFeeBillingChargeType ON TblFeeBillingSplitBillingRule.intChargeType = TblFeeBillingChargeType.TblFeeBillingChargeTypeID
				  WHERE		Charges.txtSchoolID = TblFeeBillingSplitBillingRule.txtSchoolID 
							AND TblFeeBillingChargeType.txtName = Charges.txtChargeType 
							AND TblFeeBillingSplitBillingRule.intContactAddressID = TblPupilManagementAddresses.TblPupilManagementAddressesID
							AND (TblFeeBillingSplitBillingRule.intCycleID = -1 OR TblFeeBillingSplitBillingRule.intCycleID = @intCycleId)) AS Splitbilling

	)
	,TempPup AS 
	(SELECT DISTINCT txtSchoolID, txtChargeType, ChargeCode, SUM(ISNULL(intPercentage,0)) AS 'TotalPercentage'
	 FROM	Temp
	 GROUP BY txtSchoolID, txtChargeType, ChargeCode
	)

SELECT	Temp.*, CASE WHEN intPercentage IS NOT NULL THEN intPercentage ELSE 100 - ISNULL(TotalPercentage,0) END AS 
		'CalculatedPercentage'
FROM	Temp 
LEFT OUTER JOIN TempPup ON Temp.txtSchoolID = TempPup.txtSchoolID AND Temp.txtChargeType = TempPup.txtChargeType
		AND Temp.ChargeCode = TempPup.ChargeCode</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
