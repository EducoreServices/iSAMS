    <DataSet Name="PostInvoice">
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@intCycleId">
            <Value>=Parameters!intCycleId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT  TblFeeBillingInvoiceItem.TblFeeBillingInvoiceItemID AS intChargeID, TblFeeBillingInvoiceAuditPupil.txtAccountCode, 
        TblFeeBillingInvoiceAuditPupil.txtExistingSchoolID AS txtSchoolId, TblPupilManagementPupils.txtForename, TblPupilManagementPupils.txtSurname, 
        TblFeeBillingInvoiceItem.txtNominalAccount + TblFeeBillingInvoiceItem.txtCostCentre AS txtSageCode, TblFeeBillingInvoiceItem.ChargeCode AS txtChargeCode, 
        TblFeeBillingInvoiceItem.txtDescription, TblFeeBillingInvoiceItem.txtNominalAccount, TblFeeBillingInvoiceItem.txtDepartment, 
        TblFeeBillingInvoiceItem.mTotalAmount AS mAmount, TblFeeBillingCycle.txtName AS txtFeeBillingCycleName, txtYearName,
	    TblPupilManagementPupils.txtType, TblFeeBillingCharge.txtExtraDescription, TblFeeBillingAccountCode.txtAccountCode AS 'txtBillingAccountCode',
	    TblFeeBillingCharge.bBilledInAdvance

FROM	   TblFeeBillingInvoiceRun 
INNER JOIN TblFeeBillingInvoice ON TblFeeBillingInvoice.intInvoiceRun = TblFeeBillingInvoiceRun.TblFeeBillingInvoiceRunId 
INNER JOIN TblFeeBillingInvoiceItem ON TblFeeBillingInvoiceItem.intInvoiceID = TblFeeBillingInvoice.TblFeeBillingInvoiceID 
INNER JOIN TblFeeBillingInvoiceAuditPupil ON TblFeeBillingInvoiceItem.intAuditPupilID = TblFeeBillingInvoiceAuditPupil.TblFeeBillingInvoiceAuditPupilId 
INNER JOIN TblPupilManagementPupils ON TblFeeBillingInvoiceAuditPupil.txtExistingSchoolID = TblPupilManagementPupils.txtSchoolID 
INNER JOIN TblFeeBillingInvoiceAuditCycle ON TblFeeBillingInvoiceRun.intAuditCycleId = TblFeeBillingInvoiceAuditCycle.TblFeeBillingInvoiceAuditCycleId 
LEFT OUTER JOIN TblFeeBillingCharge ON TblFeeBillingInvoiceItem.ChargeCode = TblFeeBillingCharge.ChargeCode 
LEFT OUTER JOIN TblFeeBillingCycle ON TblFeeBillingInvoiceAuditCycle.intOriginalCycleId = TblFeeBillingCycle.TblFeeBillingCycleID
LEFT OUTER JOIN TblSchoolManagementYears ON TblPupilManagementPupils.intNCYear = TblSchoolManagementYears.intNCYear

INNER JOIN TblPupilManagementAddressLink ON TblPupilManagementAddressLink.txtSchoolID = TblPupilManagementPupils.txtSchoolID
INNER JOIN TblPupilManagementAddresses ON TblPupilManagementAddresses.TblPupilManagementAddressesID = TblPupilManagementAddressLink.intAddressID
	   AND (TblPupilManagementAddresses.intBillingMailMerge = 1 OR TblPupilManagementAddresses.intMailMergeAll = 1)
LEFT OUTER JOIN TblFeeBillingAccountCode ON TblFeeBillingAccountCode.intAddressID = TblPupilManagementAddresses.TblPupilManagementAddressesID
WHERE        (TblFeeBillingInvoiceAuditCycle.intOriginalCycleId = @intCycleId)</CommandText>
      </Query>
