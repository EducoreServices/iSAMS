    <DataSet Name="CycleRun">
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT        TblFeeBillingCycleID, txtName
FROM            TblFeeBillingCycle
ORDER BY dStartDate DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="TblFeeBillingCycleID">
          <DataField>TblFeeBillingCycleID</DataField>
