FROM	TblFeeBillingSettingsInvoice</CommandText></Query><Fields><Field Name="txtInvoiceGlobalPreInvoiceMessage"><DataField>txtInvoiceGlobalPreInvoiceMessage</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtInvoiceGlobalPostInvoiceMessage"><DataField>txtInvoiceGlobalPostInvoiceMessage</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="bSummariseInvoices"><DataField>bSummariseInvoices</DataField><rd:TypeName>System.Boolean</rd:TypeName></Field></Fields></DataSet><DataSet Name="iSAMSAddress"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT	txtExternaliSAMSAddress AS 'txtiSAMSAddress' 
