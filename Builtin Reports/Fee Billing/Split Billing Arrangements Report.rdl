/Style></TextRun><TextRun><Value xml:space="preserve"> </Value><Style><FontStyle>Italic</FontStyle><FontSize>9pt</FontSize></Style></TextRun><TextRun><Value>=Today()</Value><Style><FontStyle>Italic</FontStyle><FontSize>9pt</FontSize><Format>dd MMMM yyyy</Format></Style></TextRun></TextRuns><Style><TextAlign>Right</TextAlign></Style></Paragraph></Paragraphs><rd:DefaultName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Textbox165</rd:DefaultName><Top>0.17639cm</Top><Left>22.36548cm</Left><Height>0.5cm</Height><Width>6.33452cm</Width><Style><Border><Color>LightGrey</Color><Style>None</Style><Width>0.5pt</Width></Border><PaddingLeft>1pt</PaddingLeft><PaddingRight>1pt</PaddingRight><PaddingTop>1pt</PaddingTop><PaddingBottom>1pt</PaddingBottom></Style></Textbox><Textbox Name="Textbox166"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Page Number:</Value><Style><FontStyle>Italic</FontStyle><FontSize>9pt</FontSize><FontWeight>Bold</FontWeight></Style></TextRun><TextRun><Value xml:space="preserve"> </Value><Style><FontStyle>Italic</FontStyle><FontSize>9pt</FontSize></Style></TextRun><TextRun><Value>=Globals!PageNumber</Value><Style><FontStyle>Italic</FontStyle><FontSize>9pt</FontSize></Style></TextRun></TextRuns><Style><TextAlign>Center</TextAlign></Style></Paragraph></Paragraphs><rd:DefaultName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Textbox165</rd:DefaultName><Top>0.17639cm</Top><Left>9.9845cm</Left><Height>0.5cm</Height><Width>8.73099cm</Width><ZIndex>1</ZIndex><Style><Border><Color>LightGrey</Color><Style>None</Style><Width>0.5pt</Width></Border><PaddingLeft>1pt</PaddingLeft><PaddingRight>1pt</PaddingRight><PaddingTop>1pt</PaddingTop><PaddingBottom>1pt</PaddingBottom></Style></Textbox></ReportItems><Style><Border><Style>None</Style></Border></Style></PageFooter><PageHeight>21cm</PageHeight><PageWidth>29.7cm</PageWidth><LeftMargin>0.5cm</LeftMargin><RightMargin>0.5cm</RightMargin><TopMargin>0.5cm</TopMargin><BottomMargin>0.5cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">7897c2d3-844d-47cf-84b0-d3cc46e09e17</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="SplitBilling"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT        
pupilCodes.txtAccountCode, 
pupils.txtFullName, 
contactCodes.txtAccountCode AS txtContactAccountCode, 
contacts.txtLabelSalutation, 
CASE WHEN rules.intCycleId = - 1 THEN 'Persistent' ELSE cycles.txtName END AS 'Cycle', 
CASE WHEN chargeTypes.txtName IS NULL THEN 'All Charges' ELSE chargeTypes.txtName END AS 'Category', 
ruleTypes.txtName AS 'Rule Type', 
CASE WHEN rules.intPercentage IS NULL THEN rules.mFixedAmount ELSE rules.intPercentage END AS 'Amount'

FROM            TblFeeBillingSplitBillingRule AS rules 
INNER JOIN TblPupilManagementPupils AS pupils ON rules.txtSchoolID = pupils.txtSchoolID 
INNER JOIN TblPupilManagementAddresses AS contacts ON rules.intContactAddressID = contacts.TblPupilManagementAddressesID 
LEFT OUTER JOIN TblFeeBillingAccountCode AS pupilCodes ON pupils.txtSchoolID = pupilCodes.txtSchoolID 
LEFT OUTER JOIN TblFeeBillingAccountCode AS contactCodes ON contacts.TblPupilManagementAddressesID = contactCodes.intAddressID 
LEFT OUTER JOIN TblFeeBillingCycle AS cycles ON rules.intCycleID = cycles.TblFeeBillingCycleID 
INNER JOIN TblFeeBillingSplitBillingRuleType AS ruleTypes ON rules.intRuleType = ruleTypes.TblFeeBillingSplitBillingRuleTypeID 
LEFT OUTER JOIN TblFeeBillingChargeType AS chargeTypes ON rules.intChargeType = chargeTypes.TblFeeBillingChargeTypeID
WHERE        (pupils.intSystemStatus IN (1, 0))

UNION ALL

SELECT        
pupilCodes.txtAccountCode, 
pupils.txtFullName, contactCodes.txtAccountCode AS txtContactAccountCode, contacts.txtLabelSalutation, 
CASE WHEN rules.intCycleId = - 1 THEN 'Persistent' ELSE cycles.txtName END AS 'Cycle', 
'Contribution' AS 'Category', 
ruleTypes.txtName AS 'Rule Type', 
CASE WHEN subRule.intPercentage IS NULL THEN subRule.mFixedAmount ELSE subRule.intPercentage END AS 'Amount'

FROM            TblFeeBillingSplitBillingSubRule AS subRule 
INNER JOIN TblFeeBillingSplitBillingRule AS rules ON subRule.intPrimaryRuleID = rules.TblFeeBillingSplitBillingRuleID 
INNER JOIN TblPupilManagementPupils AS pupils ON rules.txtSchoolID = pupils.txtSchoolID 
INNER JOIN TblPupilManagementAddresses AS contacts ON subRule.intContactAddressID = contacts.TblPupilManagementAddressesID 
LEFT OUTER JOIN TblFeeBillingAccountCode AS pupilCodes ON pupils.txtSchoolID = pupilCodes.txtSchoolID 
LEFT OUTER JOIN TblFeeBillingAccountCode AS contactCodes ON contacts.TblPupilManagementAddressesID = contactCodes.intAddressID 
LEFT OUTER JOIN TblFeeBillingCycle AS cycles ON rules.intCycleID = cycles.TblFeeBillingCycleID 
INNER JOIN TblFeeBillingSplitBillingRuleType AS ruleTypes ON subRule.intRuleType = ruleTypes.TblFeeBillingSplitBillingRuleTypeID 
LEFT OUTER JOIN TblFeeBillingChargeType AS chargeTypes ON rules.intChargeType = chargeTypes.TblFeeBillingChargeTypeID
WHERE        (pupils.intSystemStatus IN (1, 0))

ORDER BY pupilCodes.txtAccountCode</CommandText></Query><Fields><Field Name="txtAccountCode"><DataField>txtAccountCode</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="txtFullName"><DataField>txtFullName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="txtContactAccountCode"><DataField>txtContactAccountCode</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="txtLabelSalutation"><DataField>txtLabelSalutation</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Cycle"><DataField>Cycle</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Category"><DataField>Category</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Rule_Type"><DataField>Rule Type</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Amount"><DataField>Amount</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field></Fields></DataSet><DataSet Name="Cycle"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT        TblFeeBillingCycleID, txtName
FROM            TblFeeBillingCycle
ORDER BY dStartDate DESC</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="TblFeeBillingCycleID"><DataField>TblFeeBillingCycleID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="txtName"><DataField>txtName</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields></DataSet></DataSets><EmbeddedImages><EmbeddedImage Name="iSAMSLine"><MIMEType>image/png</MIMEType><ImageData></ImageData></EmbeddedImage></EmbeddedImages><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Cm</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">98c3bc31-3cd8-42bb-a377-8061099df572</rd:ReportID></Report>