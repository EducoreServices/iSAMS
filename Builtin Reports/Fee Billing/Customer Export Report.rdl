        <Field Name="txtTelephone">
          <DataField>txtTelephone</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT  DISTINCT TblPupilManagementPupils.txtSchoolID, TblFeeBillingAccountCode.txtAccountCode, TblPupilManagementAddresses.txtLetterSalutation, TblPupilManagementPupils.txtForename,
	   TblPupilManagementPupils.intNCYear, TblPupilManagementAddresses.txtAddress1, TblPupilManagementAddresses.txtAddress2, TblPupilManagementAddresses.txtTown, 
	   TblPupilManagementAddresses.txtCounty, TblPupilManagementAddresses.txtPostcode, '' AS 'txtBlank', CASE WHEN txtAddressType = 'Work' Then txtWorkTelephone ELSE txtDayTelephone 
	   END AS 'txtTelephone'
FROM	   TblPupilManagementPupils
INNER JOIN TblPupilManagementAddressLink ON TblPupilManagementAddressLink.txtSchoolID = TblPupilManagementPupils.txtSchoolID
INNER JOIN TblPupilManagementAddresses ON TblPupilManagementAddressLink.intAddressID = TblPupilManagementAddresses.TblPupilManagementAddressesID
LEFT JOIN TblFeeBillingAccountCode ON TblPupilManagementPupils.txtSchoolID = TblFeeBillingAccountCode.txtSchoolID
WHERE ((TblPupilManagementAddresses.intMailMergeAll = 1 AND TblPupilManagementAddresses.intMailMergeAll IS NOT NULL)
	   OR (TblPupilManagementAddresses.intBillingMailMerge = 1 AND TblPupilManagementAddresses.intBillingMailMerge IS NOT NULL))
	 AND TblPupilManagementPupils.txtSchoolID IN (@txtSchoolID)
ORDER BY TblFeeBillingAccountCode.txtAccountCode</CommandText>
        <QueryParameters>
          <QueryParameter Name="@txtSchoolID">
            <Value>=Parameters!txtSchoolID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
