    <DataSet Name="Forms">
      <Fields>
        <Field Name="txtForm">
          <DataField>txtForm</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT  txtForm
FROM	   TblSchoolManagementForms
WHERE   intNCYear IN (@intNCYear)
ORDER BY intNCYear, txtForm</CommandText>
        <QueryParameters>
          <QueryParameter Name="@intNCYear">
            <Value>=Parameters!intNCYear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Pupils">
      <Fields>
        <Field Name="txtSchoolID">
          <DataField>txtSchoolID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="txtPupilName">
          <DataField>txtPupilName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT DISTINCT txtSchoolID, txtSurname + ', ' + txtForename AS 'txtPupilName'
FROM TblPupilManagementPupils
WHERE   (txtForm IN (@txtForm))
	   AND (intNCYear IN (@intNCYear))
	   AND  (intSystemStatus = 1)
ORDER BY txtPupilName</CommandText>
        <QueryParameters>
          <QueryParameter Name="@txtForm">
            <Value>=Parameters!txtForm.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@intNCYear">
            <Value>=Parameters!intNCYear.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
