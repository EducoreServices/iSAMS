ix></ReportItems><Height>2.16535in</Height><Style /></Body><Width>21.00001cm</Width><Page><PageHeight>29.7cm</PageHeight><PageWidth>21cm</PageWidth><LeftMargin>0cm</LeftMargin><RightMargin>0cm</RightMargin><TopMargin>0cm</TopMargin><BottomMargin>0cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>dda5be6d-8613-4e0a-aa70-be7b4446cfeb</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="SiblingDiscountInformation"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@CurrentAcademicYear"><Value>=Parameters!CurrentAcademicYear.Value</Value></QueryParameter><QueryParameter Name="@Date"><Value>=Parameters!Date.Value</Value></QueryParameter><QueryParameter Name="@txtADMStatus"><Value>=Parameters!txtADMStatus.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT  DISTINCT TblPupilManagementPupils.txtSchoolID, TblPupilManagementPupils.txtSurname, TblPupilManagementPupils.txtForename, (CASE WHEN TblPupilManagementPupils.intSystemStatus = 1 
	   THEN TblPupilManagementPupils.intNCYear + @CurrentAcademicYear ELSE TblPupilManagementPupils.intEnrolmentNCYear END) AS 'intNCYear', (CASE WHEN 
	   TblPupilManagementPupils.intSystemStatus = 1 THEN (SELECT  txtYearName FROM TblSchoolManagementYears AS CurrentYear WHERE CurrentYear.intNCYear = TblPupilManagementPupils.intNCYear 
	   + @CurrentAcademicYear) ELSE (SELECT txtYearName FROM TblSchoolManagementYears AS ApplicantYear WHERE ApplicantYear.intNCYear = TblPupilManagementPupils.intEnrolmentNCYear) END) AS 
	   'txtYearCode', TblPupilManagementPupils.intFamily, (SELECT TOP (1) txtSchoolID FROM TblPupilManagementPupils AS OldestSibling WHERE (intFamily = TblPupilManagementPupils.intFamily) 
	   AND (intFamily &lt;&gt; 0) AND (intSystemStatus IN (0, 1)) AND ((CASE WHEN (OldestSibling.intSystemStatus = 1 AND (@Date &gt;= OldestSibling.txtEnrolmentDate OR OldestSibling.txtEnrolmentDate 
	   IS NULL) AND (@Date &lt;= DATEADD(day, 1, OldestSibling.txtLeavingDate) OR OldestSibling.txtLeavingDate IS NULL)) THEN 'Yes' WHEN (OldestSibling.intSystemStatus = 0 AND (@Date &gt;= 
	   OldestSibling.txtEnrolmentDate)) THEN 'Yes' ELSE 'No' END) = 'Yes') AND ((CASE WHEN OldestSibling.intSystemStatus = 1 THEN 'Current' WHEN OldestSibling.txtAdmissionsStatus = '' OR
	   OldestSibling.txtAdmissionsStatus IS NULL THEN 'No ADM Status' ELSE OldestSibling.txtAdmissionsStatus END) IN (@txtADMStatus)) AND ((CASE WHEN OldestSibling.intSystemStatus = 1 THEN 
	   OldestSibling.intNCYear + @CurrentAcademicYear ELSE 0 END) &lt;= 13) ORDER BY (CASE WHEN OldestSibling.intSystemStatus = 1 THEN OldestSibling.intNCYear + @CurrentAcademicYear ELSE 
	   OldestSibling.intEnrolmentNCYear END) DESC) AS 'txtOldestSchoolID', (CASE WHEN (TblPupilManagementPupils.intSystemStatus = 1 AND (@Date &gt;= TblPupilManagementPupils.txtEnrolmentDate OR
