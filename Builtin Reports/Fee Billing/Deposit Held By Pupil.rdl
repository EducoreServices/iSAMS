htMargin><TopMargin>1cm</TopMargin><BottomMargin>1cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>1292bb5e-2181-487d-a9bc-bd149465f0e9</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="DepositHeldDetails"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@txtDepositType"><Value>=Parameters!txtDepositType.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT  TblPupilManagementPupils.intSystemStatus, TblPupilManagementPupils.txtForename, TblPupilManagementPupils.txtSurname, TblPupilManagementPupils.intNCYear, 
	   TblPupilManagementPupils.txtSchoolID, TblFeeBillingPupilDeposit.TblFeeBillingPupilDepositID, TblFeeBillingAccountCode.txtAccountCode, 0 AS 
	   'TblFeeBillingPupilDepositRefundLogID', TblFeeBillingPupilDeposit.mAmount, TblFeeBillingPupilDepositType.txtName AS 'txtDepositType', 
	   TblFeeBillingPupilDeposit.dPaidDate, TblFeeBillingPupilDeposit.txtNotes	   
FROM	   TblFeeBillingPupilDeposit 
LEFT OUTER JOIN TblPupilManagementPupils ON TblFeeBillingPupilDeposit.txtSchoolID = TblPupilManagementPupils.txtSchoolID 
LEFT OUTER JOIN TblFeeBillingAccountCode TblFeeBillingAccountCode ON TblPupilManagementPupils.txtSchoolID = TblFeeBillingAccountCode.txtSchoolID 
LEFT OUTER JOIN TblFeeBillingPupilDepositType ON TblFeeBillingPupilDeposit.intDepositTypeID = TblFeeBillingPupilDepositType.TblFeeBillingPupilDepositTypeID
WHERE   (TblFeeBillingPupilDepositType.txtName IN (@txtDepositType))
    UNION ALL
SELECT  TblPupilManagementPupils.intSystemStatus, TblPupilManagementPupils.txtForename, TblPupilManagementPupils.txtSurname, TblPupilManagementPupils.intNCYear, 
	   TblPupilManagementPupils.txtSchoolID, TblFeeBillingPupilDeposit.TblFeeBillingPupilDepositID, TblFeeBillingAccountCode.txtAccountCode, 
	   TblFeeBillingPupilDepositRefundLog.TblFeeBillingPupilDepositRefundLogID, - TblFeeBillingPupilDepositRefundLog.mAmountRefunded, 'Refund', 
	   TblFeeBillingPupilDepositRefundLog.dDateTimeRefunded, ''
FROM	   TblFeeBillingPupilDeposit TblFeeBillingPupilDeposit
LEFT OUTER JOIN TblPupilManagementPupils TblPupilManagementPupils ON TblFeeBillingPupilDeposit.txtSchoolID = TblPupilManagementPupils.txtSchoolID 
LEFT OUTER JOIN TblFeeBillingAccountCode TblFeeBillingAccountCode ON TblPupilManagementPupils.txtSchoolID = TblFeeBillingAccountCode.txtSchoolID 
LEFT OUTER JOIN TblFeeBillingPupilDepositType TblFeeBillingPupilDepositType ON TblFeeBillingPupilDeposit.intDepositTypeID = TblFeeBillingPupilDepositType.TblFeeBillingPupilDepositTypeID 
LEFT OUTER JOIN TblFeeBillingPupilDepositRefundLog ON TblFeeBillingPupilDeposit.TblFeeBillingPupilDepositID = TblFeeBillingPupilDepositRefundLog.intDepositID
WHERE   ('Refund' IN (@txtDepositType))
	   AND TblFeeBillingPupilDepositRefundLog.mAmountRefunded IS NOT NULL</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="intSystemStatus"><DataField>intSystemStatus</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intNCYear"><DataField>intNCYear</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSchoolID"><DataField>txtSchoolID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TblFeeBillingPupilDepositID"><DataField>TblFeeBillingPupilDepositID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtAccountCode"><DataField>txtAccountCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TblFeeBillingPupilDepositRefundLogID"><DataField>TblFeeBillingPupilDepositRefundLogID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="mAmount"><DataField>mAmount</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="txtDepositType"><DataField>txtDepositType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dPaidDate"><DataField>dPaidDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtNotes"><DataField>txtNotes</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields><Filters><Filter><FilterExpression>=Fields!txtSchoolID.Value</FilterExpression><Operator>In</Operator><FilterValues><FilterValue>=Parameters!SelectPupil.Value</FilterValue></FilterValues></Filter></Filters></DataSet><DataSet Name="DepositTypes"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT  txtName 
FROM	   TblFeeBillingPupilDepositType
WHERE   txtName &lt;&gt; 'Registration Fee'
    UNION ALL
SELECT  'Refund'
ORDER BY txtName</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="txtName"><DataField>txtName</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="Pupils"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>WITH PupilTable AS
    (SELECT TblPupilManagementPupils.intSystemStatus AS 'Status', TblPupilManagementPupils.txtForename AS 'Forename', TblPupilManagementPupils.txtSurname AS 'Surname', 
		  TblPupilManagementPupils.intNCYear AS 'Year', TblPupilManagementPupils.txtSchoolID AS 'SchoolID', TblFeeBillingAccountCode.txtAccountCode AS 'AC_Code', 
		  TblPupilManagementPupils.txtFullName AS 'FullName'
     FROM	  TblFeeBillingPupilDeposit 
     LEFT OUTER JOIN TblPupilManagementPupils ON TblFeeBillingPupilDeposit.txtSchoolID = TblPupilManagementPupils.txtSchoolID 
     LEFT OUTER JOIN TblFeeBillingAccountCode TblFeeBillingAccountCode ON TblPupilManagementPupils.txtSchoolID = TblFeeBillingAccountCode.txtSchoolID 
     LEFT OUTER JOIN TblFeeBillingPupilDepositType ON TblFeeBillingPupilDeposit.intDepositTypeID = TblFeeBillingPupilDepositType.TblFeeBillingPupilDepositTypeID
	   UNION ALL
     SELECT DISTINCT TblPupilManagementPupils.intSystemStatus, TblPupilManagementPupils.txtForename, TblPupilManagementPupils.txtSurname, TblPupilManagementPupils.intNCYear, 
		  TblPupilManagementPupils.txtSchoolID, TblFeeBillingAccountCode.txtAccountCode, TblPupilManagementPupils.txtFullName
     FROM	  TblFeeBillingPupilDeposit TblFeeBillingPupilDeposit
     LEFT OUTER JOIN TblPupilManagementPupils TblPupilManagementPupils ON TblFeeBillingPupilDeposit.txtSchoolID = TblPupilManagementPupils.txtSchoolID 
     LEFT OUTER JOIN TblFeeBillingAccountCode TblFeeBillingAccountCode ON TblPupilManagementPupils.txtSchoolID = TblFeeBillingAccountCode.txtSchoolID 
     LEFT OUTER JOIN TblFeeBillingPupilDepositType AS TblFeeBillingPupilDepositType ON TblFeeBillingPupilDeposit.intDepositTypeID = TblFeeBillingPupilDepositType.TblFeeBillingPupilDepositTypeID 
     LEFT OUTER JOIN TblFeeBillingPupilDepositRefundLog ON TblFeeBillingPupilDeposit.TblFeeBillingPupilDepositID = TblFeeBillingPupilDepositRefundLog.intDepositID
     WHERE  TblFeeBillingPupilDepositRefundLog.mAmountRefunded IS NOT NULL
    )

SELECT  DISTINCT Status, Forename, Surname, Year, SchoolID, AC_Code, FullName
FROM	   PupilTable
ORDER BY Surname, Forename</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="Status"><DataField>Status</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Forename"><DataField>Forename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Surname"><DataField>Surname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Year"><DataField>Year</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="SchoolID"><DataField>SchoolID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="AC_Code"><DataField>AC_Code</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="Currency"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT txtiSAMSCurrencySymbol, 
