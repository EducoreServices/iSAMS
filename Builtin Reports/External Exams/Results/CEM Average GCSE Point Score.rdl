<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>292d6e93-dc04-438b-a0ca-1888354d3ec1</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="EntriesResults"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@ExamEntryCycle"><Value>=Parameters!ExamEntryCycle.Value</Value></QueryParameter><QueryParameter Name="@ResultsCycle"><Value>=Parameters!ResultsCycle.Value</Value></QueryParameter></QueryParameters><CommandText>With ExamEntries AS (
SELECT DISTINCT 
			CASE WHEN pupils.intSystemStatus =  10 AND pupils.intIsCandidate = 1
			   OR pupils.txtSchoolID IN ( SELECT txtSchoolID FROM TblExamManagerReturningCandidates
								   ) THEN 10
				ELSE 1 END AS intStatus , 
			 pupils.txtSchoolID , 
			 options.txtQualification , 
			 modules.txtSubject , 
			 uabs.txtUABName , 
			 programmes.txtHeaderName,
			DENSE_RANK(
							)OVER( PARTITION BY  pupils.txtSchoolID  ORDER BY  programmes.txtHeaderName
								)AS Subjectrank
  FROM
	  TblExamManagerEntries AS entries
	  INNER JOIN
	  TblPupilManagementPupils AS pupils
	  ON entries.txtSchoolID = pupils.txtSchoolID
	  INNER JOIN
	  TblExamManagerTeachingProgram AS modules
	  ON entries.txtModuleCode = modules.txtModuleCode
	 AND entries.intUABCode = modules.intUABCode
	  INNER JOIN
	  TblExamManagerTeachingProgramHeaders AS programmes
	  ON modules.intProgram = programmes.TblExamManagerTeachingProgramHeadersID
	  INNER JOIN
	  TblExamManagerBaseDataOptions AS options
	  ON entries.txtModuleCode = options.txtOptionEntryCode
	  INNER JOIN
	  TblExamsManagerUABIdentifiers AS uabs
	  ON entries.intUABCode = uabs.intUABCode
Where entries.intCycle IN (@ExamEntryCycle) ) ,

Results AS 
(
SELECT

ROW_NUMBER() OVER(ORDER BY results.ResultID) AS intResultID, 
results.Cycle, 
results.SchoolId AS txtSchoolID, 
results.Surname, 
results.Forename,
results.CandidateSurname, 
results.CandidateForenames, 
Coalesce(PupilArchive.txtForm,TblPupilManagementPupils.txtForm,TblPupilManagementPupils.txtLeavingForm) AS txtForm,
results.UCI, 
results.CandidateNumber, 
results.OptionCode, 
results.OptionTitle, 
results.UABCode, 
results.UABName, 
results.CertificationType, 
results.Qualification, 
results.QualificationLevel, 
results.Programme, 
results.FirstGrade, 
results.ResultGradeset, 

results.TakenInNCYear, 
results.Subject,
gradesets.txtGradesetCode,
gradesets.intSequenceNumber
FROM VwExamManagerResults AS results
INNER JOIN TblExamManagerCycles ON results.Cycle = TblExamManagerCycles.TblExamManagerCyclesID
INNER JOIN TblExamManagerGradesets AS Gradesets ON gradesets.txtGradesetCode = results.ResultGradeset AND Gradesets.txtGradeShortCode = results.FirstGrade
--
INNER JOIN TblExamManagerGradesetHistory AS gradesetFiles ON gradesets.intGradesetID = gradesetFiles.TblExamManagerGradesetHistoryID  AND gradesetFiles.intCycle = results.Cycle 
LEFT OUTER JOIN TblExamManagerCandidateDetail AS candDetail ON results.SchoolId = candDetail.txtSchoolID
CROSS APPLY dbo.uf_Registration_getPupilArchive (results.SchoolId,Convert(Date,Convert(nvarchar(2),TblExamManagerCycles.intFormatMonth) + '-13-' + Convert(nvarchar(4),TblExamManagerCycles.intFormatYear))) AS PupilArchive
LEFT OUTER JOIN TblPupilManagementPupils ON results.SchoolId = TblPupilManagementPupils.txtSchoolID
WHERE results.FirstGrade &lt;&gt;''
AND results.FirstGrade IS NOT NULL
AND (results.Cycle IN (@ResultsCycle)) 
and CertificationType &lt;&gt; 'u'
and FirstGrade &lt;&gt; 'X' AND FirstGrade &lt;&gt; '0'
and (Qualification = 'GCSE' or Qualification = 'IGCSE') 

),

DoubleGrades AS
(
SELECT DISTINCT
gradesets.txtGradesetCode

FROM         TblExamManagerGradesets AS gradesets 
INNER JOIN 
TblExamManagerGradesetHistory AS gradesetFiles ON gradesets.intGradesetID = gradesetFiles.TblExamManagerGradesetHistoryID 
LEFT OUTER JOIN VwExamManagerResults AS results ON gradesets.txtGradesetCode = results.ResultGradeset AND gradesetFiles.intCycle = results.Cycle

WHERE 
results.Cycle IN (@ResultsCycle)
AND 
gradesets.intSequenceNumber = 1
AND gradesets.txtGradeShortCode IN ('**','99')
and CertificationType &lt;&gt; 'u'
and FirstGrade &lt;&gt; 'X' AND FirstGrade &lt;&gt; '0'
and (Qualification = 'GCSE' or Qualification = 'IGCSE') 
),

AllGrades AS
(
SELECT
Results.txtGradesetCode,
Results.intSequenceNumber,
results.intResultID,
results.Cycle, 
results.txtSchoolId, 
results.Surname, 
results.Forename,
results.CandidateSurname, 
results.CandidateForenames, 
results.UCI, 
results.CandidateNumber, 
results.OptionCode, 
results.OptionTitle, 
results.UABCode, 
results.UABName, 
results.CertificationType, 
results.Qualification, 
results.QualificationLevel, 
results.Programme, 
results.FirstGrade, 
results.ResultGradeset,  
results.TakenInNCYear, 
results.txtForm,
TblSchoolManagementYears.txtYearName,
results.Subject
FROM Results
LEFT OUTER JOIN TblSchoolManagementYears ON results.TakenInNCYear = TblSchoolManagementYears.intNCYear 
WHERE (results.txtGradesetCode NOT IN (SELECT txtGradesetCode FROM DoubleGrades))

UNION ALL


SELECT
Results.txtGradesetCode,
Results.intSequenceNumber,
results.intResultID,
results.Cycle, 
results.txtSchoolId, 
results.Surname, 
results.Forename,
results.CandidateSurname, 
results.CandidateForenames, 
results.UCI, 
results.CandidateNumber, 
results.OptionCode, 
results.OptionTitle, 
results.UABCode, 
results.UABName, 
results.CertificationType, 
results.Qualification, 
results.QualificationLevel, 
results.Programme, 
Replace(Left(results.FirstGrade,1),'*','A*') AS FirstGrade, 
results.ResultGradeset, 
results.TakenInNCYear, 
results.txtForm,
TblSchoolManagementYears.txtYearName,
results.Subject + ' 1' AS Subject
FROM Results
LEFT OUTER JOIN TblSchoolManagementYears ON results.TakenInNCYear = TblSchoolManagementYears.intNCYear 
WHERE (results.txtGradesetCode IN (SELECT txtGradesetCode FROM DoubleGrades))

UNION ALL

SELECT
Results.txtGradesetCode,
Results.intSequenceNumber, 
results.intResultID,
results.Cycle, 
results.txtSchoolId, 
results.Surname, 
results.Forename,
results.CandidateSurname, 
results.CandidateForenames, 
results.UCI, 
results.CandidateNumber, 
results.OptionCode, 
results.OptionTitle, 
results.UABCode, 
results.UABName, 
results.CertificationType, 
results.Qualification, 
results.QualificationLevel, 
results.Programme, 
Replace(Right(results.FirstGrade,1),'*','A*') AS FirstGrade, 
results.ResultGradeset, 
results.TakenInNCYear, 
results.txtForm,
TblSchoolManagementYears.txtYearName,
results.Subject + ' 2' AS Subject
FROM Results
LEFT OUTER JOIN TblSchoolManagementYears ON results.TakenInNCYear = TblSchoolManagementYears.intNCYear 
WHERE (results.txtGradesetCode IN (SELECT txtGradesetCode FROM DoubleGrades))
)
SELECT
AllGrades.*
,TblExamManagerCandidateDetail.txtUniqueLearnerNumber
,ExamEntries.txtHeaderName
,txtPostcode
,Pupils.txtUPN
,Pupils.txtDOB
,txtGender
,	Pupils.txtEthnicGroup
,	CensusEthnicityData.txtCensusValue as txtEthnicCode
	,ExamEntries.txtQualification
	,ExamEntries.txtSubject
	,SubjectRank
	,(cast (ExamEntries.txtQualification as nvarchar) + ';' + cast(ExamEntries.txtHeaderName AS nvarchar)) AS txtQualifcationHeaderName
FROM AllGrades

LEFT OUTER JOIN 
ExamEntries
ON AllGrades.txtSchoolID = ExamEntries.txtSchoolID 

LEFT OUTER JOIN
TblExamManagerCandidateDetail
ON AllGrades.txtschoolid = TblExamManagerCandidateDetail.txtschoolid

LEFT OUTER JOIN  
TblPupilManagementPupils as Pupils on AllGrades.txtschoolid = Pupils.txtSchoolID

OUTER APPLY (Select top (1) txtPostcode
			FROM TblPupilManagementAddressLink 
					INNER JOIN TblPupilManagementAddresses on TblPupilManagementAddressLink.intaddressid = TblPupilManagementAddresses.TblPupilManagementAddressesID
			WHERE TblPupilManagementAddressLink.txtSchoolID = Pupils.txtSchoolID and 
			TblPupilManagementAddresses.intStudentHome = 1
			ORDER BY intOrder) 
			as HomeAddress

OUTER APPLY (
		    SELECT TOP (1) txtCensusValue 
			From TblUsefulLists
				LEFT OUTER JOIN TblUsefulListsCensusValue ON TblUsefulLists.TblUsefulListsid = TblUsefulListsCensusValue.tblusefullistsid
			WHERE  txtEthnicGroup= TblUsefulLists.Name) AS  CensusEthnicityData</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="txtGradesetCode"><DataField>txtGradesetCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intSequenceNumber"><DataField>intSequenceNumber</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="intResultID"><DataField>intResultID</DataField><rd:TypeName>System.Int64</rd:TypeName></Field><Field Name="Cycle"><DataField>Cycle</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSchoolId"><DataField>txtSchoolId</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Surname"><DataField>Surname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Forename"><DataField>Forename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CandidateSurname"><DataField>CandidateSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CandidateForenames"><DataField>CandidateForenames</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="UCI"><DataField>UCI</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CandidateNumber"><DataField>CandidateNumber</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="OptionCode"><DataField>OptionCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="OptionTitle"><DataField>OptionTitle</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="UABCode"><DataField>UABCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="UABName"><DataField>UABName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CertificationType"><DataField>CertificationType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Qualification"><DataField>Qualification</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="QualificationLevel"><DataField>QualificationLevel</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Programme"><DataField>Programme</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FirstGrade"><DataField>FirstGrade</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="ResultGradeset"><DataField>ResultGradeset</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TakenInNCYear"><DataField>TakenInNCYear</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtForm"><DataField>txtForm</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtYearName"><DataField>txtYearName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Subject"><DataField>Subject</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtUniqueLearnerNumber"><DataField>txtUniqueLearnerNumber</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtHeaderName"><DataField>txtHeaderName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPostcode"><DataField>txtPostcode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtUPN"><DataField>txtUPN</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtDOB"><DataField>txtDOB</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtGender"><DataField>txtGender</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEthnicGroup"><DataField>txtEthnicGroup</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEthnicCode"><DataField>txtEthnicCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtQualification"><DataField>txtQualification</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSubject"><DataField>txtSubject</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="SubjectRank"><DataField>SubjectRank</DataField><rd:TypeName>System.Int64</rd:TypeName></Field><Field Name="txtQualifcationHeaderName"><DataField>txtQualifcationHeaderName</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="ExamCycle"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT        
TblExamManagerCyclesID, 
DATENAME(month, CONVERT(datetime, '2007-' + CAST(intFormatMonth AS varchar(2)) + '-01', 120)) + ' ' + CAST(intFormatYear AS nvarchar(4)) AS txtName
FROM            TblExamManagerCycles

ORDER BY 
intFormatYear desc, intFormatMonth desc, TblExamManagerCyclesID desc</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="TblExamManagerCyclesID"><DataField>TblExamManagerCyclesID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtName"><DataField>txtName</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="ExamEntryCycle"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>Select 
0 AS TblExamManagerCyclesID, 
'None' AS txtName,
10000000000000000 AS intFormatMonth,
10000000000000000 AS intFormatYear

UNION ALL
SELECT       
TblExamManagerCyclesID, 
DATENAME(month, CONVERT(datetime, '2007-' + CAST(intFormatMonth AS varchar(2)) + '-01', 120)) + ' ' + CAST(intFormatYear AS nvarchar(4)) AS txtName,
intFormatMonth,
intFormatYear
FROM            TblExamManagerCycles

ORDER BY 
intFormatYear desc, intFormatMonth desc, TblExamManagerCyclesID desc</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="TblExamManagerCyclesID"><DataField>TblExamManagerCyclesID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtName"><DataField>txtName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intFormatMonth"><DataField>intFormatMonth</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="intFormatYear"><DataField>intFormatYear</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><ReportItems><Tablix Name="Tablix1"><TablixBody><TablixColumns><TablixColumn><Width>2.5cm</Width></TablixColumn><TablixColumn><Width>2.5cm</Width></TablixColumn><TablixColumn><Width>2.5cm</Width></TablixColumn><TablixColumn><Width>2.5cm</Width></TablixColumn><TablixColumn><Width>3.66417cm</Width></TablixColumn><TablixColumn><Width>2.5cm</Width></TablixColumn><TablixColumn><Width>2.5cm</Width></TablixColumn><TablixColumn><Width>2.5cm</Width></TablixColumn><TablixColumn><Width>2.5cm</Width></TablixColumn><TablixColumn><Width>2.76458cm</Width></TablixColumn><TablixColumn><Width>5.48979cm</Width></TablixColumn><TablixColumn><Width>2.5cm</Width></TablixColumn></TablixColumns><TablixRows><TablixRow><Height>0.5cm</Height><TablixCells><TablixCell><CellContents><Textbox Name="Textbox1"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>First name</Value><Style><FontStyle>Normal</FontStyle><FontWeight>Bold</FontWeight><TextDecoration>None</TextDecoration></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>Textbox1</rd:DefaultName><Style><Border><Style>Solid</Style><Width>0.5pt</Width></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></Ce