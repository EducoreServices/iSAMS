<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>RedStaging</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>9982f9d1-3824-4a19-a13d-79631e54464f</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Results"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intCycle"><Value>=Parameters!intCycle.Value</Value></QueryParameter><QueryParameter Name="@TblExamManagerTeachingProgramHeadersID"><Value>=Parameters!TblExamManagerTeachingProgramHeadersID.Value</Value></QueryParameter><QueryParameter Name="@txtModuleCode"><Value>=Parameters!txtModuleCode.Value</Value></QueryParameter><QueryParameter Name="@txtComponentCode"><Value>=Parameters!txtComponentCode.Value</Value></QueryParameter></QueryParameters><CommandText>DECLARE @txtSQA AS nvarchar(3);
SET @txtSQA = (SELECT TOP(1)  CASE WHEN blnSQAEnabled = 1 THEN 'Yes' ELSE 'No' END AS txtSQAOn FROM TblExamManagerConfiguration);

WITH ExamResults AS
(

-- standard results
SELECT
'JCQ' AS txtResultType,
VwExamManagerResults.SchoolId AS txtSchoolID,
VwExamManagerResults.ResultID AS intResultID,
VwExamManagerResults.UABName AS txtResultExamBoard,
VwExamManagerResults.OptionCode AS txtResultOptionCode,
VwExamManagerResults.OptionTitle  AS txtResultOptionDescription,
TblExamManagerBaseDataOptions.txtLevel AS txtResultLevel,
TblExamManagerBaseDataOptions.txtQualification AS txtResultQualification,
VwExamManagerResults.Subject AS txtResultSubject,
VwExamManagerResults.Cycle AS intCycleID,
TblExamManagerTeachingProgramHeaders.intOrder AS intProgrammeOrder,
TblExamManagerTeachingProgramHeaders.txtHeaderName AS txtProgrammeName,
TblExamManagerTeachingProgram.intSubjectOrder AS intResultSubjectOrder,
ComponentsResults.ComponentCode AS txtResultComponentCode,
ComponentsResults.ComponentTitle AS txtResultComponentDescription,
ComponentsResults.Value AS txtComponentResult,
ComponentsResults.Category AS txtComponentCategory,
ComponentsResults.Status AS txtComponentStaus,
ComponentsResults.MaximumMark AS txtComponentMaxMark
FROM VwExamManagerResults
INNER JOIN VwExamManagerResultsStoreComponents AS ComponentsResults ON VwExamManagerResults.Cycle = ComponentsResults.Cycle
AND VwExamManagerResults.OptionCode = ComponentsResults.OptionCode AND VwExamManagerResults.UABCode = ComponentsResults.UABCode
AND VwExamManagerResults.SchoolId = ComponentsResults.SchoolId 
INNER JOIN TblExamManagerTeachingProgram ON VwExamManagerResults.OptionCode = TblExamManagerTeachingProgram.txtModuleCode AND VwExamManagerResults.UABCode= TblExamManagerTeachingProgram.intUABCode
INNER JOIN tblExamManagerTeachingProgramHeaders ON TblExamManagerTeachingProgram.intProgram = tblExamManagerTeachingProgramHeaders.tblExamManagerTeachingProgramHeadersID
LEFT OUTER JOIN TblExamManagerBaseDataOptions ON VwExamManagerResults.OptionCode = TblExamManagerBaseDataOptions.txtOptionEntryCode
AND VwExamManagerResults.UABCode = TblExamManagerBaseDataOptions.intUABCode AND TblExamManagerBaseDataOptions.intCycle = VwExamManagerResults.Cycle
LEFT OUTER JOIN tblTeachingManagerSubjects ON TblTeachingManagerSubjects.txtSubjectName = VwExamManagerResults.Subject

WHERE VwExamManagerResults.Cycle = @intCycle
AND (TblExamManagerTeachingProgramHeaders.TblExamManagerTeachingProgramHeadersID IN (SELECT ID FROM dbo.SplitCSV(@TblExamManagerTeachingProgramHeadersID ) AS SplitCSV_1) OR @TblExamManagerTeachingProgramHeadersID  IS NULL)
AND (tblTeachingManagerSubjects.tblTeachingManagerSubjectsID IN (SELECT ID FROM dbo.SplitCSV(TblTeachingManagerSubjectsID ) AS SplitCSV_1) OR TblTeachingManagerSubjectsID  IS NULL)
AND (VwExamManagerResults.OptionCode IN (SELECT ID FROM dbo.SplitCSV(@txtModuleCode) AS SplitCSV_1) OR @txtModuleCode IS NULL)
AND (ComponentsResults.ComponentCode IN (SELECT ID FROM dbo.SplitCSV(@txtComponentCode) AS SplitCSV_1) OR @txtComponentCode IS NULL)
)

SELECT
@txtSQA AS txtSQAOn,
ExamResults.intResultID,
ExamResults.txtSchoolID,
ExamResults.txtResultType,
ExamResults.txtResultSubject,
ExamResults.txtResultExamBoard,
ExamResults.txtResultOptionCode,
ExamResults.txtResultOptionDescription,
ExamResults.txtResultLevel,
ExamResults.txtResultQualification,
ExamResults.intProgrammeOrder,
ExamResults.txtProgrammeName,
ExamResults.intResultSubjectOrder,
ExamResults.txtResultComponentCode,
ExamResults.txtResultComponentDescription,
ExamResults.txtComponentResult,
ExamResults.txtComponentStaus,
ExamResults.txtComponentCategory,
ExamResults.txtComponentMaxMark,

TblExamManagerCycles.intFormatMonth AS intCycleMonth,
TblExamManagerCycles.intFormatYear AS intCycleYear,
TblExamManagerCentre.txtCentreId,
TblExamManagerCentre.txtSQACentreId,

TblPupilmanagementPupils.txtForename,
TblPupilmanagementPupils.txtMiddleNames,
TblPupilmanagementPupils.txtSurname,
TblPupilmanagementPupils.txtCandidateNumber,
TblPupilmanagementPupils.txtCandidateCode,
TblPupilmanagementPupils.txtCandidateForenames,
TblPupilmanagementPupils.txtCandidateSurname,
TblPupilmanagementPupils.txtGender,
TblPupilmanagementPupils.intNCYear,
TblPupilmanagementPupils.txtForm,
TblPupilmanagementPupils.txtDOB AS dtDOB,
TblPupilmanagementPupils.txtBoardingHouse,
TblPupilmanagementPupils.txtAcademicHouse,
TblPupilmanagementPupils.txtTutor,
SQARegistrations.SCN AS txtSCN
FROM ExamResults
INNER JOIN TblExamManagerCycles ON ExamResults.intCycleID = TblExamManagerCycles.TblExamManagerCyclesID
INNER JOIN TblExamManagerCentre ON TblExamManagerCycles.txtCentreId = TblExamManagerCentre.txtCentreId
INNER JOIN TblPupilmanagementPupils ON ExamResults.txtSchoolID = TblPupilmanagementPupils.txtSchoolID
LEFT OUTER JOIN VwExamManagerSQACandidateRegistrations AS SQARegistrations ON TblPupilmanagementPupils.txtSchoolID = SQARegistrations.SchoolID


WHERE TblPupilmanagementPupils.intIsCandidate = 1</CommandText></Query><Fields><Field Name="txtSQAOn"><DataField>txtSQAOn</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intResultID"><DataField>intResultID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSchoolID"><DataField>txtSchoolID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultType"><DataField>txtResultType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultSubject"><DataField>txtResultSubject</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultExamBoard"><DataField>txtResultExamBoard</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtCandidateSurname"><DataField>txtCandidateSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultOptionCode"><DataField>txtResultOptionCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultQualification"><DataField>txtResultQualification</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intProgrammeOrder"><DataField>intProgrammeOrder</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtResultOptionDescription"><DataField>txtResultOptionDescription</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtProgrammeName"><DataField>txtProgrammeName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultComponentCode"><DataField>txtResultComponentCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intResultSubjectOrder"><DataField>intResultSubjectOrder</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtResultCompon