<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>RedStaging</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>9982f9d1-3824-4a19-a13d-79631e54464f</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Results"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intCycle"><Value>=Parameters!intCycle.Value</Value></QueryParameter><QueryParameter Name="@txtSchoolID"><Value>=Parameters!txtSchoolID.Value</Value></QueryParameter><QueryParameter Name="@intNCYear"><Value>=Parameters!intNCYear.Value</Value></QueryParameter><QueryParameter Name="@txtForm"><Value>=Parameters!txtForm.Value</Value></QueryParameter><QueryParameter Name="@txtBoardingHouse"><Value>=Parameters!txtBoardingHouse.Value</Value></QueryParameter><QueryParameter Name="@txtAcademicHouse"><Value>=Parameters!txtAcademicHouse.Value</Value></QueryParameter><QueryParameter Name="@intSystemStatus"><Value>=Parameters!intSystemStatus.Value</Value></QueryParameter></QueryParameters><CommandText>DECLARE @txtSQA AS nvarchar(3);
SET @txtSQA = (SELECT TOP(1)  CASE WHEN blnSQAEnabled = 1 THEN 'Yes' ELSE 'No' END AS txtSQAOn FROM TblExamManagerConfiguration);



WITH ExamResults AS
(
-- standard results
SELECT
'JCQ' AS txtResultType,
VwExamManagerResults.SchoolId AS txtSchoolID,
VwExamManagerResults.ResultID AS intResultID,
VwExamManagerResults.UABName AS txtResultExamBoard,
VwExamManagerResults.OptionCode AS txtResultOptionCode,
VwExamManagerResults.OptionTitle  AS txtResultOptionDescription,
TblExamManagerBaseDataOptions.txtLevel AS txtResultLevel,
VwExamManagerResults.Subject AS txtResultSubject,
VwExamManagerResults.Cycle AS intEntryCycleID,
NULL AS dCompletionDate,
VwExamManagerResults.FirstGrade AS txtFirstGrade,
VwExamManagerResults.SecondGrade AS txtSecondGrade,
VwExamManagerResults.UniformGrade AS txtUniformGrade,
VwExamManagerResults.UMS AS txtUniformMark,
VwExamManagerResults.MaxMark AS txtMaxMark,
ComponentsResults.ComponentCode AS txtResultComponentCode,
ComponentsResults.ComponentTitle AS txtResultComponentDescription,
ComponentsResults.Value AS txtComponentResult,
ComponentsResults.Category AS txtComponentCategory,
ComponentsResults.Status AS txtComponentStaus,
ComponentsResults.MaximumMark AS txtComponentMaxMark
FROM VwExamManagerResults

LEFT OUTER JOIN TblExamManagerBaseDataOptions ON VwExamManagerResults.OptionCode = TblExamManagerBaseDataOptions.txtOptionEntryCode
AND VwExamManagerResults.UABCode = TblExamManagerBaseDataOptions.intUABCode AND TblExamManagerBaseDataOptions.intCycle = VwExamManagerResults.Cycle
LEFT OUTER JOIN VwExamManagerResultsStoreComponents AS ComponentsResults ON VwExamManagerResults.Cycle = ComponentsResults.Cycle
AND VwExamManagerResults.OptionCode = ComponentsResults.OptionCode AND VwExamManagerResults.UABCode = ComponentsResults.UABCode
AND VwExamManagerResults.SchoolId = ComponentsResults.SchoolId 

WHERE VwExamManagerResults.Cycle = @intCycle


UNION ALL

-- SQA results
SELECT
'SQA' AS txtResultType,
SQAResults.SchoolId AS txtSchoolID,
SQAResults.ID AS intResultID,
'SQA' AS txtResultExamBoard,
SQAResults.Code AS txtResultOptionCode,
SQAResults.CourseTitle AS txtResultOptionDescription,
SQAQualificationLevels.Name AS txtResultLevel,
SQAResults.CourseSubject AS txtResultSubject,
SQAResults.CycleId AS intResultCycleID,
SQAResults.CompletionDate AS dCompletionDate,
SQAResults.ResultValue AS txtFirstGrade,
NULL AS txtSecondGrade,
NULL AS txtUniformGrade,
NULL AS txtUniformMark,
NULL AS txtMaxMark,
NULL AS txtResultComponentCode,
NULL AS txtResultComponentDescription,
NULL AS txtComponentResult,
NULL AS txtComponentCategory,
NULL AS txtComponentStaus,
NULL AS txtComponentMaxMark
FROM VwExamManagerSQAResults AS SQAResults
LEFT OUTER JOIN VwExamManagerSQAQualificationLevels AS SQAQualificationLevels ON SQAResults.Level = SQAQualificationLevels.Level

WHERE SQAResults.CycleId = @intCycle
)

SELECT
@txtSQA AS txtSQAOn,
ExamResults.intResultID,
ExamResults.txtSchoolID,
ExamResults.txtResultType,
ExamResults.txtResultSubject,
ExamResults.txtResultExamBoard,
ExamResults.txtResultOptionCode,
ExamResults.txtResultOptionDescription,
ExamResults.txtResultLevel,
ExamResults.dCompletionDate,
ExamResults.txtFirstGrade,
ExamResults.txtSecondGrade,
ExamResults.txtUniformGrade,
ExamResults.txtUniformMark,
ExamResults.txtMaxMark,

ExamResults.txtResultComponentCode,
ExamResults.txtResultComponentDescription,
ExamResults.txtComponentResult,
ExamResults.txtComponentCategory,
ExamResults.txtComponentStaus,
ExamResults.txtComponentMaxMark,


TblExamManagerCycles.intFormatMonth AS intCycleMonth,
TblExamManagerCycles.intFormatYear AS intCycleYear,
TblExamManagerCentre.txtCentreId,
TblExamManagerCentre.txtSQACentreId,

TblPupilmanagementPupils.txtForename,
TblPupilmanagementPupils.txtMiddleNames,
TblPupilmanagementPupils.txtSurname,
TblPupilmanagementPupils.txtCandidateNumber,
TblPupilmanagementPupils.txtCandidateCode,
TblPupilmanagementPupils.txtCandidateForenames,
TblPupilmanagementPupils.txtCandidateSurname,
TblPupilmanagementPupils.txtGender,
TblPupilmanagementPupils.intNCYear,
TblPupilmanagementPupils.txtForm,
TblPupilmanagementPupils.txtDOB AS dtDOB,
TblPupilmanagementPupils.txtBoardingHouse,
TblPupilmanagementPupils.txtAcademicHouse,
TblPupilmanagementPupils.txtTutor,
SQARegistrations.SCN AS txtSCN
FROM ExamResults
INNER JOIN TblExamManagerCycles ON ExamResults.intEntryCycleID = TblExamManagerCycles.TblExamManagerCyclesID
INNER JOIN TblExamManagerCentre ON TblExamManagerCycles.txtCentreId = TblExamManagerCentre.txtCentreId
INNER JOIN TblPupilmanagementPupils ON ExamResults.txtSchoolID = TblPupilmanagementPupils.txtSchoolID
LEFT OUTER JOIN VwExamManagerSQACandidateRegistrations AS SQARegistrations ON TblPupilmanagementPupils.txtSchoolID = SQARegistrations.SchoolID


WHERE TblPupilmanagementPupils.intIsCandidate = 1
AND (TblPupilmanagementPupils.txtSchoolID IN (SELECT ID FROM dbo.SplitCSV(@txtSchoolID) AS SplitCSV_1) OR @txtSchoolID IS NULL)
AND (TblPupilmanagementPupils.intNCYear IN (SELECT ID FROM dbo.SplitCSV(@intNCYear) AS SplitCSV_1) OR @intNCYear IS NULL) 
AND (TblPupilmanagementPupils.txtForm IN (SELECT ID FROM dbo.SplitCSV(@txtForm) AS SplitCSV_1) OR @txtForm IS NULL) 
AND (TblPupilmanagementPupils.txtBoardingHouse IN(SELECT ID	FROM dbo.SplitCSV(@txtBoardingHouse) AS SplitCSV_1) OR @txtBoardingHouse IS NULL)
AND  (TblPupilmanagementPupils.txtAcademicHouse IN(SELECT ID FROM dbo.SplitCSV(@txtAcademicHouse) AS SplitCSV_1) OR @txtAcademicHouse IS NULL)
AND (
(CASE WHEN ((TblPupilmanagementPupils.intSystemStatus = 10 AND TblPupilmanagementPupils.intIsCandidate = 1) 
OR (TblPupilmanagementPupils.txtSchoolID IN
	(SELECT txtSchoolID FROM TblExamManagerReturningCandidates))) THEN 10 
	
	WHEN TblPupilmanagementPupils.intSystemStatus = 1 OR TblPupilmanagementPupils.intSystemStatus = -1 THEN 1 ELSE NULL END) 
	
	IN (SELECT ID FROM dbo.SplitCSV(@intSystemStatus) AS SplitCSV_1) OR @intSystemStatus IS NULL)</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="txtSQAOn"><DataField>txtSQAOn</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intResultID"><DataField>intResultID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSchoolID"><DataField>txtSchoolID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultType"><DataField>txtResultType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultSubject"><DataField>txtResultSubject</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultExamBoard"><DataField>txtResultExamBoard</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultOptionCode"><DataField>txtResultOptionCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultOptionDescription"><DataField>txtResultOptionDescription</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultLevel"><DataField>txtResultLevel</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dCompletionDate"><DataField>dCompletionDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtFirstGrade"><DataField>txtFirstGrade</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSecondGrade"><DataField>txtSecondGrade</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtUniformGrade"><DataField>txtUniformGrade</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtUniformMark"><DataField>txtUniformMark</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtMaxMark"><DataField>txtMaxMark</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultComponentCode"><DataField>txtResultComponentCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intCycleMonth"><DataField>intCycleMonth</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtResultComponentDescription"><DataField>txtResultComponentDescription</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intCycleYear"><DataField>intCycleYear</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtComponentCategory"><DataField>txtComponentCategory</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtComponentStaus"><DataField>txtComponentStaus</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtComponentResult"><DataField>txtComponentResult</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtCentreId"><DataField>txtCentreId</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSQACentreId"><DataField>txtSQACentreId</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtComponentMaxMark"><DataField>txtComponentMaxMark</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtMiddleNames"><DataField>txtMiddleNames</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtCandidateNumber"><DataField>txtCandidateNumber</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtCandidateCode"><DataField>txtCandidateCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtCandidateForenames"><DataField>txtCandidateForenames</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtCandidateSurname"><DataField>txtCandidateSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtGender"><DataField>txtGender</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intNCYear"><DataField>intNCYear</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtForm"><DataField>txtForm</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dtDOB"><DataField>dtDOB</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtBoardingHouse"><DataField>txtBoardingHouse</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAcademicHouse"><DataField>txtAcademicHouse</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtTutor"><DataField>txtTutor</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSCN"><DataField>txtSCN</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="CustomText"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT        txtCustomText, txtReportName
