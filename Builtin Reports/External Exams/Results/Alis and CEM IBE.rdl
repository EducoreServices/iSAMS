<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>9982f9d1-3824-4a19-a13d-79631e54464f</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="ExamResults"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intCycle"><Value>=Parameters!intCycle.Value</Value></QueryParameter><QueryParameter Name="@intNCYear"><Value>=Parameters!intNCYear.Value</Value></QueryParameter><QueryParameter Name="@txtForm"><Value>=Parameters!txtForm.Value</Value></QueryParameter><QueryParameter Name="@txtAcademicHouse"><Value>=Parameters!txtAcademicHouse.Value</Value></QueryParameter><QueryParameter Name="@txtBoardingHouse"><Value>=Parameters!txtBoardingHouse.Value</Value></QueryParameter><QueryParameter Name="@txtSchoolID"><Value>=Parameters!txtSchoolID.Value</Value></QueryParameter><QueryParameter Name="@intSystemStatus"><Value>=Parameters!intSystemStatus.Value</Value></QueryParameter></QueryParameters><CommandText>WITH ResultsCycle AS
(
SELECT intFormatMonth AS intCycleMonth, intFormatYear AS intCycleYear, txtCentreID,
CONVERT (date,'15-' + CAST(intFormatMonth AS varchar(2)) + '-' + CAST(intFormatYear AS varchar(4)),103)  AS dtCycleDate,
TblExamManagerCycles.TblExamManagerCyclesID, CustomText.txtCustomText
FROM TblExamManagerCycles
OUTER APPLY(SELECT Cast(txtCustomText AS varchar(MAX)) AS txtCustomText FROM TblExamManagerReportOptions WHERE txtReportName ='Candidate Results Report') AS CustomText
WHERE TblExamManagerCyclesID = @intCycle
)

Select 
Results.*,
Pupils.txtGender,
CONVERT(Date,Pupils.txtDOB) AS txtDOB,
Pupils.txtUPN,
PupilArchive.txtForm,
PupilArchive.txtSurname,
PupilArchive.txtTutor,
Pupils.txtCandidateNumber
FROM VwExamManagerResults AS Results


INNER JOIN 
ResultsCycle ON Results.Cycle = ResultsCycle.TblExamManagerCyclesID

OUTER APPLY ( SELECT top(1) intNCYear, txtForm, txtAcademicHouse, txtBoardingHouse, txtPastoralTutor AS txtTutor, txtSurname
FROM TblPupilManagementPupilsArchive
WHERE TblPupilManagementPupilsArchive.txtSchoolID = Results.SchoolID
AND TblPupilManagementPupilsArchive.dteSubmitDateTime &lt;= ResultsCycle.dtCycleDate
ORDER BY TblPupilManagementPupilsArchive.dteSubmitDateTime DESC ) AS PupilArchive

INNER JOIN TblPupilManagementPupils AS Pupils ON Results.SchoolId = Pupils.txtSchoolID

WHERE 
(PupilArchive.intNCYear IN (SELECT ID FROM dbo.SplitCSV(@intNCYear) AS SplitCSV_1) OR @intNCYear IS NULL) 
AND (PupilArchive.txtForm IN (SELECT ID FROM dbo.SplitCSV(@txtForm) AS SplitCSV_1) OR @txtForm IS NULL)
AND (PupilArchive.txtAcademicHouse IN (SELECT ID FROM dbo.SplitCSV(@txtAcademicHouse) AS SplitCSV_1) OR @txtAcademicHouse IS NULL)
AND (PupilArchive.txtBoardingHouse IN (SELECT ID FROM dbo.SplitCSV(@txtBoardingHouse) AS SplitCSV_1) OR @txtBoardingHouse IS NULL)
AND (Results.SchoolID IN (SELECT ID FROM dbo.SplitCSV(@txtSchoolID) AS SplitCSV_1) OR @txtSchoolID IS NULL) 
AND 
((CASE WHEN (Pupils.intSystemStatus = 10 OR Results.SchoolID IN
	(SELECT	txtSchoolID FROM TblExamManagerReturningCandidates)) THEN 10 ELSE 1 END) = @intSystemStatus)

AND
Results.Cycle = @intCycle 
and CertificationType = 'C'</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="ResultID"><DataField>ResultID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Cycle"><DataField>Cycle</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="ResultTypeId"><DataField>ResultTypeId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="ResultType"><DataField>ResultType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="PreviousResultId"><DataField>PreviousResultId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="PupilStatus"><DataField>PupilStatus</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="SchoolId"><DataField>SchoolId</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtGender"><DataField>txtGender</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Surname"><DataField>Surname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Forename"><DataField>Forename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CandidateSurname"><DataField>CandidateSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CandidateForenames"><DataField>CandidateForenames</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="UCI"><DataField>UCI</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CandidateNumber"><DataField>CandidateNumber</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TakenInNCYear"><DataField>TakenInNCYear</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="OptionCode"><DataField>OptionCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="OptionTitle"><DataField>OptionTitle</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="OptionAbbreviation"><DataField>OptionAbbreviation</DataField><r