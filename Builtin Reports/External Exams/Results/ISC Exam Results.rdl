<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>a62902c2-cc14-47aa-8564-6d745b27e3ee</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Main"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intNCYear"><Value>=Parameters!intNCYear.Value</Value></QueryParameter><QueryParameter Name="@intRolloverFlag"><Value>=Parameters!intRolloverFlag.Value</Value></QueryParameter><QueryParameter Name="@txtCentreId"><Value>=Parameters!txtCentreId.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT
	VwExamManagerResults.ResultID,
	TblPupilManagementPupils.intNCYear ,-- PupilArchive.intNCYear as ArchiveYear, 
	TblPupilManagementPupils.intLeavingNCYear,
	TblExamManagerCandidateDetail.bExcludeFromStatistics,
	TblPupilManagementPupils.txtLeavingDate, 
	VwExamManagerResults.SchoolId AS txtSchoolID, 
	TblPupilManagementPupils.txtGender, 
	VwExamManagerResults.Qualification, 
	VwExamManagerResults.QualificationLevel, 
	VwExamManagerResults.UABName, 
	VwExamManagerResults.Programme, 
	convert(varchar(100),Cast(Components.txtFormatTimetableDate AS DATETIME), 103) AS txtFormatTimetableDate,
	VwExamManagerResults.FirstGrade, 
	VwExamManagerResults.SecondGrade,
	VwExamManagerResults.CertificationType,RIGHT('0' + CONVERT(NVARCHAR(2),intFormatMonth),2) AS intFormatMonth, 
	Datename(m, CAST('2001-' + RIGHT('0' + Cycle.intFormatMonth,2) + '-01' AS DATETIME)) AS CycleName,
	Cycle.txtCentreId,
	Cycle.intFormatYear, 
	VwExamManagerResults.OptionCode, 
	VwExamManagerResults.OptionTitle

FROM VwExamManagerResults 
LEFT OUTER JOIN TblPupilManagementPupils ON VwExamManagerResults.SchoolId = TblPupilManagementPupils.txtSchoolID
LEFT OUTER JOIN TblExamManagerReturningCandidates AS ReturningCandidates ON TblPupilManagementPupils.txtSchoolID = ReturningCandidates.txtSchoolId
LEFT OUTER JOIN TblExamManagerCandidateDetail ON TblPupilManagementPupils.txtSchoolID = TblExamManagerCandidateDetail.txtSchoolId

LEFT OUTER JOIN TblExamManagerBaseDataOptions AS BaseDataOptions
	ON VwExamManagerResults.OptionCode = BaseDataOptions.txtOptionEntryCode
	AND VwExamManagerResults.Cycle = BaseDataOptions.intCycle 
	AND VwExamManagerResults.UABCode =  BaseDataOptions.intUABCode

LEFT OUTER JOIN TblExamManagerBaseDataLink AS BaseDataLink 
	ON BaseDataOptions.txtOptionEntryCode = BaseDataLink.txtOptionCode
	AND VwExamManagerResults.Cycle = BaseDataLink.intCycle 
	AND VwExamManagerResults.UABCode =  BaseDataLink.intUABCode

LEFT OUTER JOIN TblExamManagerBaseDataComponents AS Components 
	ON BaseDataLink.txtComponentCode = Components.txtComponentCode 
	AND VwExamManagerResults.Cycle = Components.intcycle 
	AND VwExamManagerResults.UABCode =  Components.intUABCode

LEFT OUTER JOIN TblExamManagerCycles AS Cycle 
	ON VwExamManagerResults.Cycle = Cycle.TblExamManagerCyclesid


WHERE  (
--This should get all the current pupils
(TblPupilManagementPupils.intSystemStatus = 1 and TblPupilManagementPupils.intNCYear = (@intNCYear + @intRolloverFlag))
OR
-- This is for the former pupils when the rollover has happened
(@intRolloverFlag = 1 and TblPupilManagementPupils.intSystemStatus = -1 and TblPupilManagementPupils.intLeavingNCYear = @intNCYear and TblPupilManagementPupils.intLeavingschoolyear = (year(getdate())-1))
-- (@intRolloverFlag = 1 and intSystemStatus = -1 and Pupils.intLeavingNCYear = @intNCYear and Pupils.txtLeavingDate &gt; @dtYearStartDate)

) 
AND VwExamManagerResults.CertificationType &lt;&gt; 'u'
AND Cycle.txtCentreId IN (@txtCentreId)</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="ResultID"><DataField>ResultID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="intNCYear"><DataField>intNCYear</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="intLeavingNCYear"><DataField>intLeavingNCYear</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="bExcludeFromStatistics"><DataField>bExcludeFromStatistics</DataField><rd:TypeName>System.Boolean</rd:TypeName></Field><Field Name="txtLeavingDate"><DataField>txtLeavingDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtSchoolID"><DataField>txtSchoolID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtGender"><DataField>txtGender</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Qualification"><DataField>Qualification</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="QualificationLevel"><DataField>QualificationLevel</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="UABName"><DataField>UABName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="Programme"><DataField>Programme</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtFormatTimetableDate"><DataField>txtFormatTimetableDate</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FirstGrade"><DataField>FirstGrade</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="SecondGrade"><DataField>SecondGrade</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CertificationType"><DataField>CertificationType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intFormatMonth"><DataField>intFormatMonth</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="CycleName"><DataField>CycleName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtCentreId"><DataField>txtCentreId</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intformatyear"><DataField>intFormatYear</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="OptionCode"><DataField>OptionCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="OptionTitle"><DataField>OptionTitle</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="Centre"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT DISTINCT txtCentreId
FROM TblExamManagerCycles
ORDER BY txtCentreId</CommandText></Query><Fields><Field Name="txtCentreId"><DataField>txtCentreId</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><ReportItems><Tablix Name="Tablix2"><TablixBody><TablixColumns><TablixColumn><Width>3cm</Width></TablixColumn><TablixColumn><Width>2cm</Width></TablixColumn><TablixColumn><Width>2.5cm</Width></TablixColumn><TablixColumn><Width>2.5cm</Width></TablixColumn><TablixColumn><Width>2.5cm</Width></TablixColumn><TablixColumn><Width>2cm</Width></TablixColumn><TablixColumn><Width>2.5cm</Width></TablixColumn><TablixColumn><Width>2.3cm</Width></TablixColumn><TablixColumn><Width>2.7cm</Width></TablixColumn><TablixColumn><Width>2.7cm</Width></TablixColumn><TablixColumn><Width>2.5cm</Width></TablixColumn><TablixColumn><Width>1.5cm</Width></TablixColumn></TablixColumns><TablixRows><TablixRow><Height>0.5cm</Height><TablixCells><TablixCell><CellContents><Textbox Name="Textbox5"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Adno</Value><Style><FontFamily>Arial</FontFamily><FontWeight>Bold</FontWeight></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>Textbox5</rd:DefaultName><Style><Border><Style>Solid</Style><Width>0.5pt</Width></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></CellContents></TablixCell><TablixCell><CellContents><Textbox Name="Textbox7"><CanGrow>true</CanGrow><KeepTogether>true</KeepTogether><Paragraphs><Paragraph><TextRuns><TextRun><Value>Gender</Value><Style><FontFamily>Arial</FontFamily><FontWeight>Bold</FontWeight></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName>Textbox7</rd:DefaultName><Style><Border><Style>Solid</Style><Width>0.5pt</Width></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</P