<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>9982f9d1-3824-4a19-a13d-79631e54464f</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Components"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intCycle"><Value>=Parameters!intCycle.Value</Value></QueryParameter><QueryParameter Name="@TblExamManagerTeachingProgramHeadersID"><Value>=Parameters!TblExamManagerTeachingProgramHeadersID.Value</Value></QueryParameter><QueryParameter Name="@txtSubject"><Value>=Parameters!txtSubject.Value</Value></QueryParameter><QueryParameter Name="@txtComponentCode"><Value>=Parameters!txtComponentCode.Value</Value></QueryParameter><QueryParameter Name="@intSystemStatus"><Value>=Parameters!intSystemStatus.Value</Value></QueryParameter></QueryParameters><CommandText>

WITH ExamEntries AS
(

-- standard entries
SELECT
'JCQ' AS txtEntryType,
TblExamManagerEntries.txtSchoolID,
TblExamManagerEntries.TblExamManagerEntriesID,
TblExamManagerTeachingProgramHeaders.txtHeaderName AS txtProgramme,
TblExamManagerTeachingProgramHeaders.intOrder AS intProgrammeOrder,
TblExamsManagerUABIdentifiers.txtUABName AS txtEntryExamBoard,
TblExamManagerEntries.txtModuleCode AS txtEntryOptionCode,
TblExamManagerTeachingProgram.txtName AS txtEntryDescription,
TblExamManagerBaseDataOptions.txtQualification AS txtEntryQualification,
TblExamManagerBaseDataOptions.txtLevel AS txtEntryLevel,
TblExamManagerTeachingProgram.txtSubject AS txtEntrySubject,
TblExamManagerTeachingProgram.intSubjectOrder AS intEntrySubjectOrder,
TblExamManagerEntries.intCycle AS intEntryCycleID,

TblExamManagerBaseDataComponents.txtComponentCode,
TblExamManagerBaseDataComponents.txtComponentTitle,
TblExamManagerBaseDataComponents.txtMaximumMark AS txtComponentMaximumMark,
TblExamManagerCourseworkMarks.txtStatus AS txtCourseworkStatus,
TblExamManagerCourseworkMarks.txtMark AS txtCourseworkMark
FROM TblExamManagerEntries

LEFT OUTER JOIN TblExamManagerTeachingProgram ON TblExamManagerEntries.txtModuleCode = TblExamManagerTeachingProgram.txtModuleCode AND TblExamManagerEntries.intUABCode = TblExamManagerTeachingProgram.intUABCode
LEFT OUTER JOIN TblExamsManagerUABIdentifiers ON TblExamManagerEntries.intUABCode = TblExamsManagerUABIdentifiers.intUABCode
INNER JOIN TblExamManagerBaseDataOptions ON TblExamManagerEntries.txtModuleCode = TblExamManagerBaseDataOptions.txtOptionEntryCode
AND TblExamManagerEntries.intUABCode = TblExamManagerBaseDataOptions.intUABCode AND TblExamManagerBaseDataOptions.intCycle = TblExamManagerEntries.intCycle
INNER JOIN TblExamManagerBaseData ON TblExamManagerBaseDataOptions.intBaseData = TblExamManagerBaseData.TblExamManagerBaseDataID
AND TblExamManagerBaseData.intCycle = TblExamManagerEntries.intCycle
AND TblExamManagerBaseData.intUABCode = TblExamManagerEntries.intUABCode

INNER JOIN TblExamManagerBaseDataLink ON TblExamManagerBaseDataOptions.txtOptionEntryCode = TblExamManagerBaseDataLink.txtOptionCode
INNER JOIN TblExamManagerBaseData AS BDLink ON TblExamManagerBaseDataLink.intBaseData = BDLink.TblExamManagerBaseDataID
AND BDLink.intCycle = TblExamManagerEntries.intCycle
AND BDLink.intUABCode = TblExamManagerEntries.intUABCode

INNER JOIN TblExamManagerBaseDataComponents ON TblExamManagerBaseDataLink.txtComponentCode = TblExamManagerBaseDataComponents.txtComponentCode
INNER JOIN TblExamManagerBaseData AS BDComponents ON TblExamManagerBaseDataComponents.intBaseData = BDComponents.TblExamManagerBaseDataID
AND BDComponents.intCycle = TblExamManagerEntries.intCycle
AND BDComponents.intUABCode = TblExamManagerEntries.intUABCode

LEFT OUTER JOIN TblExamManagerTeachingProgramHeaders ON TblExamManagerTeachingProgram.intProgram = TblExamManagerTeachingProgramHeaders.TblExamManagerTeachingProgramHeadersID

LEFT OUTER JOIN TblExamManagerCourseworkMarks ON TblExamManagerEntries.TblExamManagerEntriesID = TblExamManagerCourseworkMarks.intEntryID
AND TblExamManagerBaseDataComponents.txtComponentCode = TblExamManagerCourseworkMarks.txtComponentCode

WHERE tblExamManagerTeachingProgramHeaders.intType IN (1,2)
AND TblExamManagerEntries.intCycle = @intCycle
AND (TblExamManagerTeachingProgramHeaders.TblExamManagerTeachingProgramHeadersID IN (SELECT ID FROM dbo.SplitCSV(@TblExamManagerTeachingProgramHeadersID ) AS SplitCSV_1) OR @TblExamManagerTeachingProgramHeadersID  IS NULL)
AND (TblExamManagerTeachingProgram.txtSubject IN (SELECT ID FROM dbo.SplitCSV(@txtSubject ) AS SplitCSV_1) OR @txtSubject  IS NULL)
AND (TblExamManagerBaseDataComponents.txtComponentCode IN (SELECT ID FROM dbo.SplitCSV(@txtComponentCode ) AS SplitCSV_1) OR @txtComponentCode  IS NULL)

AND (TblExamManagerBaseDataComponents.txtTeacherMarks = 'G' OR TblExamManagerBaseDataComponents.txtTeacherMarks = 'Y')


)

SELECT

ExamEntries.TblExamManagerEntriesID,
ExamEntries.txtSchoolID,
ExamEntries.txtEntryType,
ExamEntries.txtEntrySubject,
ExamEntries.intEntrySubjectOrder,
ExamEntries.txtProgramme,
ExamEntries.intProgrammeOrder,
ExamEntries.txtEntryExamBoard,
ExamEntries.txtEntryOptionCode,
ExamEntries.txtEntryDescription,
ExamEntries.txtEntryLevel,
ExamEntries.txtEntryQualification,
ExamEntries.txtComponentCode,
ExamEntries.txtComponentTitle,
ExamEntries.txtComponentMaximumMark,
ExamEntries.txtCourseworkStatus,
ExamEntries.txtCourseworkMark,

TblExamManagerCycles.intFormatMonth AS intCycleMonth,
TblExamManagerCycles.intFormatYear AS intCycleYear,
TblExamManagerCentre.txtCentreId,
TblExamManagerCentre.txtSQACentreId,

TblPupilmanagementPupils.txtForename,
TblPupilmanagementPupils.txtMiddleNames,
TblPupilmanagementPupils.txtSurname,
TblPupilmanagementPupils.txtCandidateNumber,
TblPupilmanagementPupils.txtCandidateCode,
TblPupilmanagementPupils.txtCandidateForenames,
TblPupilmanagementPupils.txtCandidateSurname,
TblPupilmanagementPupils.txtGender,
TblPupilmanagementPupils.intNCYear,
TblSchoolManagementYears.txtYearCode,
TblPupilmanagementPupils.txtForm,
TblPupilmanagementPupils.txtDOB AS dtDOB,
TblPupilmanagementPupils.txtBoardingHouse,
TblPupilmanagementPupils.txtAcademicHouse,
TblPupilmanagementPupils.txtTutor
FROM ExamEntries
INNER JOIN TblExamManagerCycles ON ExamEntries.intEntryCycleID = TblExamManagerCycles.TblExamManagerCyclesID
INNER JOIN TblExamManagerCentre ON TblExamManagerCycles.txtCentreId = TblExamManagerCentre.txtCentreId
INNER JOIN TblPupilmanagementPupils ON ExamEntries.txtSchoolID = TblPupilmanagementPupils.txtSchoolID
LEFT OUTER JOIN TblSchoolManagementYears ON TblPupilmanagementPupils.intNCYear = TblSchoolManagementYears.intNCYear



WHERE TblPupilmanagementPupils.intIsCandidate = 1

AND (
(CASE WHEN ((TblPupilmanagementPupils.intSystemStatus = 10 AND TblPupilmanagementPupils.intIsCandidate = 1) 
OR (TblPupilmanagementPupils.txtSchoolID IN
	(SELECT txtSchoolID FROM TblExamManagerReturningCandidates))) THEN 10 
	
	WHEN TblPupilmanagementPupils.intSystemStatus = 1 THEN 1 ELSE NULL END) 
	
	IN (SELECT ID FROM dbo.SplitCSV(@intSystemStatus) AS SplitCSV_1) OR @intSystemStatus IS NULL)</CommandText></Query><Fields><Field Name="TblExamManagerEntriesID"><DataField>TblExamManagerEntriesID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSchoolID"><DataField>txtSchoolID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEntryType"><DataField>txtEntryType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEntrySubject"><DataField>txtEntrySubject</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intEntrySubjectOrder"><DataField>intEntrySubjectOrder</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtProgramme"><DataField>txtProgramme</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEntryExamBoard"><DataField>txtEntryExamBoard</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intProgrammeOrder"><DataField>intProgrammeOrder</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtEntryOptionCode"><DataField>txtEntryOptionCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEntryDescription"><DataField>txtEntryDescription</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEntryLevel"><DataField>txtEntryLevel</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEntryQualification"><DataField>txtEntryQualification</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtComponentCode"><DataField>txtComponentCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtComponentTitle"><DataField>txtComponentTitle</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtComponentMaximumMark"><DataField>txtComponentMaximumMark</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtCourseworkStatus"><DataField>txtCourseworkStatus</DataField><rd:T