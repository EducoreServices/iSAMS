  <DataSets>
    <DataSet Name="iSAMS">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select distinct Candidates.txtCandidateForenames, Candidates.txtCandidateSurname,Candidates.txtForm, Candidates.txtCandidateNumber, Exams.txtformattimetabledate,(case when Exams.txttimetablesession='A' then 'Morning' else 'Afternoon' end) as Session from 

(SELECT DISTINCT tblExamManagerTeachingProgram.txtModuleCode, Table1.intUABCode, CONVERT(datetime,txtformattimetabledate,103) AS txtformattimetabledate, txtTimetableSession,tblExamManagerEntries.intCycle
                                                                                                      
 FROM TblExamManagerBaseDataComponents
 INNER JOIN TblExamManagerBaseDataLink ON TblExamManagerBaseDataLink.txtComponentCode = TblExamManagerBaseDataComponents.txtComponentCode
 INNER JOIN TblExamManagerBaseDataOptions ON TblExamManagerBaseDataOptions.txtOptionEntryCode = TblExamManagerBaseDataLink.txtOptionCode
 INNER JOIN TblExamManagerBaseData AS Table1 ON Table1.TblExamManagerBaseDataID = TblExamManagerBaseDataComponents.intBaseData AND Table1.intCycle = @intCycle
 INNER JOIN TblExamManagerBaseData AS Table2 ON Table2.TblExamManagerBaseDataID = TblExamManagerBaseDataLink.intBaseData AND Table2.intCycle = @intCycle
 INNER JOIN TblExamsManagerUABIdentifiers ON TblExamsManagerUABIdentifiers.intUABCode = Table1.intUABCode                                                             
 INNER JOIN tblExamManagerTeachingProgram ON tblExamManagerBaseDataOptions.txtOptionEntryCode = tblExamManagerTeachingProgram.txtModuleCode
 INNER JOIN tblExamManagerTeachingProgramHeaders ON tblExamManagerTeachingProgram.intProgram = tblExamManagerTeachingProgramHeaders.tblExamManagerTeachingProgramHeadersID AND tblExamManagerTeachingProgramHeaders.intUABCode = Table1.intUABCode                                   
 INNER JOIN tblExamManagerEntries ON tblExamManagerEntries.txtModuleCode = tblExamManagerTeachingProgram.txtModuleCode AND tblExamManagerEntries.intCycle = @intCycle                                                                              
 WHERE TblExamManagerBaseDataComponents.txtTimetabled = 'T' AND (tblExamManagerTeachingProgramHeaders.intType = 1 OR tblExamManagerTeachingProgramHeaders.intType = 2)
and txtformattimetabledate=convert(datetime,@sessiondate,103)
and txttimetablesession=@AP
and txtHeaderName like 'A%'
) Exams

inner join 

 (SELECT txtModuleCode, intUABCode, txtCandidateNumber, txtCandidateForenames, txtCandidateSurname,tblExamManagerEntries.intCycle,txtForm
 FROM TblExamManagerEntries
 INNER JOIN TblPupilManagementPupils ON TblExamManagerEntries.txtSchoolID = TblPupilManagementPupils.txtSchoolID
 WHERE (
                  (intSystemStatus = 1 AND intIsCandidate = 1) OR 
                  (intSystemStatus = 10 AND intIsCandidate = 1) OR 
                  (tblPupilManagementPupils.txtSchoolID IN (SELECT txtSchoolID FROM TblExamManagerReturningCandidates))
            ) 
 --AND TblExamManagerEntries.txtModuleCode = @modulecode
 --AND TblExamManagerEntries.intUABCode = @uabcode
) Candidates

on Exams.txtModuleCode=Candidates.txtModuleCode and Exams.intUABCode=Candidates.intUABCode
and Exams.intCycle =Candidates.intCycle

--ORDER BY ExamSessions.txtformattimetabledate,ExamSessions.txtTimetableSession,Exams.txtModuleCode,Exams.intUABCode,Candidates.txtCandidateSurname,Candidates.txtCandidateForenames
order by Candidates.txtform,Candidates.txtCandidateSurname,Candidates.txtCandidateForenames</CommandText>
        <QueryParameters>
          <QueryParameter Name="@intCycle">
            <Value>=Parameters!intCycle.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@sessiondate">
            <Value>=Parameters!sessiondate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AP">
            <Value>=Parameters!AP.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>iSAMS</DataSourceName>
      </Query>
      <Fields>
        <Field Name="txtCandidateForenames">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>txtCandidateForenames</DataField>
        </Field>
        <Field Name="txtCandidateSurname">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>txtCandidateSurname</DataField>
        </Field>
        <Field Name="txtForm">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>txtForm</DataField>
        </Field>
        <Field Name="txtCandidateNumber">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>txtCandidateNumber</DataField>
        </Field>
        <Field Name="txtformattimetabledate">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>txtformattimetabledate</DataField>
        </Field>
        <Field Name="Session">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Session</DataField>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SessionDates">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>select * from 
(SELECT DISTINCT CONVERT(datetime,txtformattimetabledate,103) AS txtformattimetabledate
 FROM TblExamManagerBaseDataComponents
 INNER JOIN TblExamManagerBaseDataLink ON TblExamManagerBaseDataLink.txtComponentCode = TblExamManagerBaseDataComponents.txtComponentCode
 INNER JOIN TblExamManagerBaseDataOptions ON TblExamManagerBaseDataOptions.txtOptionEntryCode = TblExamManagerBaseDataLink.txtOptionCode
 INNER JOIN TblExamManagerBaseData AS Table1 ON Table1.TblExamManagerBaseDataID = TblExamManagerBaseDataComponents.intBaseData AND Table1.intCycle = @intCycle
 INNER JOIN TblExamManagerBaseData AS Table2 ON Table2.TblExamManagerBaseDataID = TblExamManagerBaseDataLink.intBaseData AND Table2.intCycle = @intCycle		
 INNER JOIN TblExamsManagerUABIdentifiers ON TblExamsManagerUABIdentifiers.intUABCode = Table1.intUABCode   											
 INNER JOIN tblExamManagerTeachingProgram ON tblExamManagerBaseDataOptions.txtOptionEntryCode = tblExamManagerTeachingProgram.txtModuleCode AND tblExamManagerTeachingProgram.intUABCode = TblExamsManagerUABIdentifiers.intUABCode					
 INNER JOIN tblExamManagerTeachingProgramHeaders ON tblExamManagerTeachingProgram.intProgram = tblExamManagerTeachingProgramHeaders.tblExamManagerTeachingProgramHeadersID
 --this will ignore sessions with no entries (i think)
 INNER JOIN tblExamManagerEntries ON tblExamManagerEntries.txtModuleCode = tblExamManagerTeachingProgram.txtModuleCode AND tblExamManagerEntries.intCycle = @intCycle
WHERE TblExamManagerBaseDataComponents.txtTimetabled = 'T' AND (tblExamManagerTeachingProgramHeaders.intType = 1 OR tblExamManagerTeachingProgramHeaders.intType = 2)--1: current, 2: private, 3: archived (i think)
GROUP BY txtformattimetabledate,txtTimetableSession) ExamSessions

ORDER BY ExamSessions.txtformattimetabledate</CommandText>
        <QueryParameters>
          <QueryParameter Name="@intCycle">
            <Value>=Parameters!intCycle.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>iSAMS</DataSourceName>
      </Query>
      <Fields>
        <Field Name="txtformattimetabledate">
          <rd:TypeName>System.DateTime</rd:TypeName>
          <DataField>txtformattimetabledate</DataField>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
