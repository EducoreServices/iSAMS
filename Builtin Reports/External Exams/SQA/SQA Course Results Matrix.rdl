<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>9982f9d1-3824-4a19-a13d-79631e54464f</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Results"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intCycle"><Value>=Parameters!intCycle.Value</Value></QueryParameter><QueryParameter Name="@txtResultSubject"><Value>=Parameters!txtResultSubject.Value</Value></QueryParameter><QueryParameter Name="@txtResultLevel"><Value>=Parameters!txtResultLevel.Value</Value></QueryParameter><QueryParameter Name="@txtResultOptionCode"><Value>=Parameters!txtResultOptionCode.Value</Value></QueryParameter><QueryParameter Name="@intSystemStatus"><Value>=Parameters!intSystemStatus.Value</Value></QueryParameter></QueryParameters><CommandText>
WITH ExamResults AS
(
-- SQA results
SELECT
'SQA' AS txtResultType,
SQAResults.SchoolId AS txtSchoolID,
SQAResults.ID AS intResultID,
'SQA' AS txtResultExamBoard,
SQAResults.Code AS txtResultOptionCode,
SQAResults.CourseTitle AS txtResultOptionDescription,
SQAQualificationLevels.Name AS txtResultLevel,
SQAResults.CourseSubject AS txtResultSubject,
SQAResults.CycleId AS intResultCycleID,
SQAResults.CompletionDate AS dCompletionDate,
SQAResults.ResultValue AS txtFirstGrade
FROM VwExamManagerSQAResults AS SQAResults
LEFT OUTER JOIN VwExamManagerSQAQualificationLevels AS SQAQualificationLevels ON SQAResults.Level = SQAQualificationLevels.Level
WHERE SQAResults.CycleId = @intCycle
)

SELECT
ExamResults.intResultID,
ExamResults.txtSchoolID,
ExamResults.txtResultType,
ExamResults.txtResultSubject,
ExamResults.txtResultExamBoard,
ExamResults.txtResultOptionCode,
ExamResults.txtResultOptionDescription,
ExamResults.txtResultLevel,
ExamResults.dCompletionDate,
ExamResults.txtFirstGrade,
TblExamManagerCycles.intFormatMonth AS intCycleMonth,
TblExamManagerCycles.intFormatYear AS intCycleYear,
TblExamManagerCentre.txtCentreId,
TblExamManagerCentre.txtSQACentreId,

SQARegistrations.Forename AS txtForename,
SQARegistrations.Surname AS txtSurname,
TblPupilmanagementPupils.txtCandidateNumber,
TblPupilmanagementPupils.txtCandidateCode,
TblPupilmanagementPupils.txtGender,
TblPupilmanagementPupils.intNCYear,
TblSchoolManagementYears.txtYearName,
TblPupilmanagementPupils.txtForm,
TblPupilmanagementPupils.txtDOB AS dtDOB,
TblPupilmanagementPupils.txtBoardingHouse,
TblPupilmanagementPupils.txtAcademicHouse,
TblPupilmanagementPupils.txtTutor,
SQARegistrations.SCN AS txtSCN
FROM ExamResults
INNER JOIN TblExamManagerCycles ON ExamResults.intResultCycleID = TblExamManagerCycles.TblExamManagerCyclesID
INNER JOIN TblExamManagerCentre ON TblExamManagerCycles.txtCentreId = TblExamManagerCentre.txtCentreId
INNER JOIN TblPupilmanagementPupils ON ExamResults.txtSchoolID = TblPupilmanagementPupils.txtSchoolID
LEFT OUTER JOIN TblSchoolManagementYears ON TblPupilmanagementPupils.intNCYear = TblSchoolManagementYears.intNCYear
LEFT OUTER JOIN VwExamManagerSQACandidateRegistrations AS SQARegistrations ON TblPupilmanagementPupils.txtSchoolID = SQARegistrations.SchoolID


WHERE TblPupilmanagementPupils.intIsCandidate = 1
AND CASE WHEN ExamResults.txtResultSubject IS NULL OR ExamResults.txtResultSubject='' THEN 'Unknown Subject' ELSE ExamResults.txtResultSubject END IN (@txtResultSubject)
AND ExamResults.txtResultLevel IN (@txtResultLevel)
AND ExamResults.txtResultOptionCode IN (@txtResultOptionCode)
AND (
(CASE WHEN ((TblPupilmanagementPupils.intSystemStatus = 10 AND TblPupilmanagementPupils.intIsCandidate = 1) 
OR (TblPupilmanagementPupils.txtSchoolID IN
	(SELECT txtSchoolID FROM TblExamManagerReturningCandidates))) THEN 10 
	
	WHEN TblPupilmanagementPupils.intSystemStatus = 1 THEN 1 ELSE NULL END) 
	
	IN (@intSystemStatus))</CommandText></Query><Fields><Field Name="intResultID"><DataField>intResultID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSchoolID"><DataField>txtSchoolID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultType"><DataField>txtResultType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultSubject"><DataField>txtResultSubject</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultExamBoard"><DataField>txtResultExamBoard</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultOptionCode"><DataField>txtResultOptionCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResultOptionDescription"><DataField>txtResultOptionDescription</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dCompletionDate"><DataField>dCompletionDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtResultLevel"><DataField>txtResultLevel</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtFirstGrade"><DataField>txtFirstGrade</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtCandidateCode"><DataField>txtCandidateCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtCandidateNumber"><DataField>txtCandidateNumber</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intCycleMonth"><DataField>intCycleMonth</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtGender"><DataField>txtGender</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intCycleYear"><DataField>intCycleYear</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtCentreId"><DataField>txtCentreId</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSQACentreId"><DataField>txtSQACentreId</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForm"><DataField>txtForm</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtYearName"><DataField>txtYearName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtBoardingHouse"><DataField>txtBoardingHouse</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAcademicHouse"><DataField>txtAcademicHouse</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intNCYear"><DataField>intNCYear</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="dtDOB"><DataField>dtDOB</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtTutor"><DataField>txtTutor</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSCN"><DataField>txtSCN</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="iSAMSAddress"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT	txtExternaliSAMSAddress AS 'txtiSAMSAddress'
FROM	TbliSAMSManagerSettings</CommandText></Query><Fields><Field Name="txtiSAMSAddress"><DataField>txtiSAMSAddress</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="Cycles"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT DISTINCT 
TblExamManagerCycles.TblExamManagerCyclesID,
DATENAME(month,'2000/'+ cast(TblExamManagerCycles.intFormatMonth AS nvarchar(2)) + '/01') + ' ' + CAST(TblExamManagerCycles.intFormatYear AS nvarchar(4)) AS  txtCycleName,
TblExamManagerCycles.intFormatYear, TblExamManagerCycles.intFormatMonth
FROM VwExamManagerSQAResults AS SQAResults
INNER JOIN TblExamManagerCycles ON SQAResults.CycleId = TblExamManagerCycles.TblExamManagerCyclesID
ORDER BY TblExamManagerCycles.intFormatYear DESC, TblExamManagerCycles.intFormatMonth DESC</CommandText></Query><Fields><Field Name="TblExamManagerCyclesID"><DataField>TblExamManagerCyclesID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtCycleName"><DataField>txtCycleName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intFormatYear"><DataField>intFormatYear</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="intFormatMonth"><DataField>intFormatMonth</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="Subjects"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intCycle"><Value>=Parameters!intCycle.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT
'Unknown Subject' AS txtResultSubject,
2 AS intSubjectOrder

UNION ALL
SELECT DISTINCT 
SQAResults.CourseSubject AS txtResultSubject,
1 AS intSubjectOrder
FROM VwExamManagerSQAResults AS SQAResults
WHERE SQAResults.CycleId = @intCycle
ORDER BY intSubjectOrder, txtResultSubject</CommandText></Query><Fields><Field Name="txtResultSubject"><DataField>txtResultSubject</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intSubjectOrder"><DataField>intSubjectOrder</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="Levels"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intCycle"><Value>=Parameters!intCycle.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT DISTINCT 
SQAQualificationLevels.Name AS txtResultLevel
FROM VwExamManagerSQAResults AS SQAResults
LEFT OUTER JOIN VwExamManagerSQAQualificationLevels AS SQAQualificationLevels ON SQAResults.Level = SQAQualificationLevels.Level
WHERE SQAResults.CycleId = @intCycle
ORDER BY SQAQualificationLevels.Name</CommandText></Query><Fields><Field Name="txtResultLevel"><DataField>txtResultLevel</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="CourseCode"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intCycle"><Value>=Parameters!intCycle.Value</Value></QueryParameter><QueryParameter Name="@txtResultSubject"><Value>=Parameters!txtResultSubject.Value</Value></QueryParameter><QueryParameter Name="@txtResultLevel"><Value>=Parameters!txtResultLevel.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT DISTINCT 
SQAResults.Code AS txtResultOptionCode
FROM VwExamManagerSQAResults AS SQAResults
LEFT OUTER JOIN VwExamManagerSQAQualificationLevels AS SQAQualificationLevels ON SQAResults.Level = SQAQualificationLevels.Level
WHERE SQAResults.CycleId = @intCycle
AND CASE WHEN SQAResults.CourseSubject IS NULL OR SQAResults.CourseSubject='' THEN 'Unknown Subject' ELSE SQAResults.CourseSubject END IN (@txtResultSubject)
AND SQAQualificationLevels.Name IN (@txtResultLevel)
ORDER BY SQAResults.Code</CommandText></Query><Fields><Field Name="txtResultOptionCode"><DataField>txtResultOptionCode</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="SchoolLogo"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT        txtPath + txtFilename AS txtSchoolLogoPath
FROM            TblReportsCustomImages
WHERE        (txtName LIKE '%School Logo%')
