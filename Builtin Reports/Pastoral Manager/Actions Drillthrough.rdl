<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>3b8f61bc-0106-4716-a9b8-2bce1ba9da40</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Actions"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@userCode"><Value>=Parameters!userCode.Value</Value></QueryParameter><QueryParameter Name="@ConcernID"><Value>=Parameters!ConcernID.Value</Value></QueryParameter><QueryParameter Name="@Outstanding"><Value>=Parameters!Outstanding.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT 
--core
Pupils.txtSchoolID,
Pupils.intPersonID,
Pupils.txtSurname,
Pupils.txtForename,
Pupils.txtDoB AS dtDoB,
Pupils.txtForm,
Pupils.txtAcademicHouse,
Pupils.txtBoardingHouse,
TblSchoolManagementYears.txtYearName,

--Concern
Concern.Id AS intConcernID,
ConcernStatus.Name AS txtConcernStatus,
TblStaff.Firstname AS txtConcernSourceForename,
TblStaff.Surname AS txtConcernSourceSurname,
Concern.Location AS txtConcernLocation,
Concern.RaisedOn AS dtConcernDateTime,
ConcernType.Name AS txtConcernCategory,
--DENSE_RANK() OVER (PARTITION BY Concern.Id ORDER BY ConcernArea.Id) AS intConcernAreaRank,

--Area.Name AS txtConcernArea,
Areas.txtAreas,

--Area.Description AS txtConcernAreaDescription,
Concern.Nature AS txtConcernNature,
Concern.Details AS txtConcernDetails,

-- students invloved
InvolvedStudents.txtInvolvedStudents,

-- Concern Documents
ConcernDocument.Id AS intConcernDocumentID,
ConcernDocument.FileName AS txtConcernDocumentFileName,
ConcernDocument.FilePath AS txtConcernDocumentFilePath,

-- action
ConcernAction.ID AS intActionID,
DENSE_RANK() OVER (PARTITION BY Concern.Id ORDER BY ConcernAction.Id) AS intConcernActionRank,
ActionType.Name AS txtActionType,
ActionStaff.Firstname AS txtActionAssigneeFirstname,
ActionStaff.Surname AS txtActionAssigneeSurname,
ConcernAction.DueOn AS dActionDueDate,
ConcernAction.Description AS txtActionDescription,
ActionStatus.Name AS txtActionStatus,
ConcernActionOutcome.Outcome AS txtActionOutcome

FROM Pastoral.Concern AS Concern

LEFT OUTER JOIN TblPupilManagementPupils AS Pupils ON Pupils.txtSchoolID = Concern.SchoolID
LEFT OUTER JOIN pastoral.ConcernStatus AS ConcernStatus ON Concern.ConcernStatusId = ConcernStatus.Id
LEFT OUTER JOIN TblStaff ON Concern.SourceId = TblStaff.User_Code
LEFT OUTER JOIN pastoral.ConcernType AS ConcernType ON Concern.TypeId = ConcernType.Id

INNER JOIN pastoral.ConcernAction AS ConcernAction ON Concern.Id = ConcernAction.ConcernId
LEFT OUTER JOIN pastoral.ActionType AS ActionType ON ConcernAction.ActionTypeId = ActionType.Id
LEFT OUTER JOIN pastoral.ConcernActionAssignee AS ConcernActionAssignee ON ConcernAction.Id = ConcernActionAssignee.ConcernActionId
LEFT OUTER JOIN TblStaff AS ActionStaff ON ConcernActionAssignee.Usercode = ActionStaff.User_Code
LEFT OUTER JOIN pastoral.ActionStatus AS ActionStatus ON ConcernAction.ActionStatusId = ActionStatus.Id
LEFT OUTER JOIN pastoral.ConcernActionOutcome AS ConcernActionOutcome ON ConcernAction.Id = ConcernActionOutcome.Id
LEFT OUTER JOIN TblSchoolManagementYears ON Pupils.IntNCYear = TblSchoolManagementYears.intNCYear

LEFT OUTER JOIN Pastoral.ConcernDocument AS ConcernDocument ON Concern.Id = ConcernDocument.ConcernId

OUTER APPLY (
    SELECT STUFF ((SELECT ', ' + Area.Name 
    FROM pastoral.ConcernArea 
    LEFT OUTER JOIN pastoral.Area AS Area ON ConcernArea.AreaId = Area.Id
    WHERE Concern.Id = ConcernArea.ConcernId
    ORDER BY  Area.Name FOR XML PATH('')), 1, 2,''
    ) AS txtAreas
) AS Areas

OUTER APPLY (
    SELECT STUFF ((SELECT ', ' + TblPupilManagementPupils.txtForename + ' ' + TblPupilManagementPupils.txtSurname + ' (' + StudentRole.Name + ')'
    FROM pastoral.ConcernStudent
    LEFT OUTER JOIN TblPupilManagementPupils ON ConcernStudent.SchoolId = TblPupilManagementPupils.txtSchoolID
    LEFT OUTER JOIN pastoral.ConcernRole AS StudentRole ON ConcernStudent.RoleId = StudentRole.Id
    WHERE Concern.Id = ConcernStudent.ConcernId
    ORDER BY   ConcernStudent.Id FOR XML PATH('')), 1, 2,''
    ) AS txtInvolvedStudents
) AS InvolvedStudents

WHERE
Pastoral.UserHasConcernAccess(Concern.Id , @userCode) = 1 AND
Concern.Id = (@ConcernID) AND ( @Outstanding = ActionStatus.IsOpen  OR @Outstanding IS NULL)</CommandText><rd:UseGenericDesigner>true</rd:UseGenericDesigner></Query><Fields><Field Name="txtSchoolID"><DataField>txtSchoolID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dtDoB"><DataField>dtDoB</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtForm"><DataField>txtForm</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAcademicHouse"><DataField>txtAcademicHouse</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtBoardingHouse"><DataField>txtBoardingHouse</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtYearName"><DataField>txtYearName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intConcernID"><DataField>intConcernID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtConcernStatus"><DataField>txtConcernStatus</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtConcernSourceForename"><DataField>txtConcernSourceForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtConcernSourceSurname"><DataField>txtConcernSourceSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtConcernLocation"><DataField>txtConcernLocation</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dtConcernDateTime"><DataField>dtConcernDateTime</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtConcernCategory"><DataField>txtConcernCategory</DataField><rd:TypeName>System.String</rd:TypeName></Fie