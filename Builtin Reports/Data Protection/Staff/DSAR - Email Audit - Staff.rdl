<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>09c25523-b7f0-40b2-a998-41c5672584d9</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Emails"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value>=Parameters!intPersonId.Value</Value></QueryParameter></QueryParameters><CommandText>WITH Emails AS (
SELECT 

--core
TblStaff.intPersonID,
TblStaff.Surname AS txtSurname,
TblStaff.Firstname AS txtForename,

-- Email Audit
TblAuditEmailMessages.TblAuditEmailMessagesID,
TblAuditEmailMessages.txtModuleID,
TblAuditEmailMessages.txtTo AS txtEmailRecipient,
TblAuditEmailMessages.txtFromAddress AS txtEmailFrom,
TblAuditEmailMessages.txtSubject AS txtEmailSubject,
CONVERT(nvarchar(max),TblAuditEmailMessages.txtBody) AS txtEmailBody,
TbliSAMSManagerUsers.txtFirstname AS txtEmailSentByFirstname,
TbliSAMSManagerUsers.txtSurname AS txtEmailSentBySurname,
TblAuditEmailMessages.txtSentDateTime AS dtEmailSentDateTime,
TblAuditEmailMessagesAttachedFile.txtFileName AS txtEmailAttachedFileName,
DENSE_RANK() OVER(PARTITION BY TblStaff.intPersonID,TblAuditEmailMessages.TblAuditEmailMessagesID ORDER BY TblAuditEmailMessagesAttachedFile.TblAuditEmailMessagesAttachedFileID) AS intEmailAttachedFile

FROM TblStaff
LEFT OUTER JOIN TblAuditEmailMessages ON TblStaff.SchoolEmailAddress = TblAuditEmailMessages.txtTo
LEFT OUTER JOIN TbliSAMSManagerUsers ON TblAuditEmailMessages.txtSentBy = TbliSAMSManagerUsers.txtUserCode
LEFT OUTER JOIN TblAuditEmailMessagesAttachedFile ON TblAuditEmailMessages.TblAuditEmailMessagesID = TblAuditEmailMessagesAttachedFile.intMessageId
WHERE TblStaff.intPersonID = @intPersonId
AND TblStaff.SchoolEmailAddress &lt;&gt; ''
UNION 
SELECT 

--core
TblStaff.intPersonID,
TblStaff.Surname AS txtSurname,
TblStaff.Firstname AS txtForename,

--Email Audit
TblAuditEmailMessages.TblAuditEmailMessagesID,
TblAuditEmailMessages.txtModuleID,
TblAuditEmailMessages.txtTo AS txtEmailRecipient,
TblAuditEmailMessages.txtFromAddress AS txtEmailFrom,
TblAuditEmailMessages.txtSubject AS txtEmailSubject,
CONVERT(nvarchar(max),TblAuditEmailMessages.txtBody) AS txtEmailBody,
TbliSAMSManagerUsers.txtFirstname AS txtEmailSentByFirstname,
TbliSAMSManagerUsers.txtSurname AS txtEmailSentBySurname,
TblAuditEmailMessages.txtSentDateTime AS dtEmailSentDateTime,
TblAuditEmailMessagesAttachedFile.txtFileName AS txtEmailAttachedFileName,
DENSE_RANK() OVER(PARTITION BY TblStaff.intPersonID,TblAuditEmailMessages.TblAuditEmailMessagesID ORDER BY TblAuditEmailMessagesAttachedFile.TblAuditEmailMessagesAttachedFileID) AS intEmailAttachedFile

FROM TblStaff
LEFT OUTER JOIN TblAuditEmailMessages ON TblStaff.SchoolEmailAddress = TblAuditEmailMessages.txtFromAddress
LEFT OUTER JOIN TbliSAMSManagerUsers ON TblAuditEmailMessages.txtSentBy = TbliSAMSManagerUsers.txtUserCode
LEFT OUTER JOIN TblAuditEmailMessagesAttachedFile ON TblAuditEmailMessages.TblAuditEmailMessagesID = TblAuditEmailMessagesAttachedFile.intMessageId
WHERE TblStaff.intPersonID = @intPersonId
)
SELECT Emails.*,
SplitString.txtSplitString AS txtEmailBodySplit,
SplitString.intRowNumber AS intEmailGroup
FROM Emails
OUTER APPLY dbo.uf_SplitIntoFixedLengthString(CONVERT(nvarchar(max),Emails.txtEmailBody), 32767) AS SplitString</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TblAuditEmailMessagesID"><DataField>TblAuditEmailMessagesID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtModuleID"><DataField>txtModuleID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEmailRecipient"><DataField>txtEmailRecipient</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEmailFrom"><DataField>txtEmailFrom</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEmailSubject"><DataField>txtEmailSubject</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEmailBody"><DataField>txtEmailBody</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEmailSentByFirstname"><DataField>txtEmailSentByFirstname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEmailSentBySurname"><DataField>txtEmailSentBySurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dtEmailSentDateTime"><DataField>dtEmailSentDateTime</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtEmailAttachedFileName"><DataField>txtEmailAttachedFileName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intEmailAttachedFile"><DataField>intEmailAttachedFile</DataField><rd:TypeName>System.Int64</rd:TypeName></Field><Field Name="txtEmailBodySplit"><DataField>txtEmailBodySplit</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intEmailGroup"><DataField>intEmailGroup</DataField><rd:TypeName>S