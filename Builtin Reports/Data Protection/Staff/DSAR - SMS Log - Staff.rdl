--core
TblStaff.intPersonID,
TblStaff.Surname AS txtSurname,
TblStaff.Firstname AS txtForename,

--SMS Log
TblSMSManagerMessageLog.TblSMSManagerMessageLogID,
TblSMSManagerMessageLog.txtModule,
TblSMSManagerMessageLog.txtNumber AS txtSMSSentToNumber,
CONVERT(nvarchar(max),TblSMSManagerMessageLog.txtBody) AS txtSMSBody,
TbliSAMSManagerUsers.txtFirstname AS txtSMSSentByFirstname,
TbliSAMSManagerUsers.txtSurname AS txtSMSSentBySurname,
TblSMSManagerMessageLog.txtSubmitDateTime AS dtSMSSentDateTime

FROM TblStaff
LEFT OUTER JOIN TblSMSManagerMessageLog ON TblStaff.SchoolMobileNumber = TblSMSManagerMessageLog.txtNumber
LEFT OUTER JOIN TbliSAMSManagerUsers ON TblSMSManagerMessageLog.txtSubmitBy = TbliSAMSManagerUsers.txtUserCode
WHERE TblStaff.intPersonID = @intPersonId

UNION

SELECT
--core
TblStaff.intPersonID,
TblStaff.Surname AS txtSurname,
TblStaff.Firstname AS txtForename,

--SMS Log
TblSMSManagerMessageLog.TblSMSManagerMessageLogID,
TblSMSManagerMessageLog.txtModule,
TblSMSManagerMessageLog.txtNumber AS txtSMSSentToNumber,
CONVERT(nvarchar(max),TblSMSManagerMessageLog.txtBody) AS txtSMSBody,
TbliSAMSManagerUsers.txtFirstname AS txtSMSSentByFirstname,
TbliSAMSManagerUsers.txtSurname AS txtSMSSentBySurname,
TblSMSManagerMessageLog.txtSubmitDateTime AS dtSMSSentDateTime

FROM TblStaff
LEFT OUTER JOIN TblSMSManagerMessageLog ON TblStaff.user_code = TblSMSManagerMessageLog.txtSubmitBy
LEFT OUTER JOIN TbliSAMSManagerUsers ON TblSMSManagerMessageLog.txtSubmitBy = TbliSAMSManagerUsers.txtUserCode
WHERE TblStaff.intPersonID = @intPersonId</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TblSMSManagerMessageLogID"><DataField>TblSMSManagerMessageLogID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtModule"><DataField>txtModule</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSMSSentToNumber"><DataField>txtSMSSentToNumber</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSMSBody"><DataField>txtSMSBody</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSMSSentByFirstname"><DataField>txtSMSSentByFirstname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSMSSentBySurname"><DataField>txtSMSSentBySurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dtSMSSentDateTime"><DataField>dtSMSSentDateTime</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="intPersonId"><DataType>String</DataType><DefaultValue><Values><Value>16289</Value></Values></DefaultValue><Prompt>Staff:</Prompt></ReportParameter></ReportParameters><ConsumeContainerWhitespace>true</ConsumeContainerWhitespace><rd:ReportUnitType>Cm</rd:ReportUnitType><rd:ReportID>c59d5b76-daca-48bd-a551-042d4b03db8c</rd:ReportID></Report>