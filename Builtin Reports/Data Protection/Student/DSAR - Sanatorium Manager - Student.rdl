--core
TblPupilManagementPupils.intPersonID,
TblPupilManagementPupils.txtSurname,
TblPupilManagementPupils.txtForename,

--Doctor
TblSanatoriumManagerSurgeriesDoctors.TblSanatoriumManagerSurgeriesDoctorsID,
TblSanatoriumManagerDoctors.txtForename AS txtDoctorForename,
TblSanatoriumManagerDoctors.txtSurname AS txtDoctorSurname,

TblSanatoriumManagerSurgeries.txtName AS txtSurgeryName,
TblSanatoriumManagerSurgeries.txtAddress1 AS txtSurgeryAddress1,
TblSanatoriumManagerSurgeries.txtAddress2 AS txtSurgeryAddress2,
TblSanatoriumManagerSurgeries.txtAddress3 AS txtSurgeryAddress3,
TblSanatoriumManagerSurgeries.txtTown AS txtSurgeryTown,
TblSanatoriumManagerSurgeries.txtCounty AS txtSurgeryCounty,
TblSanatoriumManagerSurgeries.txtCountry AS txtSurgeryCountry,
TblSanatoriumManagerSurgeries.txtPostCode AS txtSurgeryPostcode,
TblSanatoriumManagerSurgeries.txtTelephone AS txtSurgeryTelephone,
TblSanatoriumManagerSurgeries.txtFaxNumber AS txtSurgeryFaxNumber,
TblSanatoriumManagerSurgeries.txtEmail AS txtSurgeryEmail,
TblSanatoriumManagerSurgeries.txtWebsite AS txtSurgeryWebsite

FROM TblPupilManagementPupils
LEFT OUTER JOIN TblPupilManagementHealthDoctors ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementHealthDoctors.txtSchoolID
LEFT OUTER JOIN TblSanatoriumManagerSurgeriesDoctors ON TblPupilManagementHealthDoctors.intSurgeriesDoctor = TblSanatoriumManagerSurgeriesDoctors.TblSanatoriumManagerSurgeriesDoctorsID
LEFT OUTER JOIN TblSanatoriumManagerDoctors ON TblSanatoriumManagerSurgeriesDoctors.intDoctor = TblSanatoriumManagerDoctors.TblSanatoriumManagerDoctorsID
LEFT OUTER JOIN TblSanatoriumManagerSurgeries ON TblSanatoriumManagerSurgeriesDoctors.intSurgery = TblSanatoriumManagerSurgeries.TblSanatoriumManagerSurgeriesID
WHERE TblPupilManagementPupils.intPersonID = @intPersonId
