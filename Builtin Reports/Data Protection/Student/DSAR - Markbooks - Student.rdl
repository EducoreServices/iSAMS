CT 

--core
TblPupilManagementPupils.intPersonID,
TblPupilManagementPupils.txtSurname,
TblPupilManagementPupils.txtForename

FROM TblPupilManagementPupils
WHERE TblPupilManagementPupils.intPersonID = @intPersonId

</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="iSAMSAddress"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT txtInternaliSAMSAddress
FROM TbliSAMSManagerSettings</CommandText></Query><Fields><Field Name="txtInternaliSAMSAddress"><DataField>txtInternaliSAMSAddress</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="Marks"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value>=Parameters!intPersonId.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT 

--core
TblPupilManagementPupils.intPersonID,
TblPupilManagementPupils.txtSurname,
TblPupilManagementPupils.txtForename,

--markbook marks
TblMarkBooksMarkBook.txtName AS txtMarkBookName,
TblMarkBooksTest.txtName AS txtMarkBookTestName,
TblTeachingManagerSubjects.txtSubjectName,
TblMarkBooksMark.txtMark AS txtMarkBookMark


FROM TblPupilManagementPupils

INNER JOIN TblMarkBooksEntry ON TblPupilManagementPupils.txtSchoolID = TblMarkBooksEntry.txtSchoolID
INNER JOIN TblMarkBooksMark ON TblMarkBooksEntry.TblMarkBooksEntryID = TblMarkBooksMark.intEntry
INNER JOIN TblMarkBooksTest ON TblMarkBooksEntry.intTest = TblMarkBooksTest.TblMarkBooksTestID 
INNER JOIN TblMarkBooksMarkBook ON TblMarkBooksTest.intMarkBook = TblMarkBooksMarkBook.TblMarkBooksMarkBookID 
INNER JOIN TblMarkBooksLink ON TblMarkBooksLink.intMarkBook = TblMarkBooksMarkBook.TblMarkBooksMarkBookID
INNER JOIN TblTeachingManagerSubjects ON TblMarkBooksLink.intSubjectID = TblTeachingManagerSubjects.TblTeachingManagerSubjectsID
WHERE TblPupilManagementPupils.intPersonID = @intPersonId
AND TblMarkBooksMark.txtMark IS NOT NULL


</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtMarkBookName"><DataField>txtMarkBookName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtMarkBookTestName"><DataField>txtMarkBookTestName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSubjectName"><DataField>txtSubjectName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtMarkBookMark"><DataField>txtMarkBookMark</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="Audit"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value>=Parameters!intPersonId.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT 

--core
TblPupilManagementPupils.intPersonID,
TblPupilManagementPupils.txtSurname,
TblPupilManagementPupils.txtForename,

--markbook marks audit
TblMarkBooksMarkBook.txtName AS txtAuditMarkBookName,
TblMarkBooksMarkHistory.txtTestName AS txtAuditTestName,
TblMarkBooksMarkHistory.txtPreviousMark AS txtAuditPreviousMark,
TblMarkBooksMarkHistory.txtNewMark AS txtAuditNewMark,
TblMarkBooksMarkHistoryAction.TxtName AS txtAuditAction


FROM TblMarkBooksMarkHistory 
INNER JOIN TblPupilManagementPupils ON TblMarkBooksMarkHistory.txtSchoolID = TblPupilManagementPupils.txtSchoolID 
INNER JOIN TblMarkBooksMarkBook ON TblMarkBooksMarkHistory.intMarkBook = TblMarkBooksMarkBook.TblMarkBooksMarkBookID 
INNER JOIN TblMarkBooksMarkHistoryAction ON TblMarkBooksMarkHistory.intAction = TblMarkBooksMarkHistoryAction.TblMarkBooksMarkHistoryActionID
WHERE TblPupilManagementPupils.intPersonID = @intPersonId



</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAuditMarkBookName"><DataField>txtAuditMarkBookName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAuditTestName"><DataField>txtAuditTestName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAuditPreviousMark"><DataField>txtAuditPreviousMark</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAuditNewMark"><DataField>txtAuditNewMark</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAuditAction"><DataField>txtAuditAction</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="intPersonId"><DataType>String</DataType><DefaultValue><Values><Value>9</Value></Values></DefaultValue><Prompt>Student:</Prompt></ReportParameter></ReportParameters><rd:ReportUnitType>Cm</rd:ReportUnitType><rd:ReportID>ce8b9b7a-051f-4ae5-80cc-7d74a8bebc08</rd:ReportID></Report>