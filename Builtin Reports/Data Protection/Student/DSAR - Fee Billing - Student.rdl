SELECT
--core
TblPupilManagementPupils.intPersonID,
TblPupilManagementPupils.txtSurname,
TblPupilManagementPupils.txtForename,

--deposits
TblFeeBillingPupilDeposit.TblFeeBillingPupilDepositID,
TblFeeBillingPupilDepositType.txtName AS txtDepositType,
TblFeeBillingPupilDeposit.mAmount AS mDepositAmount,
TblFeeBillingPupilDeposit.dPaidDate AS dDepositPaidDate,
TblFeeBillingPupilDeposit.txtNotes AS txtDepositNotes,
TblFeeBillingPupilDepositStatusType.txtName AS txtDepositStatus,
TblFeeBillingPupilDepositReturnAction.txtName AS txtRefundedReturnAction,
TblFeeBillingPupilDepositSourceType.txtName AS txtDepositSource,
TblFeeBillingPupilDepositPaymentMethod.txtName AS txtDepositPaymentMethid
  
FROM TblPupilManagementPupils
INNER JOIN TblFeeBillingPupilDeposit ON TblPupilManagementPupils.txtSchoolID = TblFeeBillingPupilDeposit.txtSchoolID
LEFT OUTER JOIN TblFeeBillingPupilDepositType TblFeeBillingPupilDepositType ON TblFeeBillingPupilDeposit.intDepositTypeID = TblFeeBillingPupilDepositType.TblFeeBillingPupilDepositTypeID 
LEFT OUTER JOIN TblFeeBillingPupilDepositStatusType ON TblFeeBillingPupilDeposit.intStatusTypeID = TblFeeBillingPupilDepositStatusType.TblFeeBillingPupilDepositStatusTypeID
LEFT OUTER JOIN TblFeeBillingPupilDepositReturnAction ON TblFeeBillingPupilDeposit.intReturnActionID = TblFeeBillingPupilDepositReturnAction.TblFeeBillingPupilDepositReturnActionID
LEFT OUTER JOIN TblFeeBillingPupilDepositSourceType ON TblFeeBillingPupilDeposit.intSourceTypeID = TblFeeBillingPupilDepositSourceType.TblFeeBillingPupilDepositSourceTypeID
LEFT OUTER JOIN TblFeeBillingPupilDepositPaymentMethod ON TblFeeBillingPupilDeposit.intPaymentMethodTypeID = TblFeeBillingPupilDepositPaymentMethodID
WHERE TblPupilManagementPupils.intPersonID = @intPersonId

UNION ALL


SELECT
--core
TblPupilManagementPupils.intPersonID,
TblPupilManagementPupils.txtSurname,
TblPupilManagementPupils.txtForename,

--deposits
TblFeeBillingPupilDeposit.TblFeeBillingPupilDepositID,
TblFeeBillingPupilDepositType.txtName AS txtDepositType,
- TblFeeBillingPupilDepositRefundLog.mAmountRefunded AS mDepositAmount,
TblFeeBillingPupilDepositRefundLog.dDateTimeRefunded AS dDepositPaidDate,
'Refund' AS txtDepositNotes,
TblFeeBillingPupilDepositStatusType.txtName AS txtDepositStatus,
TblFeeBillingPupilDepositReturnAction.txtName AS txtRefundedReturnAction,
TblFeeBillingPupilDepositSourceType.txtName AS txtDepositSource,
TblFeeBillingPupilDepositPaymentMethod.txtName AS txtDepositPaymentMethid
  
FROM TblPupilManagementPupils
INNER JOIN TblFeeBillingPupilDeposit ON TblPupilManagementPupils.txtSchoolID = TblFeeBillingPupilDeposit.txtSchoolID
INNER JOIN TblFeeBillingPupilDepositRefundLog ON TblFeeBillingPupilDeposit.TblFeeBillingPupilDepositID = TblFeeBillingPupilDepositRefundLog.intDepositID
LEFT OUTER JOIN TblFeeBillingPupilDepositType TblFeeBillingPupilDepositType ON TblFeeBillingPupilDeposit.intDepositTypeID = TblFeeBillingPupilDepositType.TblFeeBillingPupilDepositTypeID 
LEFT OUTER JOIN TblFeeBillingPupilDepositStatusType ON TblFeeBillingPupilDeposit.intStatusTypeID = TblFeeBillingPupilDepositStatusType.TblFeeBillingPupilDepositStatusTypeID
LEFT OUTER JOIN TblFeeBillingPupilDepositReturnAction ON TblFeeBillingPupilDeposit.intReturnActionID = TblFeeBillingPupilDepositReturnAction.TblFeeBillingPupilDepositReturnActionID
LEFT OUTER JOIN TblFeeBillingPupilDepositSourceType ON TblFeeBillingPupilDeposit.intSourceTypeID = TblFeeBillingPupilDepositSourceType.TblFeeBillingPupilDepositSourceTypeID
LEFT OUTER JOIN TblFeeBillingPupilDepositPaymentMethod ON TblFeeBillingPupilDeposit.intPaymentMethodTypeID = TblFeeBillingPupilDepositPaymentMethodID
WHERE TblPupilManagementPupils.intPersonID = @intPersonId
</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TblFeeBillingPupilDepositID"><DataField>TblFeeBillingPupilDepositID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtDepositType"><DataField>txtDepositType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="mDepositAmount"><DataField>mDepositAmount</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="dDepositPaidDate"><DataField>dDepositPaidDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtDepositNotes"><DataField>txtDepositNotes</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtDepositStatus"><DataField>txtDepositStatus</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtRefundedReturnAction"><DataField>txtRefundedReturnAction</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtDepositSource"><DataField>txtDepositSource</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtDepositPaymentMethid"><DataField>txtDepositPaymentMethid</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="PreInvoiceCharges"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value>=Parameters!intPersonId.Value</Value></QueryParameter></QueryParameters><CommandText>DECLARE @MyTempTable table
(
   txtType nvarchar(20),
   intID integer,
   txtAccountCode nvarchar(50),
   txtSchoolID nvarchar(50),
   txtSageCode nvarchar(50),
   ChargeCode nvarchar(50),
   txtDescription nvarchar(200),
   mAmount decimal(18,2),
   percentageAmount decimal(18,2),
   txtChargeType nvarchar(50),
   txtNominalAccount nvarchar(50),
   bBilledInAdvance bit
);

DECLARE @MyTempTable2 table
(
   txtType nvarchar(20),
   intID integer,
   txtAccountCode nvarchar(50),
   txtSchoolID nvarchar(50),
   txtSageCode nvarchar(50),
   ChargeCode nvarchar(50),
   txtDescription nvarchar(200),
   mAmount decimal(18,2),
   percentageAmount decimal(18,2),
   txtChargeType nvarchar(50),
   txtNominalAccount nvarchar(50),
   bBilledInAdvance bit,
   intCycle Integer
);


DECLARE @param INT;

DECLARE curs CURSOR LOCAL FAST_FORWARD FOR
SELECT        TblFeeBillingCycleID
FROM            TblFeeBillingCycle
OPEN curs


FETCH NEXT FROM curs INTO @param

WHILE @@FETCH_STATUS = 0 BEGIN
INSERT INTO @MyTempTable
EXEC usp_cust_GetTransactionsByCycle @param

INSERT INTO @MyTempTable2 
SELECT TempData.* ,@param
FROM @MyTempTable AS TempData

FETCH NEXT FROM curs INTO @param
END

CLOSE curs
DEALLOCATE curs


SELECT 

--core
TblPupilManagementPupils.intPersonID,
TblPupilManagementPupils.txtSurname,
TblPupilManagementPupils.txtForename,

--charges
Charges.intCycle AS intPreInvoiceFeeBillingCycleID,
TblFeeBillingCycle.txtName AS txtPreInvoiceFeeBillingCycleName,
Charges.intID AS intPreInvoiceChargeID,
Charges.txtChargeType AS txtPreInvoiceChargeType,
Charges.ChargeCode AS txtPreInvoiceChargeCode,
Charges.txtDescription AS txtPreInvoiceChargeDescription,
Charges.mAmount AS mPreInvoiceChargeAmount,
Charges.percentageAmount AS dPreInvoiceChargePercentageAmount,
Charges.txtNominalAccount AS txtPreInvoiceNominalAccount,
CASE WHEN Charges.bBilledInAdvance =1 THEN 'Yes' ELSE 'No' END  AS txtPreInvoiceChargeBilledInAdvance

FROM @MyTempTable2 AS Charges
INNER JOIN TblPupilManagementPupils ON Charges.txtSchoolID = TblPupilManagementPupils.txtSchoolID
INNER JOIN TblFeeBillingCycle ON Charges.intCycle = TblFeeBillingCycle.TblFeeBillingCycleID
WHERE TblPupilManagementPupils.intPersonID = @intPersonId</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intPreInvoiceFeeBillingCycleID"><DataField>intPreInvoiceFeeBillingCycleID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtPreInvoiceFeeBillingCycleName"><DataField>txtPreInvoiceFeeBillingCycleName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intPreInvoiceChargeID"><DataField>intPreInvoiceChargeID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtPreInvoiceChargeType"><DataField>txtPreInvoiceChargeType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPreInvoiceChargeCode"><DataField>txtPreInvoiceChargeCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPreInvoiceChargeDescription"><DataField>txtPreInvoiceChargeDescription</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="mPreInvoiceChargeAmount"><DataField>mPreInvoiceChargeAmount</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="dPreInvoiceChargePercentageAmount"><DataField>dPreInvoiceChargePercentageAmount</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="txtPreInvoiceNominalAccount"><DataField>txtPreInvoiceNominalAccount</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPreInvoiceChargeBilledInAdvance"><DataField>txtPreInvoiceChargeBilledInAdvance</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="PostInvoiceCharges"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value>=Parameters!intPersonId.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT
--core
TblPupilManagementPupils.intPersonID,
TblPupilManagementPupils.txtSurname,
TblPupilManagementPupils.txtForename,

--charges
TblFeeBillingInvoiceAuditCycle.intOriginalCycleId AS intPostInvoiceFeeBillingCycleID,
TblFeeBillingCycle.txtName AS txtPostInvoiceFeeBillingCycleName,
TblFeeBillingInvoiceItem.TblFeeBillingInvoiceItemID AS intPostInvoiceChargeID,
TblFeeBillingChargeType.txtName AS txtPostInvoiceChargeType,
TblFeeBillingInvoiceItem.ChargeCode AS txtPostInvoiceChargeCode,
TblFeeBillingInvoiceItem.txtDescription AS txtPostInvoiceChargeDescription,
TblFeeBillingInvoiceItem.mTotalAmount AS mPostInvoiceChargeAmount,
TblFeeBillingInvoiceItem.txtNominalAccount AS txtPostInvoiceNominalAccount,
CASE WHEN TblFeeBillingInvoiceItem.bBilledInAdvance =1 THEN 'Yes' ELSE 'No' END  AS txtPostInvoiceChargeBilledInAdvance,
TblFeeBillingInvoice.txtBillingNotes AS txtPostInvoiceBillingNotes


FROM TblFeeBillingInvoiceRun 
INNER JOIN TblFeeBillingInvoice ON TblFeeBillingInvoice.intInvoiceRun = TblFeeBillingInvoiceRun.TblFeeBillingInvoiceRunId 
INNER JOIN TblFeeBillingInvoiceItem ON TblFeeBillingInvoiceItem.intInvoiceID = TblFeeBillingInvoice.TblFeeBillingInvoiceID 
INNER JOIN TblFeeBillingInvoiceAuditPupil ON TblFeeBillingInvoiceItem.intAuditPupilID = TblFeeBillingInvoiceAuditPupil.TblFeeBillingInvoiceAuditPupilId 
INNER JOIN TblPupilManagementPupils ON TblFeeBillingInvoiceAuditPupil.txtExistingSchoolID = TblPupilManagementPupils.txtSchoolID 
INNER JOIN TblFeeBillingInvoiceAuditCycle ON TblFeeBillingInvoiceRun.intAuditCycleId = TblFeeBillingInvoiceAuditCycle.TblFeeBillingInvoiceAuditCycleId 
LEFT OUTER JOIN TblFeeBillingCharge ON TblFeeBillingInvoiceItem.ChargeCode = TblFeeBillingCharge.ChargeCode 
LEFT OUTER JOIN TblFeeBillingCycle ON TblFeeBillingInvoiceAuditCycle.intOriginalCycleId = TblFeeBillingCycle.TblFeeBillingCycleID
LEFT OUTER JOIN TblFeeBillingChargeType ON TblFeeBillingCharge.intChargeType = TblFeeBillingChargeType.TblFeeBillingChargeTypeID
WHERE TblPupilManagementPupils.intPersonID = @intPersonId</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intPostInvoiceFeeBillingCycleID"><DataField>intPostInvoiceFeeBillingCycleID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtPostInvoiceFeeBillingCycleName"><DataField>txtPostInvoiceFeeBillingCycleName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intPostInvoiceChargeID"><DataField>intPostInvoiceChargeID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtPostInvoiceChargeType"><DataField>txtPostInvoiceChargeType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPostInvoiceChargeCode"><DataField>txtPostInvoiceChargeCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPostInvoiceChargeDescription"><DataField>txtPostInvoiceChargeDescription</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="mPostInvoiceChargeAmount"><DataField>mPostInvoiceChargeAmount</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="txtPostInvoiceNominalAccount"><DataField>txtPostInvoiceNominalAccount</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPostInvoiceChargeBilledInAdvance"><DataField>txtPostInvoiceChargeBilledInAdvance</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPostInvoiceBillingNotes"><DataField>txtPostInvoiceBillingNotes</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="Notes"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value>=Parameters!intPersonId.Value</