LEFT OUTER JOIN TblFeeBillingCycle ON TblFeeBillingInvoiceAuditCycle.intOriginalCycleId = TblFeeBillingCycle.TblFeeBillingCycleID
LEFT OUTER JOIN TbliSAMSManagerUsers AS PastoralTutor ON TblFeeBillingInvoiceAuditPupil.txtTutor = PastoralTutor.txtUserCode
WHERE TblPupilManagementPupils.intPersonID = @intPersonId</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TblFeeBillingInvoiceAuditPupilID"><DataField>TblFeeBillingInvoiceAuditPupilID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtInvoicePupilAuditCycleName"><DataField>txtInvoicePupilAuditCycleName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtInvoicePupilAuditFullName"><DataField>txtInvoicePupilAuditFullName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtInvoicePupilAuditForename"><DataField>txtInvoicePupilAuditForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtInvoicePupilAuditeMiddleNames"><DataField>txtInvoicePupilAuditeMiddleNames</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtInvoicePupilAuditSurname"><DataField>txtInvoicePupilAuditSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtInvoicePupilAuditGender"><DataField>txtInvoicePupilAuditGender</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dInvoicePupilAuditDOB"><DataField>dInvoicePupilAuditDOB</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtInvoicePupilAuditForm"><DataField>txtInvoicePupilAuditForm</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intInvoicePupilAuditNCYear"><DataField>intInvoicePupilAuditNCYear</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtInvoicePupilAuditAcademicHouse"><DataField>txtInvoicePupilAuditAcademicHouse</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtInvoicePupilAuditBoardingHouse"><DataField>txtInvoicePupilAuditBoardingHouse</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtInvoicePupilAuditPastoralTutorForename"><DataField>txtInvoicePupilAuditPastoralTutorForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtInvoicePupilAuditPastoralTutorSurname"><DataField>txtInvoicePupilAuditPastoralTutorSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtInvoicePupilAuditAccountCode"><DataField>txtInvoicePupilAuditAccountCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dtInvoicePupilAuditDateTime"><DataField>dtInvoicePupilAuditDateTime</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="intPersonId"><DataType>String</DataType><DefaultValue><Values><Value>10877</Value></Values></DefaultValue><Prompt>Student:</Prompt></ReportParameter></ReportParameters><rd:ReportUnitType>Cm</rd:ReportUnitType><rd:ReportID>ce8b9b7a-051f-4ae5-80cc-7d74a8bebc08</rd:ReportID></Report>