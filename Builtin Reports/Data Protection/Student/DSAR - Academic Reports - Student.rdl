AcademicReports.txtReportTermName,
AcademicReports.txtReportCycleName,
AcademicReports.dtReportCyclePrintDate,
AcademicReports.txtReportTypeName,
AcademicReports.intReportTypeID,
AcademicReports.txtSubmitByFirstname,
AcademicReports.txtSubmitBySurname,
AcademicReports.txtSubjectArchivedName,

--grades
AcademicReports.intGradeID,
AcademicReports.txtGradingName,
AcademicReports.txtGrade,

