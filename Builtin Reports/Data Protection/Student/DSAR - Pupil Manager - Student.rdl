TblPupilManagementFormerDonations.txtReference AS txtDonationReference

FROM TblPupilManagementPupils
LEFT OUTER JOIN TblPupilManagementFormerDonationInfo ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementFormerDonationInfo.txtSchoolID
LEFT OUTER JOIN TblPupilManagementFormerDonations ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementFormerDonations.txtSchoolID

WHERE TblPupilManagementPupils.intPersonID = @intPersonId

