<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>09c25523-b7f0-40b2-a998-41c5672584d9</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="General"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value>=Parameters!intPersonId.Value</Value></QueryParameter></QueryParameters><CommandText>DECLARE @intCharSplit AS DECIMAL(18,2) = 32000;
DECLARE @intLoopCheck AS INT = 1;

WITH CensusGCSEHighestPriorAttainmentYearGroup AS 
	(SELECT '1' AS txtCensusMathsGCSEHighestPriorAttainmentYearGroup, 'Grade A*-C/9-4 – Achieved by End of Year 11' AS txtDetails
		UNION
	 SELECT '2' AS txtCensusMathsGCSEHighestPriorAttainmentYearGroup, 'Grade A*-C/9-4 – Achieved since End of Year 11' AS txtDetails
		UNION 
	 SELECT '2' AS txtCensusMathsGCSEHighestPriorAttainmentYearGroup, 'Grade A*-C/9-4 – Not Achieved' AS txtDetails
	)
SELECT 

	--core
	TblPupilManagementPupils.intPersonID,
	TblPupilManagementPupils.txtSurname,
	TblPupilManagementPupils.txtForename,

	--General
	TblPupilManagementPupils.txtTitle,
	TblPupilManagementPupils.txtFormerSurname,
	TblPupilManagementPupils.txtMiddleNames,
	TblPupilManagementPupils.txtInitials,
	TblPupilManagementPupils.txtFullName,
	TblPupilManagementPupils.txtOfficialName,
	TblPupilManagementPupils.txtPreName AS txtPreferredName,
	TblPupilManagementPupils.txtLabelSalutation,
	TblPupilManagementPupils.txtLetterSalutation,
	TblPupilManagementPupils.txtGender,
	TblPupilManagementPupils.txtDOB AS dDOB,
	TblPupilManagementPupils.txtSchoolCode,
	TblPupilManagementPupils.txtUPN,
	TblPupilManagementPupils.txtFormerUPN,


	--contacts
	TblPupilManagementPupils.txtContactNotes AS txtContactNotes,
	TblPupilManagementPupils.intDuplicateReports,

	TblPupilManagementAddresses.TblPupilManagementAddressesID,
	TblPupilManagementAddresses.txtRelationType AS txtContactType,
	CASE WHEN TblPupilManagementAddresses.intJustContact =1 THEN 'Yes' 
	ELSE 'No' END AS txtContactOnly,
	CASE WHEN TblPupilManagementAddresses.intMailMergeAll =1 THEN 'Yes' 
	ELSE 'No' END AS txtAllMerges,
	CASE WHEN TblPupilManagementAddresses.intBillingMailMerge =1 THEN 'Yes' 
	ELSE 'No' END AS txtBillingContact,
	CASE WHEN TblPupilManagementAddresses.intCorrespondenceMailMerge =1 THEN 'Yes' 
	ELSE 'No' END AS txtCorrespondence,
	CASE WHEN TblPupilManagementAddresses.intReportsMailMerge =1 THEN 'Yes' 
	ELSE 'No' END AS txtReports,
	CASE WHEN TblPupilManagementAddresses.intReport =1 THEN 'Yes' 
	ELSE 'No' END AS txtAcademicReports,
	CASE WHEN TblPupilManagementAddresses.intPrivate =1 THEN 'Yes' 
	ELSE 'No' END AS txtPrivateContact,
	CASE WHEN TblPupilManagementAddresses.intCanShare =1 THEN 'Yes' 
	ELSE 'No' END AS txtContactCanBeSharedWithOtherParents,
	CASE WHEN TblPupilManagementAddresses.intStudentHome =1 THEN 'Yes' 
	ELSE 'No' END AS txtStudentHomes,
	TblPupilManagementAddresses.txtAddressType AS txtContactLocation,
	CASE WHEN TblPupilManagementAddresses.blnFutureAddress =1 THEN 'Yes' 
	ELSE 'No' END AS txtFutureAddress,
	TblPupilManagementAddresses.txtMovedInDate AS dMovedInDate,

	TblPupilManagementAddresses.txtMaritalStatus AS txtContactMaritalStatus,
	TblPupilManagementAddresses.txtContactsTitle AS txt1stPersonTitle,
	TblPupilManagementAddresses.txtContactsForename AS txt1stPersonForename,
	TblPupilManagementAddresses.txtContactsMiddleNames AS txt1stPersonMiddleNames,
	TblPupilManagementAddresses.txtContactsInitials AS txt1stPersonInitials,
	TblPupilManagementAddresses.txtContactsSurname AS txt1stPersonSurname,
	TblPupilManagementAddresses.txtEmail1 AS txt1stPersonEmail,
	TblPupilManagementAddresses.txtMobile1 AS txt1stPersonMobile,
	TblPupilManagementAddresses.txtProfession AS txt1stPersonProfession,
	CASE WHEN TblPupilManagementAddresses.intDeceased =1 THEN 'Yes' 
	ELSE 'No' END AS txt1stPersonDeceased,
	CASE WHEN TblPupilManagementAddresses.blnContactHMForces =1 THEN 'Yes' 
	ELSE 'No' END AS txt1stPersonHMForces,
	CASE WHEN TblPupilManagementAddresses.intContactPTA =1 THEN 'Yes' 
	ELSE 'No' END AS txt1stPersonPTFA,

	TblPupilManagementAddresses.txtSecondaryTitle AS txt2ndPersonTitle,
	TblPupilManagementAddresses.txtSecondaryForename AS txt2ndPersonForename,
	TblPupilManagementAddresses.txtSecondaryMiddleNames AS txt2ndPersonMiddleNames,
	TblPupilManagementAddresses.txtSecondaryInitials AS txt2ndPersonInitials,
	TblPupilManagementAddresses.txtSecondarySurname AS txt2ndPersonSurname,
	TblPupilManagementAddresses.txtEmail2 AS txt2ndPersonEmail,
	TblPupilManagementAddresses.txtMobile2 AS txt2ndPersonMobile,
	TblPupilManagementAddresses.txtSecondaryProfession AS txt2ndPersonProfession,
	CASE WHEN TblPupilManagementAddresses.intSecondaryDeceased =1 THEN 'Yes' 
	ELSE 'No' END AS txt2ndPersonDeceased,
	CASE WHEN TblPupilManagementAddresses.blnSecondaryHMForces =1 THEN 'Yes' 
	ELSE 'No' END AS txt2ndPersonHMForces,
	CASE WHEN TblPupilManagementAddresses.intSecondaryPTA =1 THEN 'Yes' 
	ELSE 'No' END AS txt2ndPersonPTFA,

	TblPupilManagementAddresses.txtDayTelephone AS txtTelephone,
	TblPupilManagementAddresses.txtWorkTelephone,
	TblPupilManagementAddresses.txtFax,
	TblPupilManagementAddresses.txtWorkFax,
	TblPupilManagementAddresses.txtLabelSalutation AS txtContactLabelSalutation,
	TblPupilManagementAddresses.txtLetterSalutation AS txtContactLetterSalutation,
	TblPupilManagementAddresses.txtJointLabelSalutation AS txtContactJointLabelSalutation,
	TblPupilManagementAddresses.txtJointLetterSalutation AS txtContactLetterLabelSalutation,
	TblPupilManagementAddresses.txtSOS AS txtContactSOSNote,

	TblPupilManagementAddresses.txtAddress1 AS txtAddressLine1,
	TblPupilManagementAddresses.txtAddress2 AS txtAddressLine2,
	TblPupilManagementAddresses.txtAddress3 AS txtAddressLine3,
	TblPupilManagementAddresses.txtTown,
	TblPupilManagementAddresses.txtCounty,
	TblPupilManagementAddresses.txtCountry,
	TblPupilManagementAddresses.txtPostcode,

	TblPupilManagementAddressGroup.TblPupilManagementAddressGroupID,
	TblPupilManagementAddressGroup.txtGroupName AS txtContactCustomGroupName,
	CASE WHEN TblPupilManagementAddressGroup.TblPupilManagementAddressGroupID IS NOT NULL THEN 'Yes'
	ELSE 'No' END AS txtIsContactCustomGroupMember,

	TblPupilManagementCustomFieldContactValue.intCustomFieldID AS intContactCustomFieldID,
	TblPupilManagementCustomFieldsContact.txtName AS txtContactCustomFieldName,
	TblPupilManagementCustomFieldContactValue.txtValue AS txtContactCustomFieldValue,

	--Family
	FamilyMother.txtMotherForename,
	FamilyMother.txtMotherSurname,
	FamilyFather.txtFatherForename,
	FamilyFather.txtFatherSurname,
	Siblings.txtSiblingSchoolID,
	Siblings.txtSiblingForename,
	Siblings.txtSiblingSurname,
	Siblings.dSiblingDOB,
	Siblings.txtSiblingGender,

	--School
	CurrentYearGroup.txtYearName AS txtCurrentYearGroup,
	TblPupilManagementPupils.txtForm AS txtCurrentForm,
	TblPupilManagementPupils.txtAcademicHouse AS txtCurrentAcademicHouse,
	TblPupilManagementPupils.txtBoardingHouse AS txtCurrentBoardingHouse,
	PastoralTutor.Firstname AS txtCurrentPastoralTutorForename,
	PastoralTutor.Surname AS txtCurrentPastoralTutorSurname,
	TblPupilManagementPupils.txtType AS txtCurrentStatus,
	CASE
		WHEN TblPupilManagementPupils.intSystemStatus = 1 THEN 'Current'
		WHEN TblPupilManagementPupils.intSystemStatus = 0 THEN 'Applicant'
		WHEN TblPupilManagementPupils.intSystemStatus = 10 THEN 'External Candidate'
		WHEN TblPupilManagementPupils.intSystemStatus = 55 THEN 'Re-Admission'
		WHEN TblPupilManagementPupils.intSystemStatus = -1 THEN 'Leaver'
		ELSE Convert(varchar(2),TblPupilManagementPupils.intSystemStatus)
	END	AS txtCurrentSystemStatus,

	TblPupilManagementPupils.txtFutureForm AS txtNextYearForm,
	TblPupilManagementPupils.txtFutureAcademicHouse AS txtNextYearAcademicHouse,
	TblPupilManagementPupils.txtFutureHouse AS txtNextYearBoardingHouse,

	TblPupilManagementSelectionsStudents.intSelectionID  AS intCustomGroupID,
	TblPupilManagementSelections.txtName AS txtCustomGroupName,
	CustomGroupOwner.txtFirstname AS txtCustomGroupOwnerFirstname,
	CustomGroupOwner.txtSurname AS txtCustomGroupOwnerSurname,

	--Enrolment
	TblPupilManagementPupils.txtVerification AS txtEnrolmentInformationVerification,
	TblPupilManagementPupils.txtEntryType AS txtEnrolmentInformationEntryType,
	TblPupilManagementPupils.txtEnrolmentDate AS dEnrolmentDate,
	CASE WHEN TblPupilManagementPupils.intEnrolmentSchoolYear = 0 THEN NULL
	ELSE TblPupilManagementPupils.intEnrolmentSchoolYear END AS intEnrolmentSchoolYear,
	EnrolmentYearGroup.txtYearName AS txtEnrolmentYearGroup,
	TblPupilManagementPupils.txtEnrolmentForm,
	TblPupilManagementPupils.txtEnrolmentHouse AS txtEnrolmentBoardingHouse,
	TblPupilManagementPupils.txtEnrolmentAcademicHouse,
	TblPupilManagementPupils.txtEnrolmentTerm,
	TblSchools.Name AS txtEnrolmentInformationPreviousSchool,
	TblPupilManagementPreviousEducationValues.txtName AS txtEnrolmentInformationPreviousEducation,

	TblPupilManagementPupils.txtLeavingReason,
	TblPupilManagementPupils.txtLeavingDate AS dLeavingDate,
	TblPupilManagementPupils.txtLeavingTerm,
	CASE WHEN TblPupilManagementPupils.intLeavingSchoolYear = 0 THEN NULL
	ELSE TblPupilManagementPupils.intLeavingSchoolYear END AS intLeavingSchoolYear,
	LeavingYearGroup.txtYearName AS txtLeavingYearGroup,
	TblPupilManagementPupils.txtLeavingForm,
	TblPupilManagementPupils.txtLeavingAcademicHouse,
	TblPupilManagementPupils.txtLeavingBoardingHouse,
	FutureSchool.Name AS txtLeavingInformationFutureSchool,
	TblPupilManagementPupils.txtFutureSchoolType AS txtLeavingInformationFutureSchoolType,
	TblPupilManagementPupils.txtFutureSchoolEnrolmentDate AS dLeavingInformationFutureSchoolEnrolmentDate,
	TblPupilManagementPostYear13LeaversValues.txtName AS txtLeavingInformationPostYear13Leavers,

	--Enq and Visits
	TblPupilManagementPupils.txtEnquiryDate AS dEnquiryDate,
	TblPupilManagementPupils.txtEnquiryType,
	TblPupilManagementPupils.txtEnquiryReason,
	TblPupilManagementPupils.txtEnquiryNotes,

	TblPupilManagementPupils.txtOfferType,

	TblPupilManagementPupils.txtAdmissionsDate AS dAdmissionsDate,
	TblPupilManagementPupils.txtAdmissionsStatus,
	TblPupilManagementPupils.txtWithdrawnDate AS dWithdrawnDate,
	TblPupilManagementPupils.txtWithdrawnReason,
	TblPupilManagementPupils.txtWithdrawnReasonNote,
	TblPupilManagementPupils.txtRejectedDate AS dRejectedDate,
	TblPupilManagementPupils.txtRejectedReason,
	TblPupilManagementPupils.txtRejectedReasonNote,

	TblPupilManagementPupils.txtProspectusEnquiryDate AS dProspectusEnquiryDate,
	TblPupilManagementPupils.txtProspectusSentDate AS dProspectusSentDate,
	TblPupilManagementPupils.txtProspectusNotes,

	TblPupilManagementPupils.txtVisitDate AS dSchoolVisitDate,
	TblPupilManagementPupils.txtVisitNotes AS txtSchoolVisitNotes,
	TblPupilManagementPupils.txtHouseVisits,

	---Ass and Fees
	TblPupilManagementPupils.txtReferenceRequestedDate AS dReferenceRequestedDate,
	TblPupilManagementPupils.txtReferenceReceivedDate AS dReferenceReceivedDat,
	TblPupilManagementPupils.txtLearningSupportReceived,

	TblPupilManagementPupils.txtInterviewDate AS dInterviewDate,
	TblPupilManagementPupils.txtInterviewTime,
	TblPupilManagementPupils.txtInterviewNotes,

	TblPupilManagementPupils.txtRegisteredDate AS dRegisteredDate,
	TblPupilManagementPupils.txtDepositDate AS dRegistrationFeeDate,
	TblPupilManagementPupils.txtDepositAmount AS txtRegistrationFeeAmount,
	TblPupilManagementPupils.txtDepositSignatory AS txtRegistrationFeeSignature,
	TblPupilManagementPupils.txtEntranceFeeDate AS dEntranceFeeDate,
	TblPupilManagementPupils.txtEntranceFeeAmount,
	TblPupilManagementPupils.txtEntranceFeeSignatory,
	TblPupilManagementPupils.txtEntranceExamDate AS dCourierExamFeeDate,
	TblPupilManagementPupils.txtEntranceExamAmount AS txtCourierExamFeeAmount,
	TblPupilManagementPupils.txtEntranceExamSignatory AS txtCourierExamFeeSignature,

	--census
	CASE WHEN TblPupilManagementCensusExclusion.blnExcludeFromISCCensus = 1 THEN 'Yes' 
	WHEN TblPupilManagementCensusExclusion.blnExcludeFromISCCensus = 0 THEN 'No'
	ELSE NULL END AS txtExcludeFromISCCensus,
	CASE WHEN TblPupilManagementCensusExclusion.blnExcludeFromSchoolCensus = 1 THEN 'Yes' 
	WHEN TblPupilManagementCensusExclusion.blnExcludeFromSchoolCensus = 0 THEN 'No'
	ELSE NULL END AS txtExcludeFromSchoolCensus,
	TblPupilManagementPupils.txtCountryofResidence AS txtCensusResidentCountry,
	TblPupilManagementPupils.txtNationality AS txtCensusNationality,
	TblPupilManagementPupils.txtLanguage AS txtCensusLanguage,
	TblPupilManagementProficiencyInEnglishLevel.txtName AS txtCensusProficiencyInEnglish,
	TblPupilManagementPupils.txtEthnicGroup AS txtCensusEthnicCategory,
	TblPupilManagementPupils.txtEthnicitySource AS txtCensusEthnicitySource,
	CASE WHEN TblPupilManagementPupils.blnDiplomaticAndForces = 1 THEN 'Yes' 
	ELSE 'No'END AS txtCensusDiplomaticAndForces,
	TblPupilManagementPupils.txtSchoolLink AS txtCensusSchoolLink,

	TblPupilManagementPupils.txtReligion AS txtCensusReligion,
	CASE WHEN TblPupilManagementPupils.blnBaptised = 1 THEN 'Yes' 
	ELSE 'No'END AS txtCensusBaptised,
	CASE WHEN TblPupilManagementPupils.blnConfirmation = 1 THEN 'Yes' 
	ELSE 'No'END AS txtCensusConfirmed,

	TblPupilManagementPupils.txtBirthPlace AS txtCensusBirthPlace,
	TblPupilManagementPupils.txtBirthCounty AS txtCensusBirthCounty,
	TblPupilManagementPupils.txtBirthCountry AS txtCensusBirthCountry,

	CASE WHEN TblPupilManagementPupils.blnCensusSchoolLunchTaken = 1 THEN 'Yes' 
	WHEN TblPupilManagementPupils.blnCensusSchoolLunchTaken = 0 THEN 'No' 
	ELSE NULL END AS txtCensusSchoolLunchTaken,
	TblPupilManagementPupils.txtCensusYouthSupportServiceAgreement,
	TblPupilManagementPupils.txtCensusServiceChild,
	TblPupilManagementPupils.decCensusHoursAtSetting,
	TblPupilManagementPupils.decCensusFundedHours,
	TblPupilManagementPupilCharacteristic.decExtendedChildcareHours AS decCensusExtendedChildcareHours,
	TblPupilManagementPupilCharacteristic.txt30HourCodeIndicator AS txtCensus30HourCodeIndicator,
	CASE WHEN TblPupilManagementPupilCharacteristic.blnDisabilityAccessFundIndicator = 1 THEN 'Yes' 
	WHEN TblPupilManagementPupilCharacteristic.blnDisabilityAccessFundIndicator = 0 THEN 'No' 
	ELSE NULL END AS txtCensusDisabilityAccessFundIndicator,
	TblPupilManagementPupilCharacteristic.txt2YearOldsBasisForFunding AS txtCensus2YearOldsBasisForFunding,
	TblPupilManagementPupilCharacteristic.intCensusPruContactHours,
	CASE WHEN TblPupilManagementPupilCharacteristic.blnCensusEarlyYearsPupilPremiumEligibility = 1 THEN 'Eligible' 
	WHEN TblPupilManagementPupilCharacteristic.blnCensusEarlyYearsPupilPremiumEligibility = 0 THEN 'Not Eligible' 
	ELSE NULL END AS txtEarlyYearsPupilPremiumEligibility,
	TblPupilManagementPupils.txtCensusEarlyYearsPupilPremiumEligibilityBasis,
	CASE WHEN TblPupilManagementPupils.blnCensusTopupFunding = 1 THEN 'Yes' 
	WHEN TblPupilManagementPupils.blnCensusTopupFunding = 0 THEN 'No' 
	ELSE NULL END AS txtCensusTopupFunding,
	TblPupilManagementPupils.intCensusPlannedLearningHours,
	TblPupilManagementPupils.intCensusPlannedNonLearningHours,
	TblPupilManagementPupils.intCensusPreviousPlannedLearningHours,
	TblPupilManagementPupils.intCensusPreviousPlannedNonLearningHours,
	CASE WHEN TblPupilManagementPupils.blnCensusFullTimeEmployment = 1 THEN 'Yes' 
	WHEN TblPupilManagementPupils.blnCensusFullTimeEmployment = 0 THEN 'No' 
	ELSE NULL END AS txtCensusFullTimeEmployment,

	TblPupilManagementPupils.txtCensusMathsGCSEHighestPriorAttainment,
	MathsGCSE.txtDetails AS txtCensusMathsGCSEHighestPriorAttainmentYearGroup,
	TblPupilManagementPupils.txtCensusEnglishGCSEHighestPriorAttainment,
	EnglishGCSE.txtDetails AS txtCensusEnglishGCSEHighestPriorAttainmentYearGroup,
	TblPupilManagementPupils.txtCensusMathsFundingExemption,
	TblPupilManagementPupils.txtCensusEnglishFundingExemption,

	TblPupilManagementPupils.txtEnrolmentStatus AS txtCensusEnrolmentStatus,
	TblPupilManagementPupils.txtCensusTypeOfClass,
	CASE WHEN TblPupilManagementPupils.blnCensusPartTime = 1 THEN 'Yes' 
	WHEN TblPupilManagementPupils.blnCensusPartTime = 0 THEN 'No' 
	ELSE NULL END AS txtCensusPartTime



	FROM TblPupilManagementPupils
	LEFT OUTER JOIN TblPupilManagementAddressLink ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementAddressLink.txtSchoolID
	LEFT OUTER JOIN TblPupilManagementAddresses ON TblPupilManagementAddressLink.intAddressID = TblPupilManagementAddresses.TblPupilManagementAddressesID
	LEFT OUTER JOIN TblPupilManagementAddressGroupLink ON TblPupilManagementAddresses.TblPupilManagementAddressesID = TblPupilManagementAddressGroupLink.intPupilManagementAddressID
	LEFT OUTER JOIN TblPupilManagementAddressGroup ON TblPupilManagementAddressGroupLink.intGroupID = TblPupilManagementAddressGroup.TblPupilManagementAddressGroupID
	LEFT OUTER JOIN TblPupilManagementCustomFieldContactValue ON TblPupilManagementAddresses.TblPupilManagementAddressesID = TblPupilManagementCustomFieldContactValue.txtContactID
	LEFT OUTER JOIN TblPupilManagementCustomFieldsContact ON TblPupilManagementCustomFieldContactValue.intCustomFieldID = TblPupilManagementCustomFieldsContact.TblPupilManagementCustomFieldsContactID
	LEFT OUTER JOIN TblPupilManagementFamily ON TblPupilManagementPupils.intFamily = TblPupilManagementFamily.TblPupilManagementFamilyID
	OUTER APPLY(
	SELECT 
	CASE WHEN TblUsefulLists.Description = 'Two' THEN txtSecondaryForename ELSE txtContactsForename END AS txtMotherForename, 
	CASE WHEN TblUsefulLists.Description = 'Two' THEN txtSecondarySurname ELSE txtContactsSurname END AS txtMotherSurname, 
	TblUsefulLists.Description
	FROM TblPupilManagementAddresses
	INNER JOIN TblUsefulLists ON TblPupilManagementAddresses.txtRelationType = TblUsefulLists.Name AND TblUsefulLists.ListType = 'ContactType'
	WHERE TblPupilManagementAddressesID = TblPupilManagementFamily.intMotherID) AS FamilyMother
	OUTER APPLY(
	SELECT 
	CASE WHEN TblUsefulLists.Description = 'Two' THEN txtContactsForename ELSE txtSecondaryForename END AS txtFatherForename, 
	CASE WHEN TblUsefulLists.Description = 'Two' THEN txtContactsSurname ELSE txtSecondarySurname END AS txtFatherSurname, 
	TblUsefulLists.Description
	FROM TblPupilManagementAddresses
	INNER JOIN TblUsefulLists ON TblPupilManagementAddresses.txtRelationType = TblUsefulLists.Name AND TblUsefulLists.ListType = 'ContactType'
	WHERE TblPupilManagementAddressesID = TblPupilManagementFamily.intMotherID) AS FamilyFather

	OUTER APPLY
	(
	SELECT txtSchoolID AS txtSiblingSchoolID,
	txtForename AS txtSiblingForename,
	txtSurname AS txtSiblingSurname,
	txtGender AS txtSiblingGender,
	txtDOB AS dSiblingDOB
	FROM TblPupilManagementPupils AS Siblings
	WHERE Siblings.intFamily = TblPupilManagementPupils.intFamily
	AND Siblings.txtSchoolID &lt;&gt; TblPupilManagementPupils.txtSChoolID
	AND TblPupilManagementPupils.intFamily &lt;&gt; 0

	UNION ALL

	SELECT txtSchoolID AS txtSiblingSchoolID,
	txtForename AS txtSiblingForename,
	txtSurname AS txtSiblingSurname,
	txtGender AS txtSiblingGender,
	txtDOB AS dSiblingDOB
	FROM TblPupilManagementSiblings
	WHERE TblPupilManagementSiblings.intFamily = TblPupilManagementPupils.intFamily
	) AS Siblings

	LEFT OUTER JOIN TblSchoolManagementYears AS CurrentYearGroup ON TblPupilManagementPupils.intNCYear = CurrentYearGroup.intNCYear
	LEFT OUTER JOIN TblStaff AS PastoralTutor ON TblPupilManagementPupils.txtTutor = PastoralTutor.User_Code
	LEFT OUTER JOIN TblPupilManagementSelectionsStudents ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementSelectionsStudents.txtSchoolID
	LEFT OUTER JOIN TblPupilManagementSelections ON TblPupilManagementSelectionsStudents.intSelectionID = TblPupilManagementSelections.TblPupilManagementSelectionsID
	LEFT OUTER JOIN TbliSAMSManagerUsers AS CustomGroupOwner ON TblPupilManagementSelections.txtSubmitBy = CustomGroupOwner.txtUserCode

	LEFT OUTER JOIN TblSchoolManagementYears AS EnrolmentYearGroup ON TblPupilManagementPupils.intEnrolmentNCYear = EnrolmentYearGroup.intNCYear
	LEFT OUTER JOIN TblSchools ON TblPupilManagementPupils.intPreviousSchool = TblSchools.TblSchoolsID
	LEFT OUTER JOIN TblPupilManagementPreviousEducation ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementPreviousEducation.txtSchoolID
	LEFT OUTER JOIN TblPupilManagementPreviousEducationValues ON TblPupilManagementPreviousEducation.intPreviousEducationValuesId = TblPupilManagementPreviousEducationValues.TblPupilManagementPreviousEducationValuesID
	LEFT OUTER JOIN TblSchoolManagementYears AS LeavingYearGroup ON TblPupilManagementPupils.intLeavingNCYear = LeavingYearGroup.intNCYear
	LEFT OUTER JOIN TblSchools AS FutureSchool ON TblPupilManagementPupils.intFutureSchool = FutureSchool.TblSchoolsID
	LEFT OUTER JOIN TblPupilManagementPostYear13LeavingDestination ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementPostYear13LeavingDestination.txtSchoolID
	LEFT OUTER JOIN TblPupilManagementPostYear13LeaversValues ON TblPupilManagementPostYear13LeavingDestination.intPostYear13LeaversValueId = TblPupilManagementPostYear13LeaversValues.TblPupilManagementPostYear13LeaversValuesID

	LEFT OUTER JOIN TblPupilManagementCensusExclusion ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementCensusExclusion.txtSchoolID
	LEFT OUTER JOIN TblPupilManagementProficiencyInEnglish ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementProficiencyInEnglish.txtSchoolId
	LEFT OUTER JOIN TblPupilManagementProficiencyInEnglishLevel ON TblPupilManagementProficiencyInEnglish.intPupilManagementProficiencyInEnglishLevel = TblPupilManagementProficiencyInEnglishLevel.TblPupilManagementProficiencyInEnglishLevelID

	LEFT OUTER JOIN TblPupilManagementPupilCharacteristic ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementPupilCharacteristic.txtSchoolID
	LEFT OUTER JOIN CensusGCSEHighestPriorAttainmentYearGroup AS EnglishGCSE ON TblPupilManagementPupils.txtCensusEnglishGCSEHighestPriorAttainmentYearGroup = EnglishGCSE.txtCensusMathsGCSEHighestPriorAttainmentYearGroup
	LEFT OUTER JOIN CensusGCSEHighestPriorAttainmentYearGroup AS MathsGCSE ON TblPupilManagementPupils.txtCensusMathsGCSEHighestPriorAttainmentYearGroup = MathsGCSE.txtCensusMathsGCSEHighestPriorAttainmentYearGroup

	
	
	WHERE TblPupilManagementPupils.intPersonID = @intPersonId
