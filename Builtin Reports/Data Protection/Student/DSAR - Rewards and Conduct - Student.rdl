ers><Filter><FilterExpression>=IIF(isnothing(Fields!TblRewardsManagerFieldsID.Value),false,true)</FilterExpression><Operator>Equal</Operator><FilterValues><FilterValue>=true</FilterValue></FilterValues></Filter></Filters></Group><SortExpressions><SortExpression><Value>=Fields!TblRewardsManagerFieldsID.Value</Value></SortExpression></SortExpressions></TablixMember></TablixMembers></TablixColumnHierarchy><TablixRowHierarchy><TablixMembers><TablixMember><KeepWithGroup>After</KeepWithGroup></TablixMember><TablixMember><Group Name="TblRewardsManagerRewardsID"><GroupExpressions><GroupExpression>=Fields!TblRewardsManagerRewardsID.Value</GroupExpression></GroupExpressions></Group><SortExpressions><SortExpression><Value>=Fields!dtRewardDate.Value</Value></SortExpression></SortExpressions></TablixMember></TablixMembers></TablixRowHierarchy><DataSetName>RewardsAndConduct</DataSetName><Left>5cm</Left><Height>1.2cm</Height><Width>42.5cm</Width><ZIndex>1</ZIndex><Style><Border><Style>None</Style></Border></Style></Tablix></ReportItems><Height>0.47244in</Height><Style /></Body><Width>18.70079in</Width><Page><PageHeight>29.7cm</PageHeight><PageWidth>21cm</PageWidth><LeftMargin>2cm</LeftMargin><RightMargin>2cm</RightMargin><TopMargin>2cm</TopMargin><BottomMargin>2cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>09c25523-b7f0-40b2-a998-41c5672584d9</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="RewardsAndConduct"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value>=Parameters!intPersonId.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT 

--core
TblPupilManagementPupils.intPersonID,
TblPupilManagementPupils.txtSurname,
TblPupilManagementPupils.txtForename,

--Reward Record
TblRewardsManagerRewards.TblRewardsManagerRewardsID,
TbliSAMSManagerUsers.txtFirstname AS txtAwardingTeacherFirstname,
TbliSAMSManagerUsers.txtSurname AS txtAwardingTeacherSurname,
TblRewardsManagerRewards.txtDate AS dtRewardDate,
TblTeachingManagerSubjectDepartments.txtDepartmentName AS txtRewardDepartment,
TblTeachingManagerSubjects.txtSubjectName AS txtRewardSubject,
TblRewardsManagerRewards.txtDescription AS txtRewardComments,
TblRewardsManagerRewardTypes.txtName AS txtRewardType,
TblRewardsManagerRewardTypesCategories.txtName AS txtRewardCategory,
TblRewardsManagerRewardTypesCategories.intValue AS intRewardValue,
CASE WHEN TblRewardsManagerRewardTypes.intPositive = -1 THEN 'Negative' 
WHEN TblRewardsManagerRewardTypes.intPositive = 1 THEN 'Positive' 
ELSE NULL END AS txtRewardPositiveNegative,

--Record Fields
TblRewardsManagerFields.TblRewardsManagerFieldsID,
TblRewardsManagerFields.txtFieldName AS txtRewardRecordField,
TblRewardsManagerFieldValues.txtValueName AS txtRewardRecordFieldValue,

-- Resolution
CASE WHEN TblRewardsManagerRewards.blnResolved = 1 THEN 'Yes' 
WHEN TblRewardsManagerRewards.blnResolved = 0 THEN 'No' 
ELSE NULL END AS txtRewardResolved,
TblRewardsManagerRewards.txtResolvedNote AS txtResolutionNote,
TblRewardsManagerRewards.txtResolvedComments AS txtResolutionComments,
ResolvedBy.txtFirstname AS txtResolvedByFirstname,
ResolvedBy.txtSurname AS txtResolvedBySurname,
TblRewardsManagerRewards.txtResolvedDateTime AS dtResolvedDateTime,

--Authorisation 
CASE WHEN TblRewardsManagerRewards.intAuthorised = 1 THEN 'Yes' 
WHEN TblRewardsManagerRewards.intAuthorised = 0 THEN 'No' 
ELSE NULL END AS txtRewardAuyhorised,
AuthorisedBy.txtFirstname AS txtAuthorisedByFirstname,
AuthorisedBy.txtSurname AS txtAuthorisedBySurname


FROM TblPupilManagementPupils
LEFT OUTER JOIN TblRewardsManagerRewards ON TblPupilManagementPupils.txtSchoolID = TblRewardsManagerRewards.txtSchoolID
LEFT OUTER JOIN TblRewardsManagerRewardTypesCategories ON TblRewardsManagerRewards.intCategory = TblRewardsManagerRewardTypesCategories.TblRewardsManagerRewardTypesCategoriesID
LEFT OUTER JOIN TblRewardsManagerRewardTypes ON TblRewardsManagerRewardTypesCategories.intType = TblRewardsManagerRewardTypes.TblRewardsManagerRewardTypesID
LEFT OUTER JOIN TbliSAMSManagerUsers ON TblRewardsManagerRewards.txtTeacher = TbliSAMSManagerUsers.txtUserCode
LEFT OUTER JOIN TbliSAMSManagerUsers AS ResolvedBy ON TblRewardsManagerRewards.txtResolvedBy = ResolvedBy.txtUserCode
LEFT OUTER JOIN TbliSAMSManagerUsers AS AuthorisedBy ON TblRewardsManagerRewards.txtAuthorisedBy = AuthorisedBy.txtUserCode

LEFT OUTER JOIN TblRewardsManagerRewardsValueLink ON TblRewardsManagerRewards.TblRewardsManagerRewardsID = TblRewardsManagerRewardsValueLink.intRewardID
LEFT OUTER JOIN TblRewardsManagerFieldValues ON TblRewardsManagerRewardsValueLink.intFieldValueID = TblRewardsManagerFieldValues.TblRewardsManagerFieldValuesID
LEFT OUTER JOIN TblRewardsManagerFields ON TblRewardsManagerFieldValues.intFieldID = TblRewardsManagerFields.TblRewardsManagerFieldsID

LEFT OUTER JOIN TblTeachingManagerSubjectDepartments ON TblRewardsManagerRewards.intDepartment = TblTeachingManagerSubjectDepartments.TblTeachingManagerSubjectDepartmentsID 
LEFT OUTER JOIN TblTeachingManagerSubjects ON TblRewardsManagerRewards.intSubject = TblTeachingManagerSubjects.TblTeachingManagerSubjectsID

WHERE TblPupilManagementPupils.intPersonID = @intPersonId
</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TblRewardsManagerRewardsID"><DataField>TblRewardsManagerRewardsID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtAwardingTeacherFirstname"><DataField>txtAwardingTeacherFirstname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAwardingTeacherSurname"><DataField>txtAwardingTeacherSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dtRewardDate"><DataField>dtRewardDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtRewardDepartment"><DataField>txtRewardDepartment</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtRewardSubject"><DataField>txtRewardSubject</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtRewardComments"><DataField>txtRewardComments</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtRewardType"><DataField>txtRewardType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtRewardCategory"><DataField>txtRewardCategory</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intRewardValue"><DataField>intRewardValue</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtRewardPositiveNegative"><DataField>txtRewardPositiveNegative</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TblRewardsManagerFieldsID"><DataField>TblRewardsManagerFieldsID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtRewardRecordField"><DataField>txtRewardRecordField</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtRewardRecordFieldValue"><DataField>txtRewardRecordFieldValue</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtRewardResolved"><DataField>txtRewardResolved</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResolutionNote"><DataField>txtResolutionNote</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResolutionComments"><DataField>txtResolutionComments</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResolvedByFirstname"><DataField>txtResolvedByFirstname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtResolvedBySurname"><DataField>txtResolvedBySurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dtResolvedDateTime"><DataField>dtResolvedDateTime</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtRewardAuyhorised"><DataField>txtRewardAuyhorised</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAuthorisedByFirstname"><DataField>txtAuthorisedByFirstname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAuthorisedBySurname"><DataField>txtAuthorisedBySurname</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="intPersonId"><DataType>String</DataType><DefaultValue><Values><Value>17260</Value></Values></DefaultValue><Prompt>Student:</Prompt></ReportParameter></ReportParameters><ConsumeContainerWhitespace>true</ConsumeContainerWhitespace><rd:ReportUnitType>Cm</rd:ReportUnitType><rd:ReportID>c59d5b76-daca-48bd-a551-042d4b03db8c</rd:ReportID></Report>