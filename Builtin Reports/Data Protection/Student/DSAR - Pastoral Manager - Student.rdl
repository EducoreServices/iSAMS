urces><DataSets><DataSet Name="Concern"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value>=Parameters!intPersonId.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT
--core
Pupils.intPersonID,
Pupils.txtSurname,
Pupils.txtForename,

--Concern
Concern.Id AS intConcernID,
ConcernStatus.Name AS txtConcernStatus,
TblStaff.Firstname AS txtConcernSourceForename,
TblStaff.Surname AS txtConcernSourceSurname,
Concern.Location AS txtConcernLocation,
Concern.RaisedOn AS dtConcernDateTime,
ConcernType.Name AS txtConcernCategory,
DENSE_RANK() OVER (PARTITION BY Concern.Id ORDER BY ConcernArea.Id) AS intConcernAreaRank,
Area.Name AS txtConcernArea,
Area.Description AS txtConcernAreaDescription,
Concern.Nature AS txtConcernNature,
Concern.Details AS txtConcernDetails,

-- students invloved
DENSE_RANK() OVER (PARTITION BY Concern.Id ORDER BY ConcernStudent.Id) AS intConcernStudentRank,
TblPupilManagementPupils.txtForename AS txtConcernStudentForename,
TblPupilManagementPupils.txtSurname AS txtConcernStudentSurname,
StudentRole.Name AS txtConcernStudentRole,

-- action
DENSE_RANK() OVER (PARTITION BY Concern.Id ORDER BY ConcernAction.Id) AS intConcernActionRank,
ActionType.Name AS txtActionType,
ActionStaff.Firstname AS txtActionAssigneeFirstname,
ActionStaff.Surname AS txtActionAssigneeSurname,
ConcernAction.DueOn AS dActionDueDate,
ConcernAction.Description AS txtActionDescription,

-- documents
DENSE_RANK() OVER (PARTITION BY Concern.Id ORDER BY ConcernDocument.Id) AS intConcernDocumentRank,
ConcernDocument.Name AS txtDocumentName,
ConcernDocument.FilePath  + '/' + ConcernDocument.FileName AS txtDocumentPath, 

--form
DENSE_RANK() OVER (PARTITION BY Concern.Id ORDER BY ConcernForm.Id) AS intConcernFormRank,
ConcernForm.Name AS txtConcernFormName,
DENSE_RANK() OVER (PARTITION BY ConcernForm.Id ORDER BY FormQuestion.[Key], FormQuestion.ID) AS intConcernFormQuestionRank,
FormQuestion.[Key] AS txtFormQuestionKey,
FormQuestion.Text AS txtFormQuestion,
FormQuestionAnswer.Value AS txtFormAnswer


FROM dbo.TblPupilManagementPupils AS Pupils
LEFT OUTER JOIN Pastoral.Concern AS Concern ON Pupils.txtSchoolID = Concern.SchoolID
LEFT OUTER JOIN pastoral.ConcernStatus AS ConcernStatus ON Concern.ConcernStatusId = ConcernStatus.Id
LEFT OUTER JOIN TblStaff ON Concern.SourceId = TblStaff.User_Code
LEFT OUTER JOIN pastoral.ConcernType AS ConcernType ON Concern.TypeId = ConcernType.Id
LEFT OUTER JOIN pastoral.ConcernArea AS ConcernArea ON Concern.Id = ConcernArea.ConcernId
LEFT OUTER JOIN pastoral.Area AS Area ON ConcernArea.AreaId = Area.Id

LEFT OUTER JOIN pastoral.ConcernStudent AS ConcernStudent ON Concern.Id = ConcernStudent.ConcernId
LEFT OUTER JOIN TblPupilManagementPupils ON ConcernStudent.SchoolId = TblPupilManagementPupils.txtSchoolID
LEFT OUTER JOIN pastoral.ConcernRole AS StudentRole ON ConcernStudent.RoleId = StudentRole.Id

LEFT OUTER JOIN pastoral.ConcernAction AS ConcernAction ON Concern.Id = ConcernAction.ConcernId
LEFT OUTER JOIN pastoral.ActionType AS ActionType ON ConcernAction.ActionTypeId = ActionType.Id
LEFT OUTER JOIN pastoral.ConcernActionAssignee AS ConcernActionAssignee ON ConcernAction.Id = ConcernActionAssignee.ConcernActionId
LEFT OUTER JOIN TblStaff AS ActionStaff ON ConcernActionAssignee.Usercode = ActionStaff.User_Code

LEFT OUTER JOIN pastoral.ConcernDocument AS ConcernDocument ON Concern.Id = ConcernDocument.ConcernId

LEFT OUTER JOIN pastoral.ConcernForm AS ConcernForm ON Concern.Id = ConcernForm.Id
LEFT OUTER JOIN pastoral.FormQuestionAnswer AS FormQuestionAnswer ON ConcernForm.Id = FormQuestionAnswer.ConcernFormId
LEFT OUTER JOIN pastoral.FormQuestion AS FormQuestion ON FormQuestionAnswer.FormQuestionId = FormQuestion.Id
WHERE Pupils.intPersonID = @intPersonId</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intConcernID"><DataField>intConcernID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtConcernStatus"><DataField>txtConcernStatus</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtConcernSourceForename"><DataField>txtConcernSourceForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtConcernSourceSurname"><DataField>txtConcernSourceSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtConcernLocation"><DataField>txtConcernLocation</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dtConcernDateTime"><DataField>dtConcernDateTime</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtConcernCategory"><DataField>txtConcernCategory</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intConcernAreaRank"><DataField>intConcernAreaRank</DataField><rd:TypeName>System.Int64</rd:TypeName></Field><Field Name="txtConcernArea"><DataField>txtConcernArea</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtConcernAreaDescription"><DataField>txtConcernAreaDescription</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtConcernNature"><DataField>txtConcernNature</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtConcernDetails"><DataField>txtConcernDetails</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intConcernStudentRank"><DataField>intConcernStudentRank</DataField><rd:TypeName>System.Int64</rd:TypeName></Field><Field Name="txtConcernStudentForename"><DataField>txtConcernStudentForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtConcernStudentSurname"><DataField>txtConcernStudentSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtConcernStudentRole"><DataField>txtConcernStudentRole</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intConcernActionRank"><DataField>intConcernActionRank</DataField><rd:TypeName>System.Int64</rd:TypeName></Field><Field Name="txtActionType"><DataField>txtActionType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtActionAssigneeFirstname"><DataField>txtActionAssigneeFirstname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtActionAssigneeSurname"><DataField>txtActionAssigneeSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dActionDueDate"><DataField>dActionDueDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtActionDescription"><DataField>txtActionDescription</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intConcernDocumentRank"><DataField>intConcernDocumentRank</DataField><rd:TypeName>System.Int64</rd:TypeName></Field><Field Name="txtDocumentName"><DataField>txtDocumentName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtDocumentPath"><DataField>txtDocumentPath</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intConcernFormRank"><DataField>intConcernFormRank</DataField><rd:TypeName>System.Int64</rd:TypeName></Field><Field Name="txtConcernFormName"><DataField>txtConcernFormName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intConcernFormQuestionRank"><DataField>intConcernFormQuestionRank</DataField><rd:TypeName>System.Int64</rd:TypeName></Field><Field Name="txtFormQuestionKey"><DataField>txtFormQuestionKey</DataField><rd:TypeName>System.String</rd:TypeName></Field><Fi