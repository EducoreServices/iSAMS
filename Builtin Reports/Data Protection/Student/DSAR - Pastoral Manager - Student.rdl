FROM TbliSAMSManagerSettings</CommandText></Query><Fields><Field Name="txtInternaliSAMSAddress"><DataField>txtInternaliSAMSAddress</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="PastoralFlag"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value>=Parameters!intPersonId.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT
--core
Pupils.intPersonID,
Pupils.txtSurname,
Pupils.txtForename,

-- pastoral flag
PastoralFlag.Id AS intPastoralFlagID,
FlagType.Name AS txtPastoralFlagType,
PastoralFlag.Notes AS txtPastoralFlagNotes
FROM dbo.TblPupilManagementPupils AS Pupils
INNER JOIN pastoral.Flag AS PastoralFlag ON Pupils.txtSchoolID = PastoralFlag.SchoolId
LEFT OUTER JOIN pastoral.FlagType AS FlagType ON PastoralFlag.FlagTypeId = FlagType.Id
