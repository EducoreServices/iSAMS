/><TablixMember /><TablixMember /><TablixMember /><TablixMember /></TablixMembers></TablixColumnHierarchy><TablixRowHierarchy><TablixMembers><TablixMember><KeepWithGroup>After</KeepWithGroup></TablixMember><TablixMember><Group Name="TblDisciplineManagerExclusionID"><GroupExpressions><GroupExpression>=Fields!TblDisciplineManagerExclusionID.Value</GroupExpression></GroupExpressions></Group><SortExpressions><SortExpression><Value>=Fields!dtExclusionStartDate.Value</Value></SortExpression></SortExpressions></TablixMember></TablixMembers></TablixRowHierarchy><DataSetName>ExclusionsStudent</DataSetName><Left>5cm</Left><Height>1.2cm</Height><Width>47.5cm</Width><ZIndex>1</ZIndex><Style><Border><Style>None</Style></Border></Style></Tablix></ReportItems><Height>0.47244in</Height><Style /></Body><Width>20.66929in</Width><Page><PageHeight>29.7cm</PageHeight><PageWidth>21cm</PageWidth><LeftMargin>2cm</LeftMargin><RightMargin>2cm</RightMargin><TopMargin>2cm</TopMargin><BottomMargin>2cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>09c25523-b7f0-40b2-a998-41c5672584d9</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="ExclusionsStudent"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value>=Parameters!intPersonId.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT 

--core
TblPupilManagementPupils.intPersonID,
TblPupilManagementPupils.txtSurname,
TblPupilManagementPupils.txtForename,

-- exclusions
TblDisciplineManagerExclusion.TblDisciplineManagerExclusionID,
TblDisciplineManagerExclusionTypeList.txtName AS txtExclusionType,
TblDisciplineManagerExclusion.dteStartDateTime AS dtExclusionStartDate,
TblDisciplineManagerExclusion.dteEndDate AS dtExclusionEndDate,
TblDisciplineManagerExclusion.intSessionsAM, 
TblDisciplineManagerExclusion.intSessionsPM, 
TblDisciplineManagerExclusion.intSessionsLunch,
CASE WHEN TblDisciplineManagerExclusion.blnPartTime = 1 THEN 'Yes'
WHEN TblDisciplineManagerExclusion.blnPartTime = 0 THEN 'No'
ELSE NULL END AS txtExclusionPartTime,
TblDisciplineManagerExclusion.txtComments AS txtExclusionComments,
TblDisciplineManagerExclusion.txtAgencyInvolved,
TblDisciplineManagerExclusionReasonList.txtName AS txtExclusionReason,
CASE WHEN TblDisciplineManagerExclusion.intSessionStart = 1 THEN 'Morning Session'
WHEN TblDisciplineManagerExclusion.intSessionStart = 2 THEN 'Afternoon Session'
ELSE NULL END AS txtExclusionStartsAt,
TblDisciplineManagerExclusion.txtSenLevel AS txtSENProvision,

--Committee
TblDisciplineManagerExclusionCommittee.dteCommitteeDate,
TblDisciplineManagerExclusionCommitteeResultList.txtName AS txtCommitteeResult,
CASE WHEN TblDisciplineManagerExclusionCommittee.blnParentsPresentation = 1 THEN 'Yes'
WHEN TblDisciplineManagerExclusionCommittee.blnParentsPresentation = 0 THEN 'No'
ELSE NULL END AS txtCommitteeParentRepresentation,
TblDisciplineManagerExclusionCommittee.dteReinstatementDate AS dCommitteeReinstatementDate,

--Appeal
TblDisciplineManagerExclusionAppeal.dteAppealDate,
TblDisciplineManagerExclusionAppealResultList.txtName AS txtAppealResult,
TblDisciplineManagerExclusionAppeal.dteAppealReinstatementDate

FROM TblPupilManagementPupils
LEFT OUTER JOIN TblDisciplineManagerExclusion ON TblPupilManagementPupils.txtSchoolID = TblDisciplineManagerExclusion.txtSchoolID
LEFT OUTER JOIN TblDisciplineManagerExclusionTypeList ON TblDisciplineManagerExclusion.intExclusionType = TblDisciplineManagerExclusionTypeList.TblDisciplineManagerExclusionTypeListID 
LEFT OUTER JOIN TblDisciplineManagerExclusionReasonList ON TblDisciplineManagerExclusion.intExclusionReason = TblDisciplineManagerExclusionReasonList.TblDisciplineManagerExclusionReasonListID

LEFT OUTER JOIN TblDisciplineManagerExclusionCommitteeLink ON TblDisciplineManagerExclusion.TblDisciplineManagerExclusionID = TblDisciplineManagerExclusionCommitteeLink.intDisciplineManagerExclusionID
LEFT OUTER JOIN TblDisciplineManagerExclusionCommittee ON TblDisciplineManagerExclusionCommitteeLink.intDisciplineManagerExclusionCommitteeID = TblDisciplineManagerExclusionCommittee.TblDisciplineManagerExclusionCommitteeID
LEFT OUTER JOIN TblDisciplineManagerExclusionCommitteeResultList ON TblDisciplineManagerExclusionCommittee.intCommitteeResult = TblDisciplineManagerExclusionCommitteeResultList.TblDisciplineManagerExclusionCommitteeResultListID

LEFT OUTER JOIN TblDisciplineManagerExclusionAppeal ON TblDisciplineManagerExclusionCommittee.intDisciplineManagerExclusionAppealID = TblDisciplineManagerExclusionAppeal.TblDisciplineManagerExclusionAppealID
LEFT OUTER JOIN TblDisciplineManagerExclusionAppealResultList ON TblDisciplineManagerExclusionAppeal.intAppealResult = TblDisciplineManagerExclusionAppealResultList.TblDisciplineManagerExclusionAppealResultListID
WHERE TblPupilManagementPupils.intPersonID = @intPersonId

</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TblDisciplineManagerExclusionID"><DataField>TblDisciplineManagerExclusionID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtExclusionType"><DataField>txtExclusionType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dtExclusionStartDate"><DataField>dtExclusionStartDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="dtExclusionEndDate"><DataField>dtExclusionEndDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="intSessionsAM"><DataField>intSessionsAM</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="intSessionsPM"><DataField>intSessionsPM</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="intSessionsLunch"><DataField>intSessionsLunch</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtExclusionPartTime"><DataField>txtExclusionPartTime</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtExclusionComments"><DataField>txtExclusionComments</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAgencyInvolved"><DataField>txtAgencyInvolved</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtExclusionReason"><DataField>txtExclusionReason</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtExclusionStartsAt"><DataField>txtExclusionStartsAt</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSENProvision"><DataField>txtSENProvision</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dteCommitteeDate"><DataField>dteCommitteeDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtCommitteeResult"><DataField>txtCommitteeResult</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtCommitteeParentRepresentation"><DataField>txtCommitteeParentRepresentation</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dCommitteeReinstatementDate"><DataField>dCommitteeReinstatementDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="dteAppealDate"><DataField>dteAppealDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtAppealResult"><DataField>txtAppealResult</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dteAppealReinstatementDate"><DataField>dteAppealReinstatementDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="intPersonId"><DataType>String</DataType><DefaultValue><Values><Value>16024</Value></Values></DefaultValue><Prompt>Student:</Prompt></ReportParameter></ReportParameters><rd:ReportUnitType>Cm</rd:ReportUnitType><rd:ReportID>c59d5b76-daca-48bd-a551-042d4b03db8c</rd:ReportID></Report>