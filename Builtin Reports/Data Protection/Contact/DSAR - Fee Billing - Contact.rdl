LEFT OUTER JOIN TblFeeBillingPupilDepositSourceType ON TblFeeBillingPupilDeposit.intSourceTypeID = TblFeeBillingPupilDepositSourceType.TblFeeBillingPupilDepositSourceTypeID
LEFT OUTER JOIN TblFeeBillingPupilDepositPaymentMethod ON TblFeeBillingPupilDeposit.intPaymentMethodTypeID = TblFeeBillingPupilDepositPaymentMethodID
WHERE CoreContactDetials.intPersonID = @intPersonId</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtPersonForename"><DataField>txtPersonForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPersonSurname"><DataField>txtPersonSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TblFeeBillingPupilDepositID"><DataField>TblFeeBillingPupilDepositID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtDepositType"><DataField>txtDepositType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="mAmountRefunded"><DataField>mAmountRefunded</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="dRefundedDate"><DataField>dRefundedDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtRefundedReturnAction"><DataField>txtRefundedReturnAction</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtDepositStatus"><DataField>txtDepositStatus</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtDepositSource"><DataField>txtDepositSource</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtDepositPaymentMethid"><DataField>txtDepositPaymentMethid</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="DirectDebit"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value>=Parameters!intPersonId.Value</Value></QueryParameter></QueryParameters><CommandText>
WITH CoreContactDetials AS 
(
SELECT
TblPupilManagementAddresses.TblPupilManagementAddressesID,
TblPupilManagementAddresses.intPersonID,
TblPupilManagementAddresses.txtContactsForename AS txtPersonForename,
TblPupilManagementAddresses.txtContactsSurname AS txtPersonSurname,
TblPupilManagementAddresses.txtEmail1 AS txtPersonEmail,
TblPupilManagementAddresses.txtMobile1 AS txtPersonMobile
FROM TblPupilManagementAddresses

UNION 

SELECT
TblPupilManagementAddresses.TblPupilManagementAddressesID,
TblPupilManagementAddresses.intSecondaryPersonId AS intPersonID,
TblPupilManagementAddresses.txtSecondaryForename AS txtPersonForename,
TblPupilManagementAddresses.txtSecondarySurname AS txtPersonSurname,
TblPupilManagementAddresses.txtEmail2 AS txtPersonEmail,
TblPupilManagementAddresses.txtMobile2 AS txtPersonMobile
FROM TblPupilManagementAddresses
WHERE TblPupilManagementAddresses.intSecondaryPersonId IS NOT NULL

)

SELECT
--core
CoreContactDetials.intPersonID,
CoreContactDetials.txtPersonForename,
CoreContactDetials.txtPersonSurname,

--direct debit
TblFeeBillingDirectDebitPaymentPlanLink.TblFeeBillingDirectDebitPaymentPlanLinkID,
TblFeeBillingDirectDebitSchedule.TblFeeBillingDirectDebitScheduleId, 
TblFeeBillingInvoiceRun.txtName AS txtFeeBillingDirectDebitInvoiceRunName,
TblFeeBillingDirectDebitScheduleStatus.txtName AS txtFeeBillingDirectDebitScheduleStatus,
TblFeeBillingDirectDebitScheduledPayment.TblFeeBillingDirectDebitScheduledPaymentId, 
TblFeeBillingDirectDebitPaymentPlan.txtName AS txtFeeBillingDirectDebitPaymentPlanName,
TblFeeBillingDirectDebitScheduledPayment.dPaymentDate AS dFeeBillingDirectDebitPaymentDate, 
TblFeeBillingDirectDebitScheduledPayment.mAmount AS mFeeBillingDirectDebitAmount,
TblFeeBillingBillingNoteTemplate.txtTemplate AS txtFeeBillingDirectDebitNote

FROM CoreContactDetials
INNER JOIN TblFeeBillingBillPayerAccountDetail ON CoreContactDetials.TblPupilManagementAddressesID = TblFeeBillingBillPayerAccountDetail.intAddressID
INNER JOIN TblFeeBillingDirectDebitPaymentPlanLink ON TblFeeBillingDirectDebitPaymentPlanLink.intAccountDetailID = TblFeeBillingBillPayerAccountDetail.TblFeeBillingBillPayerAccountDetailID
INNER JOIN TblFeeBillingDirectDebitSchedule ON TblFeeBillingDirectDebitSchedule.intPlanLinkID = TblFeeBillingDirectDebitPaymentPlanLink.TblFeeBillingDirectDebitPaymentPlanLinkID
INNER JOIN TblFeeBillingDirectDebitScheduledPayment ON TblFeeBillingDirectDebitSchedule.TblFeeBillingDirectDebitScheduleId = TblFeeBillingDirectDebitScheduledPayment.intScheduleId
INNER JOIN TblFeeBillingInvoiceRun ON TblFeeBillingDirectDebitSchedule.intRunID = TblFeeBillingInvoiceRun.TblFeeBillingInvoiceRunId
INNER JOIN TblFeeBillingDirectDebitPaymentPlan ON TblFeeBillingDirectDebitPaymentPlanLink.intPlanID = TblFeeBillingDirectDebitPaymentPlan.TblFeeBillingDirectDebitPaymentPlanID
LEFT OUTER JOIN TblFeeBillingBillingNoteTemplate ON TblFeeBillingDirectDebitPaymentPlan.intBillingNoteTemplateID = TblFeeBillingBillingNoteTemplate.TblFeeBillingBillingNoteTemplateID
LEFT OUTER JOIN TblFeeBillingDirectDebitScheduleStatus ON TblFeeBillingDirectDebitSchedule.intStatus = TblFeeBillingDirectDebitScheduleStatus.TblFeeBillingDirectDebitScheduleStatusID

WHERE CoreContactDetials.intPersonID = @intPersonId</CommandText></Query><Fields><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtPersonForename"><DataField>txtPersonForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPersonSurname"><DataField>txtPersonSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TblFeeBillingDirectDebitPaymentPlanLinkID"><DataField>TblFeeBillingDirectDebitPaymentPlanLinkID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="TblFeeBillingDirectDebitScheduleId"><DataField>TblFeeBillingDirectDebitScheduleId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtFeeBillingDirectDebitInvoiceRunName"><DataField>txtFeeBillingDirectDebitInvoiceRunName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtFeeBillingDirectDebitScheduleStatus"><DataField>txtFeeBillingDirectDebitScheduleStatus</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="TblFeeBillingDirectDebitScheduledPaymentId"><DataField>TblFeeBillingDirectDebitScheduledPaymentId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtFeeBillingDirectDebitPaymentPlanName"><DataField>txtFeeBillingDirectDebitPaymentPlanName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="dFeeBillingDirectDebitPaymentDate"><DataField>dFeeBillingDirectDebitPaymentDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="mFeeBillingDirectDebitAmount"><DataField>mFeeBillingDirectDebitAmount</DataField><rd:TypeName>System.Decimal</rd:TypeName></Field><Field Name="txtFeeBillingDirectDebitNote"><DataField>txtFeeBillingDirectDebitNote</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="PostInvoiceCharges"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value