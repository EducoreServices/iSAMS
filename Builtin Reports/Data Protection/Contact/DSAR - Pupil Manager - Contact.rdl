rValues></Filter></Filters><Left>5cm</Left><Height>1.2cm</Height><Width>97.5cm</Width><ZIndex>1</ZIndex><Style><Border><Style>None</Style></Border></Style></Tablix></ReportItems><Height>0.47244in</Height><Style /></Body><Width>40.35433in</Width><Page><PageHeight>29.7cm</PageHeight><PageWidth>21cm</PageWidth><LeftMargin>2cm</LeftMargin><RightMargin>2cm</RightMargin><TopMargin>2cm</TopMargin><BottomMargin>2cm</BottomMargin><ColumnSpacing>0.13cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>09c25523-b7f0-40b2-a998-41c5672584d9</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Contact"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intPersonId"><Value>=Parameters!intPersonId.Value</Value></QueryParameter></QueryParameters><CommandText>WITH CoreContactDetials AS 
(

SELECT
TblPupilManagementAddresses.TblPupilManagementAddressesID,
TblPupilManagementAddresses.intPersonID,
TblPupilManagementAddresses.txtContactsTitle AS txtPersonTitle,
TblPupilManagementAddresses.txtContactsForename AS txtPersonForename,
TblPupilManagementAddresses.txtContactsMiddleNames AS txtPersonMiddleNames,
TblPupilManagementAddresses.txtContactsInitials AS txtPersonInitials,
TblPupilManagementAddresses.txtContactsSurname AS txtPersonSurname,
TblPupilManagementAddresses.txtEmail1 AS txtPersonEmail,
TblPupilManagementAddresses.txtMobile1 AS txtPersonMobile,
TblPupilManagementAddresses.txtProfession AS txtPersonProfession,
CASE WHEN TblPupilManagementAddresses.intDeceased =1 THEN 'Yes' 
ELSE 'No' END AS txtPersonDeceased,
CASE WHEN TblPupilManagementAddresses.blnContactHMForces =1 THEN 'Yes' 
ELSE 'No' END AS txtPersonHMForces,
CASE WHEN TblPupilManagementAddresses.intContactPTA =1 THEN 'Yes' 
ELSE 'No' END AS txtPersonPTFA

FROM TblPupilManagementAddresses


UNION 


SELECT
TblPupilManagementAddresses.TblPupilManagementAddressesID,
TblPupilManagementAddresses.intSecondaryPersonId AS intPersonID,
TblPupilManagementAddresses.txtSecondaryTitle AS txtPersonTitle,
TblPupilManagementAddresses.txtSecondaryForename AS txtPersonForename,
TblPupilManagementAddresses.txtSecondaryMiddleNames AS txtPersonMiddleNames,
TblPupilManagementAddresses.txtSecondaryInitials AS txtPersonInitials,
TblPupilManagementAddresses.txtSecondarySurname AS txtPersonSurname,
TblPupilManagementAddresses.txtEmail2 AS txtPersonEmail,
TblPupilManagementAddresses.txtMobile2 AS txtPersonMobile,
TblPupilManagementAddresses.txtSecondaryProfession AS txtPersonProfession,
CASE WHEN TblPupilManagementAddresses.intSecondaryDeceased =1 THEN 'Yes' 
ELSE 'No' END AS txtPersonDeceased,
CASE WHEN TblPupilManagementAddresses.blnSecondaryHMForces =1 THEN 'Yes' 
ELSE 'No' END AS txtPersonHMForces,
CASE WHEN TblPupilManagementAddresses.intSecondaryPTA =1 THEN 'Yes' 
ELSE 'No' END AS txtPersonPTFA

FROM TblPupilManagementAddresses
WHERE TblPupilManagementAddresses.intSecondaryPersonId IS NOT NULL

)

SELECT
CoreContactDetials.TblPupilManagementAddressesID,
CoreContactDetials.intPersonID,
CoreContactDetials.txtPersonTitle,
CoreContactDetials.txtPersonForename,
CoreContactDetials.txtPersonMiddleNames,
CoreContactDetials.txtPersonInitials,
CoreContactDetials.txtPersonSurname,
CoreContactDetials.txtPersonEmail,
CoreContactDetials.txtPersonMobile,
CoreContactDetials.txtPersonProfession,
CoreContactDetials.txtPersonDeceased,
CoreContactDetials.txtPersonHMForces,
CoreContactDetials.txtPersonPTFA,

TblPupilManagementAddresses.txtRelationType AS txtContactOrPartOfContactType,
CASE WHEN TblPupilManagementAddresses.intJustContact =1 THEN 'Yes' 
ELSE 'No' END AS txtContactOnly,
CASE WHEN TblPupilManagementAddresses.intMailMergeAll =1 THEN 'Yes' 
ELSE 'No' END AS txtAllMerges,
CASE WHEN TblPupilManagementAddresses.intBillingMailMerge =1 THEN 'Yes' 
ELSE 'No' END AS txtBillingContact,
CASE WHEN TblPupilManagementAddresses.intCorrespondenceMailMerge =1 THEN 'Yes' 
ELSE 'No' END AS txtCorrespondence,
CASE WHEN TblPupilManagementAddresses.intReportsMailMerge =1 THEN 'Yes' 
ELSE 'No' END AS txtReports,
CASE WHEN TblPupilManagementAddresses.intReport =1 THEN 'Yes' 
ELSE 'No' END AS txtAcademicReports,
CASE WHEN TblPupilManagementAddresses.intPrivate =1 THEN 'Yes' 
ELSE 'No' END AS txtPrivateContact,
CASE WHEN TblPupilManagementAddresses.intCanShare =1 THEN 'Yes' 
ELSE 'No' END AS txtContactCanBeSharedWithOtherParents,
CASE WHEN TblPupilManagementAddresses.intStudentHome =1 THEN 'Yes' 
ELSE 'No' END AS txtStudentHomes,
TblPupilManagementAddresses.txtAddressType AS txtContactLocation,
CASE WHEN TblPupilManagementAddresses.blnFutureAddress =1 THEN 'Yes' 
ELSE 'No' END AS txtFutureAddress,
TblPupilManagementAddresses.txtMovedInDate AS dMovedInDate,

TblPupilManagementAddresses.txtMaritalStatus AS txtPersonMaritalStatus,
TblPupilManagementAddresses.txtDayTelephone AS txtTelephone,
TblPupilManagementAddresses.txtWorkTelephone,
TblPupilManagementAddresses.txtFax,
TblPupilManagementAddresses.txtWorkFax,
TblPupilManagementAddresses.txtSOS AS txtContactSOSNote,

TblPupilManagementAddresses.txtAddress1 AS txtAddressLine1,
TblPupilManagementAddresses.txtAddress2 AS txtAddressLine2,
TblPupilManagementAddresses.txtAddress3 AS txtAddressLine3,
TblPupilManagementAddresses.txtTown,
TblPupilManagementAddresses.txtCounty,
TblPupilManagementAddresses.txtCountry,
TblPupilManagementAddresses.txtPostcode,

TblPupilManagementAddressGroup.TblPupilManagementAddressGroupID,
TblPupilManagementAddressGroup.txtGroupName AS txtContactCustomGroupName,
CASE WHEN TblPupilManagementAddressGroup.TblPupilManagementAddressGroupID IS NOT NULL THEN 'Yes'
ELSE 'No' END AS txtIsContactCustomGroupMember,

TblPupilManagementCustomFieldContactValue.intCustomFieldID AS intContactCustomFieldID,
TblPupilManagementCustomFieldsContact.txtName AS txtContactCustomFieldName,
TblPupilManagementCustomFieldContactValue.txtValue AS txtContactCustomFieldValue

FROM CoreContactDetials
INNER JOIN TblPupilManagementAddresses ON CoreContactDetials.TblPupilManagementAddressesID = TblPupilManagementAddresses.TblPupilManagementAddressesID
LEFT OUTER JOIN TblPupilManagementAddressGroupLink ON TblPupilManagementAddresses.TblPupilManagementAddressesID = TblPupilManagementAddressGroupLink.intPupilManagementAddressID
LEFT OUTER JOIN TblPupilManagementAddressGroup ON TblPupilManagementAddressGroupLink.intGroupID = TblPupilManagementAddressGroup.TblPupilManagementAddressGroupID
LEFT OUTER JOIN TblPupilManagementCustomFieldContactValue ON TblPupilManagementAddresses.TblPupilManagementAddressesID = TblPupilManagementCustomFieldContactValue.txtContactID
LEFT OUTER JOIN TblPupilManagementCustomFieldsContact ON TblPupilManagementCustomFieldContactValue.intCustomFieldID = TblPupilManagementCustomFieldsContact.TblPupilManagementCustomFieldsContactID
WHERE CoreContactDetials.intPersonID = @intPersonId
</CommandText></Query><Fields><Field Name="TblPupilManagementAddressesID"><DataField>TblPupilManagementAddressesID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="intPersonID"><DataField>intPersonID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtPersonTitle"><DataField>txtPersonTitle</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPersonForename"><DataField>txtPersonForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPersonMiddleNames"><DataField>txtPersonMiddleNames</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPersonInitials"><DataField>txtPersonInitials</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPersonSurname"><DataField>txtPersonSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPersonEmail"><DataField>txtPersonEmail</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPersonMobile"><DataField>txtPersonMobile</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPersonProfession"><DataField>txtPersonProfession</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPersonDeceased"><DataField>txtPersonDeceased</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPersonHMForces"><DataField>txtPersonHMForces</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtPersonPTFA"><DataField>txtPersonPTFA</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtContactOrPartOfContactType"><DataField>txtContactOrPartOfContactType</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtContactOnly"><DataField>txtContactOnly</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAllMerges"><DataField>txtAllMerges</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtBillingContact"><DataField>txtBi