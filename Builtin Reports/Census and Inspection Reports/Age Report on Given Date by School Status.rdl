        </Field>
      </Fields>
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT txtHouseName FROM TblSchoolManagementHouses
WHERE txtHouseType ='Boarding' 
ORDER BY txtHouseName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Form">
      <Fields>
        <Field Name="txtForm">
          <DataField>txtForm</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT txtForm
FROM TblSchoolManagementForms
ORDER BY intNCYear, txtForm</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="Year">
      <Fields>
        <Field Name="txtYearName">
