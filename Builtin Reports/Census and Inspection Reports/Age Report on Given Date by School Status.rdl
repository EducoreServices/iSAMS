        </Field>
        <Field Name="txtType">
          <DataField>txtType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT        txtSchoolID, dbo.GetAge(txtDOB, @GivenDate) AS intAge, txtGender, txtType
FROM            TblPupilManagementPupils
WHERE        (intSystemStatus = 1)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@GivenDate">
            <Value>=Parameters!GivenDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="AHouse">
      <Fields>
        <Field Name="txtHouseName">
          <DataField>txtHouseName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT txtHouseName FROM TblSchoolManagementHouses
WHERE txtHouseType ='Academic' 
ORDER BY txtHouseName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="BHouse">
      <Fields>
        <Field Name="txtHouseName">
