    <DataSet Name="Main">
      <Fields>
        <Field Name="txtSchoolID">
          <DataField>txtSchoolID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="intAge">
          <DataField>intAge</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="txtGender">
