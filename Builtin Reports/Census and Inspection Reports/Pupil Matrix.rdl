                         TblPupilManagementPupils.txtEnrolmentTerm, TblPupilManagementPupils.blnBaptised, TblPupilManagementPupils.blnConfirmation, 
                         TblPupilManagementPupils.txtFutureSchoolType, TblPupilManagementPupils.txtPreviousSchoolType
FROM            TblPupilManagementPupils LEFT OUTER JOIN
                         TblSchools ON TblPupilManagementPupils.intPreviousSchool = TblSchools.TblSchoolsID LEFT OUTER JOIN
                         TblSchools AS FutureScholol ON TblPupilManagementPupils.intFutureSchool = FutureScholol.TblSchoolsID LEFT OUTER JOIN
                         TblStaff ON TblPupilManagementPupils.txtTutor = TblStaff.User_Code
WHERE        (TblPupilManagementPupils.intNCYear IN (@intNCYear) OR
                         (- 10) IN (@intNCYear)) AND (TblPupilManagementPupils.intSystemStatus = 1) AND 
                         (TblPupilManagementPupils.txtAcademicHouse IN (@txtAcademicHouse) OR
                         'ALL' IN (@txtAcademicHouse)) AND (TblPupilManagementPupils.txtBoardingHouse IN (@txtBoardingHouse) OR
                         'ALL' IN (@txtBoardingHouse)) AND (TblPupilManagementPupils.txtForm IN (@txtForm) OR
                         'ALL' IN (@txtForm)) AND (TblPupilManagementPupils.txtType IN (@txtType) OR
                         'ALL' IN (@txtType)) AND (TblPupilManagementPupils.txtEnrolmentDate BETWEEN @DateStart AND @EndDate OR
                         TblPupilManagementPupils.txtEnrolmentDate IS NULL)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@intNCYear">
            <Value>=Parameters!intNCYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@txtAcademicHouse">
            <Value>=Parameters!txtAcademicHouse.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@txtBoardingHouse">
            <Value>=Parameters!txtBoardingHouse.Value</Value>
