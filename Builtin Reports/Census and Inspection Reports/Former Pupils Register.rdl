  <DataSets>
    <DataSet Name="Admission">
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@txtStartDate">
            <Value>=Parameters!txtStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@txtEndDate">
            <Value>=Parameters!txtEndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH Pupils AS
(
SELECT        
TblPupilManagementPupils.txtForename, 
TblPupilManagementPupils.txtMiddleNames, 
TblPupilManagementPupils.txtEnrolmentDate, 
TblPupilManagementPupils.txtSurname, 
TblPupilManagementPupils.txtForm, 
TblPupilManagementPupils.intNCYear, 
TblPupilManagementPupils.txtAdmissionsStatus, 
TblPupilManagementPupils.txtAdmissionsDate, 
TblPupilManagementPupils.txtAcademicHouse, 
TblPupilManagementPupils.txtGender, 
TblPupilManagementPupils.txtDOB, 
TblPupilManagementPupils.txtType, 
TblPupilManagementPupils.intSystemStatus,
TblPupilManagementPupils.txtSchoolID,
TblPupilManagementPupils.txtleavingreason,
TblPupilManagementPupils.txtLeavingdate,

FutureSchool.Name AS FutureSchoolName, 
FutureSchool.Address1 AS FutureSchoolAddress1, 
FutureSchool.Address2 AS FutureSchoolAddress2, 
FutureSchool.Address3 AS FutureSchoolAddress3, 
FutureSchool.Town AS FutureSchoolTown, 
FutureSchool.County AS FutureSchoolCounty, 
FutureSchool.Postcode AS FutureSchoolPostcode, 
FutureSchool.Country AS FutureSchoolCountry, 
FutureSchool.Telephone AS FutureSchoolTelephone,

TblPupilManagementPupils.txtFutureSchoolEnrolmentDate,
FutureSchool.Name AS txtFutureSchoolName
                         
FROM  TblPupilManagementPupils
LEFT OUTER JOIN TblSchools ON TblPupilManagementPupils.intPreviousSchool = TblSchools.TblSchoolsID
LEFT OUTER JOIN TblSchools AS FutureSchool ON TblPupilManagementPupils.intFutureSchool = FutureSchool.TblSchoolsID

WHERE
TblPupilManagementPupils.txtLeavingdate Between @txtStartDate and @txtEndDate
--(
--(TblPupilManagementPupils.intSystemStatus = 1) AND (TblPupilManagementPupils.txtEnrolmentDate &lt;= @txtOnRegisterDate OR TblPupilManagementPupils.txtEnrolmentDate IS NULL)
--)
--OR
--(
--(TblPupilManagementPupils.intSystemStatus = - 1) AND (TblPupilManagementPupils.txtEnrolmentDate &lt;= @txtOnRegisterDate) AND (TblPupilManagementPupils.txtLeavingDate &gt;= @txtOnRegisterDate) 
--)

),

Addresses AS
(
SELECT 
TblPupilManagementAddresses.txtDayTelephone, 
TblPupilManagementAddresses.txtAddress1, 
TblPupilManagementAddresses.txtAddress2, 
TblPupilManagementAddresses.txtAddress3, 
TblPupilManagementAddresses.txtTown, 
TblPupilManagementAddresses.txtCounty, 
TblPupilManagementAddresses.txtPostcode,  
TblPupilManagementAddresses.txtRelationType, 
TblPupilManagementAddresses.txtAddressType, 
TblPupilManagementAddresses.intStudentHome, 
TblPupilManagementAddresses.TblPupilManagementAddressesID,
TblPupilManagementAddresses.txtContactsTitle, 
TblPupilManagementAddresses.txtContactsForename, 
TblPupilManagementAddresses.txtContactsSurname, 
TblPupilManagementAddresses.txtSecondaryTitle, 
TblPupilManagementAddresses.txtSecondaryForename, 
TblPupilManagementAddresses.txtSecondarySurname,
TblPupilManagementAddresses.txtCountry,
TblPupilManagementAddressLink.txtSchoolID,
TblUsefulLists.Description AS txtHowMany,
TblPupilManagementAddresses.txtEmail1,
TblPupilManagementAddresses.txtEmail2

FROM TblPupilManagementAddressLink
INNER JOIN TblPupilManagementAddresses ON TblPupilManagementAddressLink.intAddressID = TblPupilManagementAddresses.TblPupilManagementAddressesID 
LEFT OUTER JOIN TblUsefulLists ON TblPupilManagementAddresses.txtRelationType = TblUsefulLists.Name AND TblUsefulLists.ListType = 'ContactType'
WHERE  
( (TblPupilManagementAddresses.intStudentHome = 1) AND (isnull(TblPupilManagementAddresses.blnFutureAddress,0) = 0))
 OR
 ((TblPupilManagementAddresses.txtAddressType LIKE '%Home%') AND (TblPupilManagementAddresses.txtRelationType IN ('Mother', 'Father', 'Parents', 'Father and Stepmother', 'Stepfather and Mother'))  AND (isnull(TblPupilManagementAddresses.blnFutureAddress,0) = 0 ))

),

FutureAddresses As
(
SELECT 
TblPupilManagementAddresses.txtDayTelephone AS txtFutureDayTelephone, 
TblPupilManagementAddresses.txtAddress1 AS txtFutureAddress1, 
TblPupilManagementAddresses.txtAddress2 AS txtFutureAddress2, 
TblPupilManagementAddresses.txtAddress3 AS txtFutureAddress3, 
TblPupilManagementAddresses.txtTown AS txtFutureTown, 
TblPupilManagementAddresses.txtCounty AS txtFutureCounty, 
TblPupilManagementAddresses.txtPostcode AS txtFuturePostcode,  
TblPupilManagementAddresses.txtRelationType AS txtFutureRelationType, 
TblPupilManagementAddresses.txtAddressType AS txtFutureAddressType, 
TblPupilManagementAddresses.intStudentHome AS intFutureStudentHome, 
TblPupilManagementAddresses.TblPupilManagementAddressesID AS intFutureAddressID,
TblPupilManagementAddresses.txtContactsTitle AS txtFutureContactsTitle, 
TblPupilManagementAddresses.txtContactsForename AS txtFutureContactsForename, 
TblPupilManagementAddresses.txtContactsSurname AS txtFutureContactsSurname, 
TblPupilManagementAddresses.txtSecondaryTitle AS txtFutureSecondaryTitle, 
TblPupilManagementAddresses.txtSecondaryForename AS txtFutureSecondaryForename, 
TblPupilManagementAddresses.txtSecondarySurname AS txtFutureSecondarySurname,
TblPupilManagementAddresses.txtCountry AS txtFutureCountry,
TblPupilManagementAddresses.txtMovedInDate,
TblPupilManagementAddressLink.txtSchoolID,
TblUsefulLists.Description AS txtFutureHowMany

FROM TblPupilManagementAddressLink
INNER JOIN TblPupilManagementAddresses ON TblPupilManagementAddressLink.intAddressID = TblPupilManagementAddresses.TblPupilManagementAddressesID 
LEFT OUTER JOIN TblUsefulLists ON TblPupilManagementAddresses.txtRelationType = TblUsefulLists.Name AND TblUsefulLists.ListType = 'ContactType'
WHERE  (isnull(TblPupilManagementAddresses.blnFutureAddress,0) = 1)
)

SELECT 
Pupils.*,
Addresses.TblPupilManagementAddressesID,
Addresses.intStudentHome,
Addresses.txtAddress1,
Addresses.txtAddress2,
Addresses.txtAddress3,
Addresses.txtAddressType,
Addresses.txtCountry,
Addresses.txtCounty,
Addresses.txtDayTelephone, 
Addresses.txtContactsTitle,
Addresses.txtContactsForename,
Addresses.txtContactsSurname,
Addresses.txtSecondaryTitle,
Addresses.txtSecondaryForename,
Addresses.txtSecondarySurname,
Addresses.txtHowMany,
Addresses.txtPostcode,
Addresses.txtRelationType,
Addresses.txtTown,
Addresses.txtEmail1,
Addresses.txtEmail2,

FutureAddresses.intFutureAddressID,
FutureAddresses.intFutureStudentHome,
FutureAddresses.txtFutureAddress1,
FutureAddresses.txtFutureAddress2,
FutureAddresses.txtFutureAddress3,
FutureAddresses.txtFutureAddressType,
FutureAddresses.txtFutureCountry,
FutureAddresses.txtFutureCounty,
FutureAddresses.txtFutureDayTelephone, 
FutureAddresses.txtFutureContactsTitle,
FutureAddresses.txtFutureContactsForename,
FutureAddresses.txtFutureContactsSurname,
FutureAddresses.txtFutureSecondaryTitle,
FutureAddresses.txtFutureSecondaryForename,
FutureAddresses.txtFutureSecondarySurname,
FutureAddresses.txtFuturePostcode,
FutureAddresses.txtFutureRelationType,
FutureAddresses.txtFutureHowMany,
FutureAddresses.txtFutureTown,
FutureAddresses.txtMovedInDate

FROM Pupils
LEFT OUTER JOIN Addresses ON Pupils.txtSchoolID = Addresses.txtSchoolID
LEFT OUTER JOIN FutureAddresses ON Pupils.txtSchoolID = FutureAddresses.txtSchoolID</CommandText>
