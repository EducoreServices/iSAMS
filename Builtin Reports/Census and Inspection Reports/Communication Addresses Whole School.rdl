      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT        TblPupilManagementPupils.intSystemStatus, TblPupilManagementPupils.TblPupilManagementPupilsID, TblPupilManagementPupils.txtForename, 
                         TblPupilManagementPupils.txtSurname, TblPupilManagementPupils.intNCYear, TblPupilManagementPupils.txtForm, 
                         TblPupilManagementPupils.txtBoardingHouse, TblPupilManagementAddresses.txtAddressType, TblPupilManagementAddresses.txtRelationType, 
                         TblPupilManagementAddresses.txtAddress1, TblPupilManagementAddresses.txtAddress2, TblPupilManagementAddresses.txtAddress3, 
                         TblPupilManagementAddresses.txtTown, TblPupilManagementAddresses.txtCounty, TblPupilManagementAddresses.txtCountry, 
                         TblPupilManagementAddresses.txtPostcode, TblPupilManagementAddresses.txtFax, TblPupilManagementAddresses.intOrder, 
                         TblPupilManagementAddresses.txtLetterSalutation, TblPupilManagementAddresses.txtDayTelephone, TblPupilManagementAddresses.txtMobile1, 
                         TblPupilManagementPupils.txtDOB, TblPupilManagementAddresses.intStudentHome, DATEDIFF(Month, TblPupilManagementPupils.txtDOB, { fn NOW() }) 
                         AS intMonths, TblPupilManagementPupils.txtSchoolID, TblPupilManagementPupils.txtGender, TblPupilManagementAddresses.txtMobile2, 
                         TblPupilManagementAddresses.txtProfession, TblPupilManagementAddresses.txtWorkTelephone, 
                         TblPupilManagementAddresses.txtAddress1 AS StxtAddress1, TblPupilManagementAddresses.txtSOS, TblPupilManagementPupils.intFamily, 
                         TblPupilManagementAddresses.TblPupilManagementAddressesID, TblPupilManagementAddresses.txtWorkFax, TblSchoolManagementYears.txtYearCode, 
                         TblPupilManagementAddresses.txtLabelSalutation, TblPupilManagementPupils.txtSiblingsIDList, TblPupilManagementAddresses.txtSecondarySurname, 
                         TblPupilManagementPupils.txtPreName, TblPupilManagementPupils.txtType
FROM            TblPupilManagementPupils INNER JOIN
                         TblPupilManagementAddressLink ON TblPupilManagementAddressLink.txtSchoolID = TblPupilManagementPupils.txtSchoolID LEFT OUTER JOIN
                         TblPupilManagementAddresses ON 
                         TblPupilManagementAddressLink.intAddressID = TblPupilManagementAddresses.TblPupilManagementAddressesID LEFT OUTER JOIN
                         TblSchoolManagementYears ON TblPupilManagementPupils.intNCYear = TblSchoolManagementYears.intNCYear
WHERE        (TblPupilManagementAddresses.intStudentHome = 1) AND (TblPupilManagementPupils.intSystemStatus = 1)</CommandText>
      </Query>
