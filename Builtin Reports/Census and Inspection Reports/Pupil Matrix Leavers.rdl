                         TblPupilManagementPupils.txtLeavingDate, TblPupilManagementPupils.txtLeavingTerm, TblStaff.StaffID, TblStaff.User_Code, TblStaff.Title, 
                         TblStaff.Firstname, TblStaff.MiddleNames, TblStaff.Surname, TblPupilManagementPupils.blnBaptised, TblPupilManagementPupils.blnConfirmation, 
                         TblPupilManagementPupils.txtFutureSchoolType
FROM            TblPupilManagementPupils LEFT OUTER JOIN
                         TblSchools ON TblPupilManagementPupils.intFutureSchool = TblSchools.TblSchoolsID LEFT OUTER JOIN
                         TblStaff ON TblPupilManagementPupils.txtTutor = TblStaff.User_Code
WHERE        (TblPupilManagementPupils.intSystemStatus = - 1) AND (TblPupilManagementPupils.txtType IN (@txtType) OR
                         'ALL' IN (@txtType)) AND (TblPupilManagementPupils.txtLeavingAcademicHouse IN (@txtLeavingAcademicHouse) OR
                         'ALL' IN (@txtLeavingAcademicHouse)) AND (TblPupilManagementPupils.txtLeavingBoardingHouse IN (@txtLeavingBoardingHouse) OR
                         'ALL' IN (@txtLeavingBoardingHouse)) AND (TblPupilManagementPupils.txtLeavingForm IN (@txtLeavingForm) OR
                         'ALL' IN (@txtLeavingForm)) AND (TblPupilManagementPupils.intLeavingNCYear IN (@intLeavingNCYear) OR
                         (- 10) IN (@intLeavingNCYear)) AND (TblPupilManagementPupils.intEnrolmentSchoolYear IN (@intEnrolmentSchoolYear) OR
                         (- 10) IN (@intEnrolmentSchoolYear)) AND (TblPupilManagementPupils.txtLeavingDate BETWEEN @DateStart AND @EndDate OR
                         TblPupilManagementPupils.txtLeavingDate IS NULL) AND (TblPupilManagementPupils.intLeavingSchoolYear IN (@intLeavingSchoolYear) OR
                         (- 10) IN (@intLeavingSchoolYear)) AND (CAST(TblPupilManagementPupils.txtLeavingReason AS varchar(500)) IN (@txtLeavingReason) OR
                         'ALL' IN (@txtLeavingReason))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@txtType">
            <Value>=Parameters!txtType.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@txtLeavingAcademicHouse">
            <Value>=Parameters!txtLeavingAcademicHouse.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@txtLeavingBoardingHouse">
            <Value>=Parameters!txtLeavingBoardingHouse.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@txtLeavingForm">
            <Value>=Parameters!txtLeavingForm.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@intLeavingNCYear">
            <Value>=Parameters!intLeavingNCYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@intEnrolmentSchoolYear">
            <Value>=Parameters!intEnrolmentSchoolYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateStart">
            <Value>=Parameters!DateStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@intLeavingSchoolYear">
            <Value>=Parameters!intLeavingSchoolYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@txtLeavingReason">
            <Value>=Parameters!txtLeavingReason.Value</Value>
          </QueryParameter>
        </QueryParameters>
