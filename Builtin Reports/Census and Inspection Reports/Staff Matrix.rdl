      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT        TblStaff.Surname, TblStaff.Initials, TblStaff.User_Code, TblStaff.Title, TblStaff.Firstname, TblStaff.MiddleNames, TblStaff.DOB, TblStaff.Sex, 
                         TblStaff.CountryofResidence, TblStaff.Nationality, TblStaff.Language, TblStaff.Religion, TblStaff.EthnicGroup, TblStaff.BirthPlace, TblStaff.BirthCounty, 
                         TblStaff.BirthCountry, TblStaff.EnrolmentDate, TblStaff.intEnrolmentSchoolYear, TblStaff.EnrolmentTerm, TblStaff.PreviousSchool, TblStaff.LeavingDate, 
                         TblStaff.intLeavingSchoolYear, TblStaff.LeavingTerm, TblStaff.FutureSchool, TblStaff.PartTime, TblStaff.HoursWorkedPW, TblStaff.TeachingStaff, 
                         TblStaff.SystemStatus, TblSchools.Name, FutureScholol.Name AS FutureName, TblStaff.TblStaffID, TblStaff.MaritalStatus, TblStaff.ChildrenInSchool, 
                         TblStaff.txtLeavingReason, TblStaff.intRetirementAge, TblStaff.blnBaptised, TblStaff.blnConfirmation, TblStaff.PersonalTutor, 
                         TblUniversities.Name AS UniversityName
FROM            TblStaff LEFT OUTER JOIN
                         TblSchools ON TblStaff.PreviousSchool = TblSchools.TblSchoolsID LEFT OUTER JOIN
                         TblSchools AS FutureScholol ON TblStaff.FutureSchool = FutureScholol.TblSchoolsID LEFT OUTER JOIN
                         TblUniversities ON TblStaff.University = TblUniversities.UID
WHERE        (TblStaff.SystemStatus IN (@SystemStatus)) AND (TblStaff.EnrolmentDate BETWEEN @DateStart AND @EndDate OR
                         TblStaff.EnrolmentDate IS NULL) AND (TblStaff.TeachingStaff IN (@TeachingStaff) OR
                         TblStaff.TeachingStaff IS NULL) AND (TblStaff.PartTime IN (@PartTime) OR
                         TblStaff.PartTime IS NULL)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@SystemStatus">
            <Value>=Parameters!SystemStatus.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateStart">
            <Value>=Parameters!DateStart.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TeachingStaff">
            <Value>=Parameters!TeachingStaff.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PartTime">
            <Value>=Parameters!PartTime.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
