        </Field>
      </Fields>
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT MIN(EnrolmentDate) AS MinEDate,  MAX(EnrolmentDate) AS MaxEDate  FROM TblStaff</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
