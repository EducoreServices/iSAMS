<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>57e78aee-6a09-4863-b31f-d80d75909072</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="FamilyAndPupils"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@txtSchoolID"><Value>=Parameters!txtSchoolID.Value</Value></QueryParameter></QueryParameters><CommandText>WITH FamilyDetails AS 
(
	SELECT DISTINCT
	TblPupilManagementPupils.intFamily,
	TblPupilManagementPupils.txtSurname AS txtSelectedPupilSurname,
	TblPupilManagementPupils.txtForename AS txtSelectedPupilForename
	FROM TblPupilManagementPupils
	WHERE  isnull(TblPupilManagementPupils.intFamily,0) &lt;&gt;0
	AND TblPupilManagementPupils.txtSchoolID IN (SELECT ID FROM SplitCSV(@txtSchoolID))
)
,ParentalDetails AS
(
	SELECT
	FamilyDetails.intFamily,
	FamilyDetails.txtSelectedPupilSurname,
	FamilyDetails.txtSelectedPupilForename,
	TblPupilManagementFamily.intFatherID,
	FatherAddress.txtContactsForename AS txtFatherForename,
	FatherAddress.txtContactsSurname AS txtFatherSurname,
	TblPupilManagementFamily.intMotherID,
	CASE WHEN MotherAddress.txtHowMany = 'Two' THEN MotherAddress.txtSecondaryForename ELSE MotherAddress.txtContactsForename END AS txtMotherForename,
	CASE WHEN MotherAddress.txtHowMany = 'Two' THEN MotherAddress.txtSecondarySurname ELSE MotherAddress.txtContactsSurname END AS txtMotherSurname
	FROM FamilyDetails
	INNER JOIN TblPupilManagementFamily ON FamilyDetails.intFamily = TblPupilManagementFamily.TblPupilManagementFamilyID
	LEFT OUTER JOIN TblPupilManagementAddresses AS FatherAddress ON TblPupilManagementFamily.intFatherID = FatherAddress.TblPupilManagementAddressesID
	OUTER APPLY (
	SELECT txtContactsForename, txtContactsSurname, txtSecondaryForename, txtSecondarySurname, TblUsefulLists.Description AS txtHowMany
	FROM TblPupilManagementAddresses 
	INNER JOIN TblUsefulLists ON TblPupilManagementAddresses.txtRelationType = TblUsefulLists.Name
	WHERE TblPupilManagementAddresses.TblPupilManagementAddressesID = TblPupilManagementFamily.intMotherID
	AND TblUsefulLists.ListType = 'ContactType'
	) AS MotherAddress
)
,PupilDetails AS
(
	SELECT ParentalDetails.*,
	TblPupilManagementPupils.txtSchoolID,
	TblPupilManagementPupils.txtForename, 
	TblPupilManagementPupils.txtSurname,
	TblPupilManagementPupils.intSystemStatus,
	TblPupilManagementPupils.txtDOB
	FROM ParentalDetails
	INNER JOIN TblPupilManagementPupils ON ParentalDetails.intFamily = TblPupilManagementPupils.intFamily
	WHERE TblPupilManagementPupils.intSystemStatus IN (0,1,-1)
	UNION 
	SELECT ParentalDetails.*,
	TblPupilManagementSiblings.txtSchoolid,
	TblPupilManagementSiblings.txtForename, 
	TblPupilManagementSiblings.txtSurname,
	2 AS intSystemStatus,
	TblPupilManagementSiblings.txtDOB
	FROM ParentalDetails
	INNER JOIN TblPupilManagementSiblings ON ParentalDetails.intFamily = TblPupilManagementSiblings.intFamily
)
,FamilySize AS
(
	SELECT PupilDetails.intFamily, Count(Distinct PupilDetails.txtSchoolID) AS intFamilySize
	FROM PupilDetails
	GROUP BY PupilDetails.intFamily
)

SELECT 
PupilDetails.*,
FamilySize.intFamilySize
FROM PupilDetails
LEFT OUTER JOIN FamilySize ON PupilDetails.intFamily = FamilySize.intFamily</CommandText></Query><Fields><Field Name="intFamily"><DataField>intFamily</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtSelectedPupilSurname"><DataField>txtSelectedPupilSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSelectedPupilForename"><DataField>txtSelectedPupilForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intFatherID"><DataField>intFatherID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtFatherForename"><DataField>txtFatherForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtFatherSurname"><DataField>txtFatherSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intMotherID"><DataField>intMotherID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtMotherForename"><DataField>txtMotherForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtMotherSurname"><DataField>txtMotherSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSchoolID"><DataField>txtSchoolID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtForename"><DataField>txtForename</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="intSystemStatus"><DataField>intSystemStatus</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtDOB"><DataField>txtDOB</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="intFamilySize"><DataField>intFamilySize</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="iSAMSAddress"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT txtExternaliSAMSAddress AS txtiSAMSAddress
FROM TbliSAMSManagerSettings</CommandText></Query><Fields><Field Name="txtiSAMSAddress"><DataField>txtiSAMSAddress</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet><DataSet Name="SchoolLogo"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>SELECT TblReportsCustomImages.txtPath + TblReportsCustomImages.txtFileName AS txtSchoolLogoPath, txtName
