<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>bf002bca-db3a-4190-8a0d-1a1764039471</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="iSAMS"><Query><DataSourceName>iSAMS</DataSourceName><CommandText>WITH SinglePupil
     AS (SELECT DISTINCT
                txtSchoolID,
                txtForename,
                txtSurname,
                intFamily,
                intSystemStatus,
                RANK() OVER(PARTITION BY intfamily ORDER BY CASE
                                                                WHEN intsystemstatus = 1
                                                                THEN 0
                                                                ELSE 1
                                                            END) AS intSinglePupilStatusRank
         FROM TblPupilManagementPupils),
     LastLoginDate
     AS (SELECT txtUserCode,
                MAX(txtDateTime) AS 'dtLastLoginDate'
         FROM TblAuditLogin
         WHERE intLoginCode IN(1060, 6000)
         GROUP BY txtUserCode)
     SELECT TbliSAMSManagerUsers.TbliSAMSManagerUsersID,
            TbliSAMSManagerUsers.txtTitle,
            TbliSAMSManagerUsers.txtFirstname,
            TbliSAMSManagerUsers.txtSurname,
            TbliSAMSManagerUsers.txtEmailAddress,
            TbliSAMSManagerUsers.txtUsername,
            TbliSAMSManagerUsers.txtSecurityID,
            tbliSAMSManagerSecurity.txtName,
            CASE
                WHEN TbliSAMSManagerUsers.txtStatus = '0'
                THEN 'Active'
                ELSE TbliSAMSManagerUsers.txtStatus
            END AS txtAccountStatus,
            TbliSAMSManagerUsers.txtCreateDateTime,
            LastLoginDate.dtLastLoginDate,
            STUFF(
                 (
                     SELECT ', '+SinglePupil.txtSchoolID+' '+SinglePupil.txtForename,
                            +' '+SinglePupil.txtSurname
                         FROM TbliSAMSManagerUsers
          INNER JOIN TblParentPortalUserLinks ON TbliSAMSManagerUsers.txtUserCode = TblParentPortalUserLinks.txtUserCode
          INNER JOIN SinglePupil ON TblParentPortalUserLinks.txtID = SinglePupil.intFamily
		          WHERE SinglePupil.intSinglePupilStatusRank = 1
           AND SinglePupil.intSystemStatus &lt;&gt; 1
           AND SinglePupil.intFamily IS NOT NULL
           AND SinglePupil.intFamily &lt;&gt; 0
           AND TbliSAMSManagerUsers.txtStatus &lt;&gt; 'Disabled'
           AND (TbliSAMSManagerUsers.txtSecurityID = 'pp'
                OR intiSAMSLogin = 0
                AND intPPLogin = 1)
                      FOR XML PATH('') 
                 ), 1, 1, '')AS txtStudents
     FROM TbliSAMSManagerUsers
          INNER JOIN TblParentPortalUserLinks ON TbliSAMSManagerUsers.txtUserCode = TblParentPortalUserLinks.txtUserCode
          INNER JOIN SinglePupil ON TblParentPortalUserLinks.txtID = SinglePupil.intFamily
          LEFT OUTER JOIN LastLoginDate ON TbliSAMSManagerUsers.txtUserCode = LastLoginDate.txtUserCode
          LEFT OUTER JOIN tbliSAMSManagerSecurity ON TbliSAMSManagerUsers.txtSecurityID = tbliSAMSManagerSecurity.tbliSAMSManagerSecurityID
     WHERE SinglePupil.intSinglePupilStatusRank = 1
           AND SinglePupil.intSystemStatus &lt;&gt; 1
           AND SinglePupil.intFamily IS NOT NULL
           AND SinglePupil.intFamily &lt;&gt; 0
           AND TbliSAMSManagerUsers.txtStatus &lt;&gt; 'Disabled'
           AND (TbliSAMSManagerUsers.txtSecurityID = 'pp'
                OR intiSAMSLogin = 0
                AND intPPLogin = 1);</CommandText></Query><Fields><Field Name="TbliSAMSManagerUsersID"><DataField>TbliSAMSManagerUsersID</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="txtTitle"><DataField>txtTitle</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtFirstname"><DataField>txtFirstname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSurname"><DataField>txtSurname</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtEmailAddress"><DataField>txtEmailAddress</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtUsername"><DataField>txtUsername</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtSecurityID"><DataField>txtSecurityID</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtName"><DataField>txtName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtAccountStatus"><DataField>txtAccountStatus</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="txtCreateDateTime"><DataField>txtCreateDateTime</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="dtLastLoginDate"><DataField>dtLastLoginDate</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="txtStudents"><DataField>txtStudents</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportSections><ReportSection><Body><ReportItems><Rectangle Name="Rectangle1"><ReportItems><Tablix Name="Tablix2"><TablixBody><TablixColumns><TablixColumn><Width>3.21702cm</Width></TablixColumn><TablixColumn><Width>3.21702cm</Width></TablixColumn><TablixColumn><Width>3.21702cm</Width></TablixColumn><TablixColumn><Width>3.21702cm</Width></TablixColumn><TablixColumn><Width>3.21702cm</Width></TablixColumn><TablixColumn><Width>3.21702cm</Width></TablixColumn><TablixColumn><Width>3.21702cm</Width></TablixColumn><TablixColumn><Width>3.21702cm</Width></Ta