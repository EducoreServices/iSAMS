          <QueryParameter Name="@txtTeacher">
            <Value>=Parameters!txtTeacher.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        TblCoverManagerAbsencesReasons.TblCoverManagerAbsencesReasonsId, TblCoverManagerAbsencesReasons.txtName, TblCoverManagerAbsencesReasons.txtCode, TblCoverManagerAbsencesReasons.intOrder, 
                         TblCoverManagerAbsencesReasons.blnActive, TblCoverManagerAbsencesReasons.blnConfidential, TblCoverManagerAbsences.intCycle, TblCoverManagerAbsences.TblCoverManagerAbsencesId, 
                         TblCoverManagerAbsences.txtTeacher, TblCoverManagerAbsences.intReason, TblCoverManagerAbsences.dteStartDateTime, TblCoverManagerAbsences.dteEndDateTime, TblStaff.Title, TblStaff.Surname, TblStaff.NameInitials, 
                         TblStaff.TblStaffID, TblStaff.Initials, TblStaff.Firstname, StartPeriod.txtShortName AS StartPeriodCode, EndPeriod.txtShortName AS EndPeriodCode, StartPeriod.TblTimetableManagerPeriodsID AS StartPeriodID, 
                         EndPeriod.TblTimetableManagerPeriodsID AS EndPeriodID, StartDay.txtShortName AS StartDayCode, EndDay.txtShortName AS EndDayCode, StartWeek.txtShortName AS StartWeekCode, 
                         EndWeek.txtShortName AS EndWeekCode, TblCoverManagerAbsences.intStartPeriod, TblCoverManagerAbsences.intEndPeriod, 
                         CASE WHEN TblCoverManagerAbsences.intStartPeriod = 0 THEN 1 ELSE 0 END AS MyPeriodOrder
FROM            TblCoverManagerAbsences LEFT OUTER JOIN
                         TblCoverManagerAbsencesReasons ON TblCoverManagerAbsences.intReason = TblCoverManagerAbsencesReasons.TblCoverManagerAbsencesReasonsId LEFT OUTER JOIN
                         TblStaff ON TblCoverManagerAbsences.txtTeacher = TblStaff.User_Code LEFT OUTER JOIN
                         TblTimetableManagerPeriods AS StartPeriod ON TblCoverManagerAbsences.intStartPeriod = StartPeriod.TblTimetableManagerPeriodsID LEFT OUTER JOIN
                         TblTimetableManagerDays AS StartDay ON StartPeriod.intDay = StartDay.TblTimetableManagerDaysID LEFT OUTER JOIN
                         TblTimetableManagerWeeks AS StartWeek ON StartDay.intWeek = StartWeek.TblTimetableManagerWeeksID LEFT OUTER JOIN
                         TblTimetableManagerPeriods AS EndPeriod ON TblCoverManagerAbsences.intEndPeriod = EndPeriod.TblTimetableManagerPeriodsID LEFT OUTER JOIN
                         TblTimetableManagerDays AS EndDay ON EndPeriod.intDay = EndDay.TblTimetableManagerDaysID LEFT OUTER JOIN
                         TblTimetableManagerWeeks AS EndWeek ON EndDay.intWeek = EndWeek.TblTimetableManagerWeeksID
WHERE       
(CAST(TblCoverManagerAbsences.TblCoverManagerAbsencesId as nvarchar) IN   (Select ID FROM dbo.SplitCSV(@TblCoverManagerAbsencesId)))
OR 
(@TblCoverManagerAbsencesId IS NULL   
AND (TblCoverManagerAbsences.dteStartDateTime &lt;= DATEADD(day, 1, @txtEndDate)) AND (TblCoverManagerAbsences.dteEndDateTime &gt;= @txtStartDate)
AND txtTeacher IN (@txtTeacher)
)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
