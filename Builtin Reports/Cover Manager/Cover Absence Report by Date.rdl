    <DataSet Name="Teachers">
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TblCoverManagerAbsencesId">
            <Value>=Parameters!TblCoverManagerAbsencesId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@txtEndDate">
            <Value>=Parameters!txtEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@txtStartDate">
            <Value>=Parameters!txtStartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
SELECT DISTINCT TblCoverManagerAbsences.txtTeacher, TblStaff.Surname + ', ' + TblStaff.Firstname AS Teacher
FROM            TblCoverManagerAbsences LEFT OUTER JOIN
                         TblCoverManagerAbsencesReasons ON TblCoverManagerAbsences.intReason = TblCoverManagerAbsencesReasons.TblCoverManagerAbsencesReasonsId LEFT OUTER JOIN
                         TblStaff ON TblCoverManagerAbsences.txtTeacher = TblStaff.User_Code LEFT OUTER JOIN
                         TblTimetableManagerPeriods AS StartPeriod ON TblCoverManagerAbsences.intStartPeriod = StartPeriod.TblTimetableManagerPeriodsID LEFT OUTER JOIN
                         TblTimetableManagerDays AS StartDay ON StartPeriod.intDay = StartDay.TblTimetableManagerDaysID LEFT OUTER JOIN
                         TblTimetableManagerWeeks AS StartWeek ON StartDay.intWeek = StartWeek.TblTimetableManagerWeeksID LEFT OUTER JOIN
                         TblTimetableManagerPeriods AS EndPeriod ON TblCoverManagerAbsences.intEndPeriod = EndPeriod.TblTimetableManagerPeriodsID LEFT OUTER JOIN
                         TblTimetableManagerDays AS EndDay ON EndPeriod.intDay = EndDay.TblTimetableManagerDaysID LEFT OUTER JOIN
                         TblTimetableManagerWeeks AS EndWeek ON EndDay.intWeek = EndWeek.TblTimetableManagerWeeksID

WHERE       
(CAST(TblCoverManagerAbsences.TblCoverManagerAbsencesId as nvarchar) IN   (Select ID FROM dbo.SplitCSV(@TblCoverManagerAbsencesId)))
OR 
(@TblCoverManagerAbsencesId IS NULL   
AND (TblCoverManagerAbsences.dteStartDateTime &lt;= DATEADD(day, 1, @txtEndDate)) AND (TblCoverManagerAbsences.dteEndDateTime &gt;= @txtStartDate)
)

ORDER BY
TblStaff.Surname + ', ' + TblStaff.Firstname


</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
