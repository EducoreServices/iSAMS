        <Field Name="txtTeacher">
          <DataField>txtTeacher</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Teacher">
          <DataField>Teacher</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
