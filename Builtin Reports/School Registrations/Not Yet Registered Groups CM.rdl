      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT        TblRegistrationSchoolRegistrationDateTime.TblRegistrationSchoolRegistrationDateTimeID, TblRegistrationSchoolRegistrationDateTime.txtDateTime, 
                         TblTimetableManagerSchedule.txtCode, TblTimetableManagerSchedule.intPeriod, TblPupilManagementPupils.txtSchoolID, TblPupilManagementPupils.txtSurname, 
                         TblPupilManagementPupils.txtPreName, TblPupilManagementPupils.txtDOB, TblPupilManagementPupils.txtForm, TblPupilManagementPupils.intNCYear, 
                         TblPupilManagementPupils.txtAcademicHouse, TblPupilManagementPupils.txtBoardingHouse, TblRegistrationSchoolRegistrationPupils.blnRegistered, 
                         TblSchoolManagementYears.txtYearName, LEFT(CONVERT(varchar(8), TblRegistrationSchoolRegistrationDateTime.txtDateTime, 108), 5) AS txtTime, 
                         TblRegistrationSchoolRegistrationPupils.txtReason, TblRegistrationSchoolRegistrationPupils.blnLate, TblRegistrationSchoolRegistrationPupils.intMinutesLate, 
                         (CASE WHEN OOSSet.txtSchoolID IS NOT NULL THEN 'OOS Set' WHEN RegistrationPupilsSet.blnRegistered IS NOT NULL AND 
                         RegistrationPupilsSet.txtSubmitBy IS NOT NULL THEN 'Set Registered' WHEN OOSForm.txtSchoolID IS NOT NULL 
                         THEN 'OOS Form' WHEN RegistrationPupilsForm.blnRegistered IS NOT NULL AND RegistrationPupilsForm.txtSubmitBy IS NOT NULL 
                         THEN 'Form Registered' ELSE 'Not Registered' END) AS txtRegistrationStatus, TblStaff.Initials, TblStaff.Surname, TblStaff.Firstname, 
                         TblPupilManagementPupils.txtTutor, TblCurriculumManagerModuleGroup.txtGroupName, TblCurriculumManagerModuleGroup.TblCurriculumManagerModuleGroupId, 
                         TblStaff.Title
FROM            TblRegistrationSchoolRegistrationDateTime INNER JOIN
                         TblTimetableManagerPeriods ON LEFT(CONVERT(varchar(8), TblRegistrationSchoolRegistrationDateTime.txtDateTime, 108), 5) BETWEEN 
                         TblTimetableManagerPeriods.txtStartTime AND TblTimetableManagerPeriods.txtEndTime AND LEFT(CONVERT(varchar(8), 
                         TblRegistrationSchoolRegistrationDateTime.txtDateTime, 108), 5) &lt; TblTimetableManagerPeriods.txtEndTime INNER JOIN
                         TblTimetableManagerDays ON TblTimetableManagerPeriods.intDay = TblTimetableManagerDays.TblTimetableManagerDaysID AND 
                         TblTimetableManagerDays.intDay = DATEPART(weekday, TblRegistrationSchoolRegistrationDateTime.txtDateTime) INNER JOIN
                         TblTimetableManagerWeeks ON TblTimetableManagerDays.intWeek = TblTimetableManagerWeeks.TblTimetableManagerWeeksID AND 
                         TblTimetableManagerWeeks.intActive = 1 INNER JOIN
                         TblTimetableManagerWeeksAllocations ON TblTimetableManagerWeeks.TblTimetableManagerWeeksID = TblTimetableManagerWeeksAllocations.intWeek AND 
                         TblTimetableManagerWeeksAllocations.intYearWeek = DATEPART(wk, TblRegistrationSchoolRegistrationDateTime.txtDateTime) INNER JOIN
                         TblTimetableManagerSchedule ON TblTimetableManagerPeriods.TblTimetableManagerPeriodsID = TblTimetableManagerSchedule.intPeriod INNER JOIN
                         TblTimetableManagerTimetables ON TblTimetableManagerSchedule.intTimetableID = TblTimetableManagerTimetables.TblTimetableManagerTimetablesID AND 
                         TblTimetableManagerTimetables.intPublished = 1 INNER JOIN
                         TblRegistrationSchoolRegistrationDateTimeRulesObjects ON 
                         TblRegistrationSchoolRegistrationDateTimeRulesObjects.intRule = TblRegistrationSchoolRegistrationDateTime.intRule INNER JOIN
                         TblCurriculumManagerModuleGroupList ON CAST(TblCurriculumManagerModuleGroupList.TblCurriculumManagerModuleGroupListId AS varchar) 
                         = TblRegistrationSchoolRegistrationDateTimeRulesObjects.txtValue AND 
                         TblTimetableManagerSchedule.txtCode = TblCurriculumManagerModuleGroupList.txtRegisterName INNER JOIN
                         TblCurriculumManagerModuleGroup ON 
                         TblCurriculumManagerModuleGroup.intGroupListId = TblCurriculumManagerModuleGroupList.TblCurriculumManagerModuleGroupListId INNER JOIN
                         TblCurriculumManagerModuleGroupListPupils ON 
                         TblCurriculumManagerModuleGroupListPupils.intGroupListId = TblCurriculumManagerModuleGroupList.TblCurriculumManagerModuleGroupListId AND 
                         (TblCurriculumManagerModuleGroupListPupils.dteRemovedOn IS NULL OR
                         TblCurriculumManagerModuleGroupListPupils.dteRemovedOn &gt; @txtEndDate) INNER JOIN
                         TblPupilManagementPupils ON TblCurriculumManagerModuleGroupListPupils.txtSchoolId = TblPupilManagementPupils.txtSchoolID LEFT OUTER JOIN
                         TblRegistrationOutOfSchoolPupils ON TblRegistrationOutOfSchoolPupils.txtSchoolID = TblPupilManagementPupils.txtSchoolID AND 
                         TblRegistrationSchoolRegistrationDateTime.txtDateTime BETWEEN TblRegistrationOutOfSchoolPupils.txtStartDateTime AND 
                         TblRegistrationOutOfSchoolPupils.txtEndDateTime LEFT OUTER JOIN
                         TblRegistrationSchoolRegistrationRegister ON 
                         TblRegistrationSchoolRegistrationRegister.intRegistrationDateTime = TblRegistrationSchoolRegistrationDateTime.TblRegistrationSchoolRegistrationDateTimeID LEFT
                          OUTER JOIN
                         TblRegistrationSchoolRegistrationPupils ON 
                         TblRegistrationSchoolRegistrationPupils.intRegister = TblRegistrationSchoolRegistrationRegister.TblRegistrationSchoolRegistrationRegisterID AND 
                         TblRegistrationSchoolRegistrationPupils.txtSchoolID = TblCurriculumManagerModuleGroupListPupils.txtSchoolId LEFT OUTER JOIN
                         TblSchoolManagementYears ON TblPupilManagementPupils.intNCYear = TblSchoolManagementYears.intNCYear LEFT OUTER JOIN
                         TblStaff ON TblTimetableManagerSchedule.txtTeacher = TblStaff.User_Code
WHERE        (TblPupilManagementPupils.intSystemStatus = 1) AND (TblPupilManagementPupils.txtSchoolID IN
                             (SELECT        ID
                               FROM            dbo.SplitCSV(@txtSchoolID) AS SplitCSV_1) OR
                         @txtSchoolID IS NULL) AND (TblRegistrationOutOfSchoolPupils.txtSchoolID IS NULL) AND (TblRegistrationSchoolRegistrationPupils.blnRegistered IS NULL) AND 
                         (TblPupilManagementPupils.txtForm IN
                             (SELECT        ID
                               FROM            dbo.SplitCSV(@txtForm) AS SplitCSV_1) OR
                         @txtForm IS NULL) AND (TblPupilManagementPupils.intNCYear IN
                             (SELECT        ID
                               FROM            dbo.SplitCSV(@intNCYear) AS SplitCSV_1) OR
                         @intNCYear IS NULL) AND (TblPupilManagementPupils.txtAcademicHouse IN
                             (SELECT        ID
                               FROM            dbo.SplitCSV(@txtAcademicHouse) AS SplitCSV_1) OR
                         @txtAcademicHouse IS NULL) AND (TblPupilManagementPupils.txtBoardingHouse IN
                             (SELECT        ID
                               FROM            dbo.SplitCSV(@txtBoardingHouse) AS SplitCSV_1) OR
                         @txtBoardingHouse IS NULL) AND (LEFT(CONVERT(varchar(8), TblRegistrationSchoolRegistrationDateTime.txtDateTime, 108), 5) IN
                             (SELECT        ID
                               FROM            dbo.SplitCSV(@txtTime) AS SplitCSV_1) OR
                         @txtTime IS NULL) AND (TblRegistrationSchoolRegistrationDateTime.txtDateTime BETWEEN @txtStartDate AND @txtEndDate)</CommandText>
      </Query>
    </DataSet>
