      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT        TblPupilManagementPupils.intSystemStatus, TblSchoolManagementHouses.txtHouseCode, TblSchoolManagementYears.txtYearCode, 
                         TblPupilManagementPupils.txtForename, TblPupilManagementPupils.txtPreName, TblPupilManagementPupils.txtTutor, 
                         TblPupilManagementPupils.txtSurname, TblPupilManagementPupils.txtGender, TblPupilManagementPupils.txtForm, TblSchoolManagementYears.intNCYear, 
                         TblPupilManagementPupils.txtAcademicHouse, TblPupilManagementPupils.txtBoardingHouse, TblPupilManagementPupils.txtSchoolID, TblStaff.Initials, 
                         TblRegistrationOutOfSchoolPupils.TblRegistrationOutOfSchoolPupilsID, TblRegistrationOutOfSchoolPupils.intCode, 
                         TblRegistrationOutOfSchoolPupils.txtStartDateTime, TblRegistrationOutOfSchoolPupils.txtEndDateTime, LEFT(CONVERT(varchar(8), 
                         TblRegistrationOutOfSchoolPupils.txtStartDateTime, 108), 5) AS txtStartTime, LEFT(CONVERT(varchar(8), TblRegistrationOutOfSchoolPupils.txtEndDateTime, 
                         108), 5) AS txtEndTime, TblRegistrationOutOfSchoolPupils.txtReason, TblRegistrationSchoolRegistrationCodes.txtCode, 
                         TblRegistrationSchoolRegistrationCodes.txtName, TblSchoolManagementYears.txtYearName
FROM            TblRegistrationOutOfSchoolPupils INNER JOIN
                         TblPupilManagementPupils ON TblRegistrationOutOfSchoolPupils.txtSchoolID = TblPupilManagementPupils.txtSchoolID LEFT OUTER JOIN
                         TblStaff ON TblStaff.User_Code = TblPupilManagementPupils.txtTutor LEFT OUTER JOIN
                         TblRegistrationSchoolRegistrationCodes ON 
                         TblRegistrationSchoolRegistrationCodes.TblRegistrationSchoolRegistrationCodesID = TblRegistrationOutOfSchoolPupils.intCode LEFT OUTER JOIN
                         TblSchoolManagementHouses ON 
                         TblPupilManagementPupils.txtBoardingHouse COLLATE database_default = TblSchoolManagementHouses.txtHouseName COLLATE database_default LEFT OUTER
                          JOIN
                         TblSchoolManagementYears ON TblPupilManagementPupils.intNCYear = TblSchoolManagementYears.intNCYear
WHERE        (TblPupilManagementPupils.intSystemStatus = 1) AND (TblPupilManagementPupils.txtSchoolID IN
                             (SELECT        ID
                               FROM            dbo.SplitCSV(@txtSchoolID) AS SplitCSV_1) OR
                         @txtSchoolID IS NULL) AND (TblPupilManagementPupils.txtForm IN
                             (SELECT        ID
                               FROM            dbo.SplitCSV(@txtForm) AS SplitCSV_1) OR
                         @txtForm IS NULL) AND (TblSchoolManagementYears.intNCYear IN
                             (SELECT        ID
                               FROM            dbo.SplitCSV(@intNCYear) AS SplitCSV_1) OR
                         @intNCYear IS NULL) AND (TblPupilManagementPupils.txtAcademicHouse IN
                             (SELECT        ID
                               FROM            dbo.SplitCSV(@txtAcademicHouse) AS SplitCSV_1) OR
                         @txtAcademicHouse IS NULL) AND (TblPupilManagementPupils.txtBoardingHouse IN
                             (SELECT        ID
                               FROM            dbo.SplitCSV(@txtBoardingHouse) AS SplitCSV_1) OR
                         @txtBoardingHouse IS NULL) AND (TblRegistrationOutOfSchoolPupils.txtStartDateTime BETWEEN @txtStartDate AND @txtEndDate OR
                         TblRegistrationOutOfSchoolPupils.txtEndDateTime BETWEEN @txtStartDate AND @txtEndDate)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@txtSchoolID">
