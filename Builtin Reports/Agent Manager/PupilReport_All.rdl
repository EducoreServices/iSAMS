      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <CommandText>SELECT     TblAgentManagerAgency.txtAgencyName, TblAgentManagerAgencyBranch.txtBranchName, TblPupilManagementPupils.txtForename, 
                      TblPupilManagementPupils.txtSurname, TblPupilManagementPupils.txtGender, TblPupilManagementPupils.txtDOB, 
                      TblPupilManagementPupils.txtForm, TblPupilManagementPupils.intNCYear, TblStaff.Fullname, TblAgentManagerPaymentType.txtTypeName, 
                      TblAgentManagerAgencyBranchPupilPaymentSchedule.intYear, TblAgentManagerAgencyBranchPupilPaymentSchedule.dblValue, 
                      TblAgentManagerAgencyBranchPupil.txtSchoolID, TblAgentManagerPaymentType.TblAgentManagerPaymentTypeID
FROM         TblAgentManagerAgency INNER JOIN
                      TblAgentManagerAgencyBranch ON TblAgentManagerAgency.TblAgentManagerAgencyID = TblAgentManagerAgencyBranch.intAgencyID INNER JOIN
                      TblAgentManagerAgencyBranchPupil ON 
                      TblAgentManagerAgencyBranch.TblAgentManagerAgencyBranchID = TblAgentManagerAgencyBranchPupil.intAgencyBranchID INNER JOIN
                      TblAgentManagerAgencyBranchPupilPaymentSchedule ON 
                      TblAgentManagerAgencyBranchPupil.TblAgentManagerAgencyBranchPupilID = TblAgentManagerAgencyBranchPupilPaymentSchedule.intAgencyPupilLinkID
                       INNER JOIN
                      TblAgentManagerPaymentType ON 
                      TblAgentManagerAgencyBranchPupil.intPaymentType = TblAgentManagerPaymentType.TblAgentManagerPaymentTypeID INNER JOIN
                      TblPupilManagementPupils ON TblAgentManagerAgencyBranchPupil.txtSchoolID = TblPupilManagementPupils.txtSchoolID LEFT OUTER JOIN
                      TblStaff ON TblPupilManagementPupils.txtTutor = TblStaff.User_Code
ORDER BY TblPupilManagementPupils.txtSurname, TblAgentManagerAgencyBranchPupilPaymentSchedule.intYear</CommandText>
      </Query>
