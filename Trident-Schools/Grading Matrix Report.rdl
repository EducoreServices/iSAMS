<?xml version="1.0" encoding="utf-8"?><Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner"><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="iSAMS"><DataSourceReference>iSAMS</DataSourceReference><rd:SecurityType>None</rd:SecurityType><rd:DataSourceID>fbc7e57b-eea0-412e-b7c9-ff238f1178d8</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="AcademicReports"><Query><DataSourceName>iSAMS</DataSourceName><QueryParameters><QueryParameter Name="@intReportCycle"><Value>=Parameters!intReportCycle.Value</Value></QueryParameter><QueryParameter Name="@txtForm"><Value>=Parameters!txtForm.Value</Value></QueryParameter><QueryParameter Name="@txtSubject"><Value>=Parameters!txtSubject.Value</Value></QueryParameter><QueryParameter Name="@intNCYear"><Value>=Parameters!intNCYear.Value</Value></QueryParameter><QueryParameter Name="@txtSchoolID"><Value>=Parameters!txtSchoolID.Value</Value></QueryParameter><QueryParameter Name="@txtHouse"><Value>=Parameters!txtHouse.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT AcademicReports.*
FROM VwReportsManagementAcademicReports AS AcademicReports
LEFT OUTER JOIN tblStaff on AcademicReports.txtSubmitBy = tblStaff.User_Code


WHERE (AcademicReports.intReportCycleID = @intReportCycle) 
AND (AcademicReports.intReportProgress = 1) 

----PARAMETERS

AND (AcademicReports.intNCYear IN (SELECT ID FROM dbo.SplitCSV(@intNCYear) AS SplitCSV_1) OR @intNCYear IS NULL) 
AND (AcademicReports.txtForm IN (SELECT ID FROM dbo.SplitCSV(@txtForm) AS SplitCSV_1) OR @txtForm IS NULL) 
AND (AcademicReports.txtID IN (SELECT ID FROM dbo.SplitCSV(@txtSubject) AS SplitCSV_1) OR @txtSubject IS NULL) 
AND (AcademicReports.txtSchoolID IN (SELECT ID FROM dbo.SplitCSV(@txtSchoolID) AS SplitCSV_1) OR @txtSchoolID IS NULL) 
AND (AcademicReports.txtBoardingHouse IN (SELECT ID	FROM dbo.SplitCSV(@txtHouse) AS SplitCSV_1) OR @txtHouse IS NULL 
OR AcademicReports.txtAcademicHouse IN (SELECT ID FROM dbo.SplitCSV(@txtHouse) AS SplitCSV_1) OR @txtHouse IS NULL)


