        <CommandText>--Pupils included in the selected cycle and the dates describing the cycle
WITH PupilsAndCycleDates AS 
(SELECT DISTINCT txtSchoolID, 
TblReportsManagementCycle.txtStartReports AS dtCycleStartDate,
TblReportsManagementCycle.txtFinishReports AS dtCycleEndDate,
TblReportsManagementCycle.txtPrintReports AS dtCyclePrintDate,
TblSchoolManagementTermDates.txtStartDate AS dtTermStartDate, 
TblSchoolManagementTermDates.txtFinishDate AS dtTermEndDate, 
BeginingOfAcademicYear.dtStartOfAcademicYear

FROM TblReportsStorePupilArchive 
INNER JOIN TblReportsManagementCycle ON  TblReportsStorePupilArchive.intReportCycle = TblReportsManagementCycle.TblReportsManagementCycleID 
INNER JOIN TblSchoolManagementTermDates ON TblReportsManagementCycle.intReportTerm = TblSchoolManagementTermDates.intTerm 
	AND TblReportsManagementCycle.intReportYear = TblSchoolManagementTermDates.intSchoolYear 
OUTER APPLY (SELECT TOP (1) txtStartDate AS dtStartOfAcademicYear
FROM TblSchoolManagementTermDates AS TD
WHERE TD.intSchoolYear = TblSchoolManagementTermDates.intSchoolYear
ORDER BY TD.txtStartDate) AS BeginingOfAcademicYear

WHERE        (TblReportsStorePupilArchive.intReportCycle = @intReportCycle) 
AND (TblReportsStorePupilArchive.intNCYear IN (SELECT ID FROM dbo.SplitCSV(@intNCYear) AS SplitCSV_1) OR @intNCYear IS NULL) 
AND (TblReportsStorePupilArchive.txtForm IN (SELECT ID FROM dbo.SplitCSV(@txtForm) AS SplitCSV_1) OR @txtForm IS NULL) 
AND (TblReportsStorePupilArchive.txtSchoolID IN (SELECT ID FROM dbo.SplitCSV(@txtSchoolID) AS SplitCSV_1) OR @txtSchoolID IS NULL) 
AND (TblReportsStorePupilArchive.txtBoardingHouse IN (SELECT ID FROM dbo.SplitCSV(@txtHouse) AS SplitCSV_1) OR @txtHouse IS NULL 
	OR TblReportsStorePupilArchive.txtAcademicHouse IN (SELECT ID FROM dbo.SplitCSV(@txtHouse) AS SplitCSV_1) OR @txtHouse IS NULL)), 
	


-- registration counts
Registrations AS (
SELECT COUNT(DISTINCT Attendance.intRegistrationDateTimeID) AS intRegistrationCount, 
COUNT(DISTINCT CASE Attendance.txtRegistrationStatisticalStatus WHEN 'Present' THEN Attendance.intRegistrationDateTimeID ELSE NULL end) AS intPresentCount,
COUNT(DISTINCT CASE WHEN Attendance.txtRegistrationStatisticalStatus = 'Present' AND Attendance.intIsLate = 1 THEN Attendance.intRegistrationDateTimeID ELSE NULL end) AS intLateCount,
COUNT(DISTINCT CASE Attendance.txtRegistrationStatisticalStatus WHEN 'Absent' THEN Attendance.intRegistrationDateTimeID ELSE NULL end) AS intAbsentCount,
COUNT(DISTINCT CASE WHEN Attendance.txtRegistrationStatisticalStatus = 'Absent' AND Attendance.intIsAuthorisedAbsence = 1 THEN Attendance.intRegistrationDateTimeID ELSE NULL end) AS intAuthorisedAbsentCount,
COUNT(DISTINCT CASE WHEN Attendance.txtRegistrationStatisticalStatus = 'Absent' AND Attendance.intIsAuthorisedAbsence = 0 THEN Attendance.intRegistrationDateTimeID ELSE NULL end) AS intUnAuthorisedAbsentCount,
COUNT(DISTINCT CASE Attendance.txtRegistrationStatisticalStatus WHEN 'Not Registered' THEN Attendance.intRegistrationDateTimeID ELSE NULL end) AS intNotRegisteredCount,
Attendance.txtSchoolID
FROM VwRegistrationSchoolRegistrationAttendance AS Attendance 
INNER JOIN PupilsAndCycleDates ON Attendance.txtSchoolID = PupilsAndCycleDates.txtSchoolID
WHERE Attendance.txtSchoolID = PupilsAndCycleDates.txtSchoolID
AND Attendance.dtRegistrationDateTime BETWEEN PupilsAndCycleDates.dtStartOfAcademicYear AND DATEADD(DAY, 1, PupilsAndCycleDates.dtCyclePrintDate) 
AND Attendance.txtRegistrationAMPM IN ('am') 
AND Attendance.intIsIncludedInStatistics = 1
GROUP BY Attendance.txtSchoolID),

 RewardsByType AS
(
SELECT SUM (TblRewardsManagerRewardTypesCategories.intValue) AS intRewardSum,
COUNT(DISTINCT TblRewardsManagerRewards.TblRewardsManagerRewardsID) AS intRewardCount,
 TblRewardsManagerRewards.txtSchoolID
FROM TblRewardsManagerRewards
INNER JOIN TblRewardsManagerRewardTypesCategories ON TblRewardsManagerRewards.intCategory = TblRewardsManagerRewardTypesCategories.TblRewardsManagerRewardTypesCategoriesID
INNER JOIN PupilsAndCycleDates ON TblRewardsManagerRewards.txtSchoolID = PupilsAndCycleDates.txtSchoolID
WHERE  TblRewardsManagerRewards.intAuthorised = 1
AND TblRewardsManagerRewards.txtSchoolID = PupilsAndCycleDates.txtSchoolID
AND TblRewardsManagerRewards.txtDate BETWEEN PupilsAndCycleDates.dtTermStartDate AND DATEADD(DAY, 1, PupilsAndCycleDates.dtTermEndDate) 
AND TblRewardsManagerRewards.intType IN (3)
GROUP BY  TblRewardsManagerRewards.txtSchoolID ),

Detentions AS
(SELECT COUNT (DISTINCT TblDisciplineManagerDetentionsPupils.TblDisciplineManagerDetentionsPupilsID) AS intDetentionCount, TblDisciplineManagerDetentionsPupils.txtSchoolID 
FROM TblDisciplineManagerDetentionsPupils
INNER JOIN PupilsAndCycleDates ON TblDisciplineManagerDetentionsPupils.txtSchoolID = PupilsAndCycleDates.txtSchoolID
WHERE  TblDisciplineManagerDetentionsPupils.intAuthorised = 1
AND TblDisciplineManagerDetentionsPupils.txtSchoolID = PupilsAndCycleDates.txtSchoolID

AND TblDisciplineManagerDetentionsPupils.dDetentionDate BETWEEN  PupilsAndCycleDates.dtTermStartDate AND DATEADD(DAY, 1, PupilsAndCycleDates.dtTermEndDate) 
GROUP BY TblDisciplineManagerDetentionsPupils.txtSchoolID 

) 





SELECT AcademicReports.*,
Registrations.intPresentCount, Registrations.intLateCount, Registrations.intRegistrationCount, Registrations.intAbsentCount,
Registrations.intAuthorisedAbsentCount, Registrations.intUnAuthorisedAbsentCount, Registrations.intNotRegisteredCount, RewardsByType.intRewardCount, Detentions.intDetentionCount

FROM VwReportsManagementAcademicReports AS AcademicReports 
INNER JOIN PupilsAndCycleDates ON AcademicReports.txtSchoolID = PupilsAndCycleDates.txtSchoolID 
LEFT OUTER JOIN Registrations ON AcademicReports.txtSchoolID = Registrations.txtSchoolID
LEFT OUTER JOIN RewardsByType ON AcademicReports.txtSchoolID = RewardsByType.txtSchoolID
LEFT OUTER JOIN Detentions ON AcademicReports.txtSchoolID = Detentions.txtSchoolID
WHERE (AcademicReports.intReportCycleID = @intReportCycle) 
AND (AcademicReports.intReportProgress = 1) 
AND (AcademicReports.txtID IN (SELECT ID FROM dbo.SplitCSV(@txtSubject) AS SplitCSV_1) OR @txtSubject IS NULL)</CommandText>
