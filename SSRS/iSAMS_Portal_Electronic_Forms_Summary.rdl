    <DataSet Name="Consent">
      <Query>
        <DataSourceName>iSAMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@intForm">
            <Value>=Parameters!intForm.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@NCYear">
            <Value>=Join(Parameters!NCYear.Value,",")</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>WITH RepliesFrom AS (SELECT DISTINCT Response.intResponse, Response.txtValue
FROM TblParentPortalFeaturesInteractiveFormResponse AS Response
INNER JOIN TblParentPortalFeaturesInteractiveFormItems AS Items ON Items.TblParentPortalFeaturesInteractiveFormItemsId = Response.intFormItem
INNER JOIN TblPupilManagementPupils ON TblPupilManagementPupils.txtSchoolID = Response.txtValue
WHERE Items.intForm = @intForm AND Items.txtType = 'pupil' AND TblPupilManagementPupils.intNCYear IN (SELECT ID FROM dbo.SplitCSV(@NCYear))  AND TblPupilManagementPupils.intSystemStatus = 1),

Pupils AS (SELECT DISTINCT PMP.txtSchoolID, PMP.txtPreName, PMP.txtSurname, PMP.intNCYear, PMP.txtForm
FROM TblPupilManagementPupils AS PMP WHERE intNCYear IN (SELECT ID FROM dbo.SplitCSV(@NCYear)) AND PMP.intSystemStatus = 1)

SELECT Responses.intResponse, Items.txtName, Items.txtType, Items.intOrder,
CASE WHEN Responses.txtValue = '' THEN 'N' WHEN Responses.txtValue IS NULL THEN 'Not Completed' WHEN LEFT(Responses.txtValue,5) = 'I con' THEN 'Y' ELSE Responses.txtValue END AS txtResponse,
CAST (Responses.dteSubmitDateTime AS date) AS txtDate, LEFT(CAST (Responses.dteSubmitDateTime AS time),5) AS txtTime, Responses.txtSubmitBy,
Pupils.txtSchoolID, Pupils.txtPreName, Pupils.txtSurname, Pupils.intNCYear, Pupils.txtForm

FROM Pupils
LEFT OUTER JOIN RepliesFrom 
	INNER JOIN TblParentPortalFeaturesInteractiveFormResponse AS Responses 
	INNER JOIN TblParentPortalFeaturesInteractiveFormItems AS Items ON Items.TblParentPortalFeaturesInteractiveFormItemsId = Responses.intFormItem
				AND Items.intForm = @intForm AND Items.txtType NOT IN ('pupil')
	On Responses.intResponse = RepliesFrom.intResponse
ON Pupils.txtSchoolID = RepliesFrom.txtValue

WHERE Responses.dteSubmitDateTime &gt; DATEFROMPARTS(CASE WHEN MONTH(GETDATE())&gt;8 THEN YEAR(GETDATE()) ELSE YEAR(GETDATE())-1 END,09,01) OR Responses.dteSubmitDateTime IS NULL</CommandText>
      </Query>
