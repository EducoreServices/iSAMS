                         TblPupilManagementPupils.txtBirthCountry, TblPupilManagementPupils.blnDiplomaticAndForces, TblPupilManagementPupils.txtSchoolLink, 
                         TblPupilManagementNotes.txtType AS txtNoteType, TblPupilManagementNotes.txtNote AS txtNoteBody, TblPupilManagementNotes.txtDate AS txtNoteDate, 
                         TblPupilManagementNotes.TblPupilManagementNotesID, TblPupilManagementVisas.TblPupilManagementVisasId, TblPupilManagementVisas.txtVisaID, 
                         TblPupilManagementVisas.txtCAS, TblPupilManagementVisas.txtPlaceIssued, TblPupilManagementVisas.txtNationality AS txtVisaNationality, 
                         TblPupilManagementVisas.txtType AS txtVisaType, TblPupilManagementVisas.txtNote AS txtVisaNote, TblPupilManagementVisas.dteValidFrom, 
                         TblPupilManagementVisas.dteEntryDate, TblPupilManagementVisas.dteExpires, TblPupilManagementPassports.txtPassportNumber, 
