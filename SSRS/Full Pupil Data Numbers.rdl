                         TblPupilManagementPassports.txtSubmitDateTime AS txtPassportSubmitDateTime, TblPupilManagementPupils.blnVisaRequired
FROM            TblPupilManagementPupils INNER JOIN
                         TblPupilManagementAddressLink ON TblPupilManagementAddressLink.txtSchoolID = TblPupilManagementPupils.txtSchoolID LEFT OUTER JOIN
                         TblPupilManagementAddresses ON 
                         TblPupilManagementAddressLink.intAddressID = TblPupilManagementAddresses.TblPupilManagementAddressesID LEFT OUTER JOIN
                         TblSchools ON TblPupilManagementPupils.intPreviousSchool = TblSchools.TblSchoolsID LEFT OUTER JOIN
                         TblStaff ON TblPupilManagementPupils.txtTutor = TblStaff.User_Code LEFT OUTER JOIN
                         TblSchoolManagementYears ON TblPupilManagementPupils.intNCYear = TblSchoolManagementYears.intNCYear LEFT OUTER JOIN
                         TblPupilManagementPupils AS TblPMP ON TblPupilManagementPupils.intFamily = TblPMP.intFamily AND 
                         TblPupilManagementPupils.txtSchoolID &lt;&gt; TblPMP.txtSchoolID AND TblPupilManagementPupils.intFamily &lt;&gt; 0 LEFT OUTER JOIN
                         TblPupilManagementSelectionsStudents ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementSelectionsStudents.txtSchoolID LEFT OUTER JOIN
                         TblPupilManagementSelections ON 
                         TblPupilManagementSelectionsStudents.intSelectionID = TblPupilManagementSelections.TblPupilManagementSelectionsID AND 
                         ISNULL(TblPupilManagementSelectionsStudents.intHidden, 0) = 0 AND ISNULL(TblPupilManagementSelections.intHidden, 0) = 0 LEFT OUTER JOIN
                         TblStaff AS GroupOwner ON TblPupilManagementSelections.txtSubmitBy = GroupOwner.User_Code LEFT OUTER JOIN
                         TblSchoolManagementYears AS EnrolmentSchoolYear ON 
                         TblPupilManagementPupils.intEnrolmentNCYear = EnrolmentSchoolYear.intNCYear LEFT OUTER JOIN
                         TblSchoolManagementYears AS LeavingSchoolYear ON TblPupilManagementPupils.intLeavingNCYear = LeavingSchoolYear.intNCYear LEFT OUTER JOIN
                         TblSchools AS FutureSchool ON TblPupilManagementPupils.intFutureSchool = FutureSchool.TblSchoolsID LEFT OUTER JOIN
                         TblPupilManagementNotes ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementNotes.txtSchoolID LEFT OUTER JOIN
                         TblPupilManagementVisas ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementVisas.txtSchoolId LEFT OUTER JOIN
                         TblPupilManagementPassports ON TblPupilManagementPupils.txtSchoolID = TblPupilManagementPassports.txtSchoolID
WHERE
TblPupilManagementPupils.intSystemStatus = 1</CommandText>
