                         TblPupilManagementAddresses.txtContactsForename, TblPupilManagementAddresses.txtContactsSurname, 
                         TblPupilManagementAddresses.txtLetterSalutation, TblPupilManagementAddresses.txtDayTelephone, TblPupilManagementAddresses.txtMobile1, 
                         TblPupilManagementAddresses.txtEmail1, TblPupilManagementPupils.txtDOB, TblPupilManagementAddresses.intStudentHome, DATEDIFF(Month, 
                         TblPupilManagementPupils.txtDOB, { fn NOW() }) AS intMonths,
                             (SELECT        TOP (1) txtPath
                               FROM            TblPupilManagementPictures
                               WHERE        (txtSchoolID = TblPupilManagementPupils.txtSchoolID)
                               ORDER BY intOrder DESC) AS txtPath, TblPupilManagementPupils.txtSchoolID, TblPupilManagementPupils.txtMiddleNames, 
                         TblPupilManagementPupils.txtGender, TblPupilManagementPupils.txtTutor, TblPupilManagementPupils.txtType, 
